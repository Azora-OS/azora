{
  "version": 2,
  "builds": [
    { "src": "apps/azora-sapiens/package.json", "use": "@vercel/next" },
    { "src": "apps/azora-buildspaces/package.json", "use": "@vercel/next" },
    { "src": "apps/azora-oracle/package.json", "use": "@vercel/next" },
    { "src": "apps/azora-compliance/package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
    { "src": "apps/azora-pay/package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
    { "src": "apps/azora-dev/package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
    { "src": "apps/azora-jobspaces/package.json", "use": "@vercel/static-build", "config": { "distDir": "build" } },
    {
      "src": "services/education-revenue-engine/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/elara-ai-orchestrator/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/elara-onboarding/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/azora-auth/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/azora-education/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/azora-marketplace/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/azora-pay/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/azora-mint/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/azora-sapiens/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/azora-studyspaces/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/azora-research-center/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/azora-forge/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/azora-assessment/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/azora-analytics/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/azora-erp/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/azora-finance/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/azora-treasury/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/azora-pricing/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/apps/sapiens/(.*)", "dest": "/apps/azora-sapiens/$1" },
    { "src": "/apps/build/(.*)", "dest": "/apps/azora-buildspaces/$1" },
    { "src": "/apps/oracle/(.*)", "dest": "/apps/azora-oracle/$1" },
    { "src": "/apps/compliance/(.*)", "dest": "/apps/azora-compliance/$1" },
    { "src": "/apps/pay/(.*)", "dest": "/apps/azora-pay/$1" },
    { "src": "/apps/dev/(.*)", "dest": "/apps/azora-dev/$1" },
    { "src": "/apps/jobs/(.*)", "dest": "/apps/azora-jobspaces/$1" },
    {
      "src": "/api/education/(.*)",
      "dest": "services/education-revenue-engine/dist/index.js"
    },
    {
      "src": "/api/elara/(.*)",
      "dest": "services/elara-ai-orchestrator/dist/index.js"
    },
    {
      "src": "/api/onboarding/(.*)",
      "dest": "services/elara-onboarding/dist/index.js"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "services/azora-auth/dist/index.js"
    },
    {
      "src": "/api/education-service/(.*)",
      "dest": "services/azora-education/dist/index.js"
    },
    {
      "src": "/api/marketplace/(.*)",
      "dest": "services/azora-marketplace/dist/index.js"
    },
    {
      "src": "/api/pay/(.*)",
      "dest": "services/azora-pay/dist/index.js"
    },
    {
      "src": "/api/mint/(.*)",
      "dest": "services/azora-mint/dist/index.js"
    },
    {
      "src": "/api/sapiens/(.*)",
      "dest": "services/azora-sapiens/dist/index.js"
    },
    {
      "src": "/api/studyspaces/(.*)",
      "dest": "services/azora-studyspaces/dist/index.js"
    },
    {
      "src": "/api/research/(.*)",
      "dest": "services/azora-research-center/dist/index.js"
    },
    {
      "src": "/api/forge/(.*)",
      "dest": "services/azora-forge/dist/index.js"
    },
    {
      "src": "/api/assessment/(.*)",
      "dest": "services/azora-assessment/dist/index.js"
    },
    {
      "src": "/api/analytics/(.*)",
      "dest": "services/azora-analytics/dist/index.js"
    },
    {
      "src": "/api/erp/(.*)",
      "dest": "services/azora-erp/dist/index.js"
    },
    {
      "src": "/api/finance/(.*)",
      "dest": "services/azora-finance/dist/index.js"
    },
    {
      "src": "/api/treasury/(.*)",
      "dest": "services/azora-treasury/dist/index.js"
    },
    {
      "src": "/api/pricing/(.*)",
      "dest": "services/azora-pricing/dist/index.js"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "REDIS_URL": "@redis_url",
    "OPENAI_API_KEY": "@openai_api_key",
    "PINECONE_API_KEY": "@pinecone_api_key",
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_ANON_KEY": "@supabase_anon_key"
  }
}
