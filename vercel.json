{
  "version": 2,
  "builds": [
    {
      "src": "services/education-revenue-engine/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/elara-ai-orchestrator/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "services/elara-onboarding/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/education/(.*)",
      "dest": "services/education-revenue-engine/dist/index.js"
    },
    {
      "src": "/api/elara/(.*)",
      "dest": "services/elara-ai-orchestrator/dist/index.js"
    },
    {
      "src": "/api/onboarding/(.*)",
      "dest": "services/elara-onboarding/dist/index.ts"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "REDIS_URL": "@redis_url",
    "OPENAI_API_KEY": "@openai_api_key",
    "PINECONE_API_KEY": "@pinecone_api_key"
  }
}
