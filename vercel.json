{
  "version": 2,
  "name": "azora-supreme-organism",
  "builds": [
    {
      "src": "azora-ui/student-portal/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "azora-ui/job-board/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "azora-ui/mint-dashboard/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "azora-ui/admin-panel/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/student/(.*)",
      "dest": "azora-ui/student-portal/$1"
    },
    {
      "src": "/jobs/(.*)",
      "dest": "azora-ui/job-board/$1"
    },
    {
      "src": "/mint/(.*)",
      "dest": "azora-ui/mint-dashboard/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "azora-ui/admin-panel/$1"
    },
    {
      "src": "/(.*)",
      "dest": "azora-ui/student-portal/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
