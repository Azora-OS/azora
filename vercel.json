{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "env": {
    "AZORA_VERSION": "1.0.0",
    "NODE_ENV": "production"
  },
  "functions": {
    "apps/web/app/**": {
      "maxDuration": 30
    }
  }
}
