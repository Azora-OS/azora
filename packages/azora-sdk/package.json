{
  "name": "@azora/sdk",
  "version": "1.0.0",
  "description": "Official TypeScript/JavaScript SDK for Azora OS Platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "azora",
    "sdk",
    "api",
    "education",
    "payments",
    "ai",
    "africa"
  ],
  "author": "Azora ES (Pty) Ltd",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "https://github.com/azora-os/azora-os"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "tsup": "^8.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "peerDependencies": {},
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
