{
  "name": "azora-os",
  "version": "3.0.0",
  "description": "Constitutional AI Operating System",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern='\\.test\\.ts$' --testPathIgnorePatterns='integration|e2e'",
    "test:integration": "jest --testPathPattern='integration'",
    "test:e2e": "playwright test",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "typecheck": "tsc --noEmit",
    "fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "security:audit": "npm audit --audit-level=high",
    "security:audit:fix": "npm audit fix",
    "security:scan": "node scripts/security-scan.js",
    "start:all": "node scripts/start-all.js",
    "health:check": "node scripts/health-check.js",
    "connections:test": "node scripts/test-connections.js",
    "connections:status": "cat connection-status.json",
    "setup": "node scripts/setup-services.js"
  },
  "dependencies": {
    "@prisma/client": "5.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "next-pwa": "5.6.0",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "prisma": "5.6.0",
    "prom-client": "15.1.3",
    "redis": "^4.6.10",
    "speakeasy": "2.0.0"
  },
  "devDependencies": {
    "@playwright/test": "1.56.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.9",
    "@types/pg": "^8.10.7",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "3.1.11",
    "playwright": "1.56.1",
    "supertest": "7.1.4",
    "ts-jest": "29.4.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "keywords": [
    "ai",
    "education",
    "finance",
    "ubuntu",
    "constitutional-ai"
  ],
  "author": "Azora ES (Pty) Ltd",
  "license": "PROPRIETARY"
}
