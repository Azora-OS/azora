{
  "name": "azora-os",
  "version": "3.0.0",
  "description": "The World's First Constitutional AI Operating System",
  "packageManager": "npm@10.2.4",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Running tests...\" && exit 0",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "cross-env ESLINT_USE_FLAT_CONFIG=true eslint --config config/eslint.config.js health-check.js quick-health-check.js",
    "type-check": "turbo run type-check",
    "test:turbo": "turbo run test",
    "clean": "docker-compose down -v --remove-orphans && docker system prune -f",
    "validate-env": "node scripts/validate-env.js",
    "validate-env-schema": "node scripts/validate-env-schema.js",
    "scan-secrets": "node scripts/scan-secrets.js",
    "db:migrate:turbo": "turbo run db:migrate",
    "db:generate:turbo": "turbo run db:generate",
    "integrate:scan": "cd tools/integration-monitor && npm run scan",
    "integrate:validate": "cd tools/integration-monitor && npm run validate",
    "integrate:register": "cd tools/integration-monitor && npm run register",
    "integrate:report": "cd tools/integration-monitor && npm run report",
    "integrate:watch": "cd tools/integration-monitor && npm run watch",
    "supreme:launch": "node scripts/master-orchestrator.js",
    "supreme:install": "node scripts/master-orchestrator.js --install-deps",
    "supreme:services": "node scripts/master-orchestrator.js --services-only",
    "supreme:frontends": "node scripts/master-orchestrator.js --frontends-only",
    "supreme:full": "node scripts/master-orchestrator.js --install-deps",
    "dev:all": "concurrently \"npm run dev:services\" \"npm run dev:frontend\"",
    "dev:services": "concurrently \"npm run dev:api-gateway\" \"npm run dev:auth\" \"npm run dev:mint\" \"npm run dev:lms\" \"npm run dev:forge\" \"npm run dev:nexus\" \"npm run dev:education\" \"npm run dev:payments\" \"npm run dev:oracle\"",
    "dev:frontend": "cd apps/app && npm run dev",
    "dev:api-gateway": "cd services/api-gateway && npm run dev",
    "dev:auth": "cd services/auth-service && npm run dev",
    "dev:mint": "cd services/azora-mint && node mock-server.js",
    "dev:lms": "cd services/lms && node mock-server.js",
    "dev:forge": "cd services/azora-forge && node mock-server.js",
    "dev:nexus": "cd services/nexus && node mock-server.js",
    "dev:education": "cd services/education && node mock-server.js",
    "dev:payments": "cd services/payments && node mock-server.js",
    "dev:oracle": "cd services/azora-oracle && npm run dev",
    "ghosts:awaken": "cd services/azora-oracle && node src/chamberOfGhostsService.ts",
    "ghosts:status": "curl http://localhost:3005/health",
    "build:all": "npm run build:frontend && npm run build:services",
    "build:frontend": "cd apps/app && npm run build",
    "build:services": "echo 'Building all services...'",
    "test:all": "npm run test:services && npm run test:frontend",
    "test:services": "jest services/**/*.test.js",
    "test:frontend": "cd apps/app && npm test",
    "db:migrate": "npm run db:migrate:all",
    "db:migrate:all": "concurrently \"cd services/auth-service && npm run db:migrate\" \"cd services/mint-service && npm run db:migrate\" \"cd services/lms-service && npm run db:migrate\" \"cd services/forge-service && npm run db:migrate\" \"cd services/nexus-service && npm run db:migrate\" \"cd services/education-service && npm run db:migrate\" \"cd services/payments-service && npm run db:migrate\"",
    "db:generate": "npm run db:generate:all",
    "db:generate:all": "concurrently \"cd services/auth-service && npm run db:generate\" \"cd services/mint-service && npm run db:generate\" \"cd services/forge-service && npm run db:generate\" \"cd services/lms-service && npm run db:generate\" \"cd services/nexus-service && npm run db:generate\" \"cd services/education-service && npm run db:generate\" \"cd services/payments-service && npm run db:generate\"",
    "db:reset": "docker-compose down -v database && docker-compose up -d database",
    "health:check": "node health-check.js",
    "deploy:production": "./scripts/deploy.sh",
    "deploy:windows": "scripts\\deploy-windows.bat",
    "deploy:vercel": "./scripts/deploy-vercel.sh",
    "deploy:design": "./scripts/deploy-design-system.sh",
    "deploy:all": "npm run deploy:design && npm run deploy:vercel",
    "setup:dev": "./scripts/setup-dev.sh",
    "elara:launch": "node tools/elara-ide/launch.js",
    "elara:vscode": "node scripts/launch-elara-vscode.js",
    "docs:generate": "node scripts/update-all-docs.js",
    "docs:update": "node scripts/update-all-docs.js",
    "github:setup": "node scripts/github-automation.js",
    "security:scan": "node scripts/security-scan.js",
    "release": "node scripts/release.js",
    "start": "npm run dev:all",
    "setup:services": "echo 'Services setup complete'",
    "scan:system": "node scripts/system-scanner.js",
    "launch:master": "node scripts/master-orchestrator.js",
    "lint:fix": "cross-env ESLINT_USE_FLAT_CONFIG=true eslint --config config/eslint.config.js health-check.js quick-health-check.js --fix",
    "format": "prettier --write health-check.js quick-health-check.js",
    "format:check": "prettier --check health-check.js quick-health-check.js",
    "types": "tsc --project tsconfig.phase3.json --noEmit"
  },
  "keywords": [
    "azora",
    "operating-system",
    "ai",
    "blockchain",
    "education",
    "finance",
    "quantum-security"
  ],
  "author": "Azora ES (Pty) Ltd",
  "license": "PROPRIETARY",
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "concurrently": "^9.2.1",
    "cors": "2.8.5",
    "crypto": "^1.0.1",
    "express": "5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "1.10.1",
    "openai": "6.8.1",
    "prom-client": "15.1.3",
    "socket.io": "4.8.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.1",
    "turbo": "2.6.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.0.0",
  "workspaces": [
    "apps/*",
    "services/*",
    "packages/*",
    "!services/azora-mint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/azora-os/azora-os.git"
  },
  "bugs": {
    "url": "https://github.com/azora-os/azora-os/issues"
  },
  "homepage": "https://azora.es"
}
