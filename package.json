{
  "name": "azora-os",
  "version": "3.0.0",
  "description": "Constitutional AI Operating System",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:services": "jest services/",
    "lint": "eslint services/",
    "start:all": "node scripts/start-all-services.js",
    "health:check": "./scripts/health-check-all.sh",
    "services:install": "chmod +x scripts/install-services.sh && ./scripts/install-services.sh",
    "services:start": "chmod +x scripts/start-services-daemon.sh && ./scripts/start-services-daemon.sh",
    "services:stop": "chmod +x scripts/stop-services-daemon.sh && ./scripts/stop-services-daemon.sh",
    "extension:install": "cd tools/elara-vscode-extension && npm install && npm run compile && npm run install:local"
  },
  "devDependencies": {
    "@playwright/test": "1.56.1",
    "@swc/core": "^1.5.7",
    "@types/node": "24.10.1",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "1.13.2",
    "compression": "1.8.1",
    "cors": "2.8.5",
    "dotenv": "17.2.3",
    "express": "5.1.0",
    "helmet": "8.1.0",
    "openai": "6.8.1"
  }
}
