{
  "name": "azora.agents",
  "displayName": "Azora Agents",
  "description": "Built-in extension for Azora AI agents (Elara, etc.)",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "AI",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "azora.elara.invoke",
        "title": "Invoke Elara Agent"
      }
      ,
      {
        "command": "azora.startChat",
        "title": "Start Chat with Azora"
      },
      {
        "command": "azora.openAgentSessions",
        "title": "Open Azora Agent Sessions"
      }
      ,
      {
        "command": "azora.agentSessions.refresh",
        "title": "Refresh Azora Agent Sessions"
      }
      ,
      {
        "command": "azora.sankofa.invoke",
        "title": "Invoke Sankofa Agent"
      }
      ,
      {
        "command": "azora.inlineChat",
        "title": "Azora Inline Chat"
      }
      ,
      {
        "command": "azora.openChatWithAgent",
        "title": "Open Chat With Agent"
      }
      ,
      {
        "command": "azora.agentSessions.refresh",
        "title": "Refresh Azora Agent Sessions"
      }
      ,
      {
        "command": "azora.chatAgents.refresh",
        "title": "Refresh Azora Chat Agents"
      }
      ,
      {
        "command": "azora.deadLetter.refresh",
        "title": "Refresh Dead Letter Queue"
      }
      ,
      {
        "command": "azora.deadLetter.retry",
        "title": "Retry Dead Letter Job"
      }
    ]
    ,
    "views": {
      "explorer": [
        {
          "id": "azora.agentSessions",
          "name": "Azora Agent Sessions"
        }
        ,
        {
          "id": "azora.chatAgents",
          "name": "Azora Chat Agents"
        }
          ,
          {
            "id": "azora.deadLetterExplorer",
            "name": "Azora Dead Letter Queue"
          }
      ]
    }
      
    ,
    "chatParticipants": [
      {
        "id": "azora.elara",
        "name": "elara",
        "description": "ELARA — Master Orchestrator AI",
        "isDefault": true
      },
      {
        "id": "azora.sankofa",
        "name": "sankofa",
        "description": "SANKOFA — Code Architect"
      }
      ,
      {
        "id": "azora.kofi",
        "name": "kofi",
        "description": "KOFI — Math Maestro"
      },
      {
        "id": "azora.imani",
        "name": "imani",
        "description": "IMANI — Creative Director"
      },
      {
        "id": "azora.nia",
        "name": "nia",
        "description": "NIA — Data Scientist"
      },
      {
        "id": "azora.amara",
        "name": "amara",
        "description": "AMARA — Simulation Specialist"
      },
      {
        "id": "azora.jabari",
        "name": "jabari",
        "description": "JABARI — Business Strategist"
      },
      {
        "id": "azora.thabo",
        "name": "thabo",
        "description": "THABO — DevOps & Infrastructure"
      },
      {
        "id": "azora.zuri",
        "name": "zuri",
        "description": "ZURI — Science Sage"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.0"
  }
}