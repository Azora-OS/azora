/**
 * Transaction wrapper for multiple operations
 */
async function withTransaction(callback) {
  /* istanbul ignore next */
  cov_1a6e740eem().f[5]++;
  const client =
  /* istanbul ignore next */
  (cov_1a6e740eem().s[22]++, getPrismaClient());
