    /* istanbul ignore next */
    cov_1a6e740eem().s[24]++;
    return await client.$transaction(async tx => {
      /* istanbul ignore next */
      cov_1a6e740eem().f[6]++;
      cov_1a6e740eem().s[25]++;
      return await callback(tx);
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_1a6e740eem().s[26]++;
    throw handlePrismaError(error);
