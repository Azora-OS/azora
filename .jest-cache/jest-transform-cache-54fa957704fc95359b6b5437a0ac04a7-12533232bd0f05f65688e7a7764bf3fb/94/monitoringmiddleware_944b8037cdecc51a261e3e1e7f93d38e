        /* istanbul ignore next */
        _structuredLogger.
        /* istanbul ignore next */
        structuredLogger.warn(`Slow request detected: ${req.method} ${req.path} took ${duration}ms`, {
          requestId: req.requestId,
          userId: req.userId
        }, {
          method: req.method,
          path: req.path,
          duration,
          threshold: slowThresholdMs
        });
      } else
      /* istanbul ignore next */
      {
        cov_tkkm0f9lo().b[6][1]++;
      }
      cov_tkkm0f9lo().s[26]++;
      return originalSend.call(this, data);
    };
    /* istanbul ignore next */
    cov_tkkm0f9lo().s[27]++;
    next();
  };
}

/**
 * Middleware to log request body (for debugging)
 */
function requestBodyLoggingMiddleware(req, res,
// eslint-disable-line @typescript-eslint/no-unused-vars
next) {
  /* istanbul ignore next */
  cov_tkkm0f9lo().f[8]++;
  cov_tkkm0f9lo().s[28]++;
  if (
  /* istanbul ignore next */
  (cov_tkkm0f9lo().b[8][0]++, process.env.LOG_REQUEST_BODY === 'true') &&
  /* istanbul ignore next */
  (cov_tkkm0f9lo().b[8][1]++, req.method !== 'GET')) {
    /* istanbul ignore next */
    cov_tkkm0f9lo().b[7][0]++;
    cov_tkkm0f9lo().s[29]++;
    /* istanbul ignore next */
    _structuredLogger.
    /* istanbul ignore next */
    structuredLogger.debug(`Request body: ${req.method} ${req.path}`, {
      requestId: req.requestId,
      userId: req.userId
    }, {
      body: req.body
    });
  } else
  /* istanbul ignore next */
  {
    cov_tkkm0f9lo().b[7][1]++;
  }
  cov_tkkm0f9lo().s[30]++;
  next();
}
