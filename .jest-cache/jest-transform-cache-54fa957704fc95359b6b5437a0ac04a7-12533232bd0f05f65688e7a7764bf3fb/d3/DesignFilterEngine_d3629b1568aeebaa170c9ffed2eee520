// Minimal stub of DesignFilterEngine for build-time
// TODO: Replace with full implementation

class DesignFilterEngine {
  filters = null;
  constructor(projectRoot) {
    this.root = projectRoot || process.cwd();
  }
  async initialize() {
    const file = path.join(this.root, '.azstudio', 'design-filters.json');
    if (fs.existsSync(file)) {
      try {
        const content = await fs.promises.readFile(file, 'utf-8');
        const parsed = JSON.parse(content);
        this.filters = parsed.filters;
      } catch (err) {
        // fallback to default
        this.filters = this.defaultFilters();
      }
    } else {
      this.filters = this.defaultFilters();
    }
  }
  getFilters() {
    return this.filters || this.defaultFilters();
  }
  getFilterByType(type) {
    return this.getFilters().find(f => f.type === type);
  }
  addFilter(filter) {
    if (!this.filters) this.filters = this.defaultFilters();
    this.filters.push(filter);
  }
  defaultFilters() {
    return [{
      type: 'modern-saas',
      name: 'Modern SaaS',
      description: 'A clean modern SaaS theme',
      tokens: {
        colors: {
          primary: {
            50: '#f7fafc',
            100: '#edf2f7',
            200: '#e2e8f0',
            300: '#cbd5e1',
            400: '#94a3b8',
            500: '#64748b',
            600: '#475569',
            700: '#334155',
            800: '#1f2937',
            900: '#0f172a',
            950: '#020617'
          }
        }
      },
      classTransforms: {
        'btn-primary': 'btn--primary'
      }
    }, {
      type: 'enterprise',
      name: 'Enterprise',
      description: 'A stable design language for enterprise apps',
      tokens: {
        colors: {
          primary: {
            50: '#f8fafc',
            100: '#eef2ff',
            200: '#e0e7ff',
            300: '#c7d2fe',
            400: '#a5b4fc',
            500: '#818cf8',
            600: '#6366f1',
            700: '#4f46e5',
            800: '#4338ca',
            900: '#3730a3',
            950: '#312e81'
          }
        }
      },
      classTransforms: {
        'card-old': 'az-card'
      }
    }];
  }
}
