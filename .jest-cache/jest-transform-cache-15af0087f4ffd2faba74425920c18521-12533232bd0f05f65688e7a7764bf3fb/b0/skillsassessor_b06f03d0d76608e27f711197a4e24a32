    return skills.filter(s => {
      /* istanbul ignore next */
      cov_tk6as30v2().f[9]++;
      cov_tk6as30v2().s[26]++;
      return /* istanbul ignore next */(cov_tk6as30v2().b[6][0]++, s.level === 'expert') ||
      /* istanbul ignore next */
      (cov_tk6as30v2().b[6][1]++, s.level === 'advanced');
    }).map(s => {
      /* istanbul ignore next */
      cov_tk6as30v2().f[10]++;
      cov_tk6as30v2().s[27]++;
      return s.name;
    });
  }
  getRecommendations(skills) {
    /* istanbul ignore next */
    cov_tk6as30v2().f[11]++;
    const weakSkills =
    /* istanbul ignore next */
    (cov_tk6as30v2().s[28]++, skills.filter(s => {
      /* istanbul ignore next */
      cov_tk6as30v2().f[12]++;
      cov_tk6as30v2().s[29]++;
      return /* istanbul ignore next */(cov_tk6as30v2().b[7][0]++, s.level === 'beginner') ||
      /* istanbul ignore next */
      (cov_tk6as30v2().b[7][1]++, s.level === 'intermediate');
    }));
    /* istanbul ignore next */
    cov_tk6as30v2().s[30]++;
    return weakSkills.map(s => {
      /* istanbul ignore next */
      cov_tk6as30v2().f[13]++;
      cov_tk6as30v2().s[31]++;
      return `Improve ${s.name} to advance your career`;
    });
  }
}
