/**
 * Get logo path for a service
 */
export function getLogoPath(shortName, style =
/* istanbul ignore next */
(cov_1rnkcutfog().b[0][0]++, 'geometric'), size =
/* istanbul ignore next */
(cov_1rnkcutfog().b[1][0]++, 'full')) {
  /* istanbul ignore next */
  cov_1rnkcutfog().f[0]++;
  cov_1rnkcutfog().s[1]++;
