  createWallet(userId) {
    /* istanbul ignore next */
    cov_q9orqzdjk().f[1]++;
    const address =
    /* istanbul ignore next */
    (cov_q9orqzdjk().s[4]++, crypto.randomBytes(20).toString('hex'));
    const wallet =
    /* istanbul ignore next */
