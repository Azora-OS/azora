      cov_q9orqzdjk().b[2][1]++;
    }
    const validation =
    /* istanbul ignore next */
    (cov_q9orqzdjk().s[23]++, this.policy.validateTransaction(amount, from.balance));
    /* istanbul ignore next */
    cov_q9orqzdjk().s[24]++;
    if (!validation.valid) {
      /* istanbul ignore next */
      cov_q9orqzdjk().b[4][0]++;
      cov_q9orqzdjk().s[25]++;
      return {
        success: false,
        message: validation.message
      };
    } else
    /* istanbul ignore next */
    {
      cov_q9orqzdjk().b[4][1]++;
    }
    cov_q9orqzdjk().s[26]++;
    from.balance -= amount;
    /* istanbul ignore next */
    cov_q9orqzdjk().s[27]++;
    to.balance += amount;
    /* istanbul ignore next */
    cov_q9orqzdjk().s[28]++;
    this.transactions.push({
      id: crypto.randomBytes(16).toString('hex'),
      type: 'transfer',
      from: fromAddress,
      to: toAddress,
      amount,
      timestamp: Date.now()
    });
    /* istanbul ignore next */
    cov_q9orqzdjk().s[29]++;
    return {
      success: true,
      from: from.balance,
      to: to.balance
    };
  }
  stake(address, amount) {
    /* istanbul ignore next */
    cov_q9orqzdjk().f[5]++;
    const wallet =
    /* istanbul ignore next */
    (cov_q9orqzdjk().s[30]++, this.wallets.get(address));
    /* istanbul ignore next */
    cov_q9orqzdjk().s[31]++;
    if (!wallet) {
      /* istanbul ignore next */
      cov_q9orqzdjk().b[5][0]++;
      cov_q9orqzdjk().s[32]++;
      return {
        success: false,
        message: 'Wallet not found'
      };
    } else
    /* istanbul ignore next */
    {
      cov_q9orqzdjk().b[5][1]++;
    }
    const validation =
    /* istanbul ignore next */
    (cov_q9orqzdjk().s[33]++, this.policy.validateTransaction(amount, wallet.balance));
    /* istanbul ignore next */
    cov_q9orqzdjk().s[34]++;
    if (!validation.valid) {
      /* istanbul ignore next */
      cov_q9orqzdjk().b[6][0]++;
      cov_q9orqzdjk().s[35]++;
      return {
        success: false,
        message: validation.message
      };
    } else
    /* istanbul ignore next */
    {
      cov_q9orqzdjk().b[6][1]++;
    }
    cov_q9orqzdjk().s[36]++;
    wallet.balance -= amount;
    /* istanbul ignore next */
    cov_q9orqzdjk().s[37]++;
    wallet.staked += amount;
    /* istanbul ignore next */
    cov_q9orqzdjk().s[38]++;
    return {
      success: true,
      staked: wallet.staked,
      balance: wallet.balance
    };
  }
  unstake(address, amount) {
    /* istanbul ignore next */
    cov_q9orqzdjk().f[6]++;
    const wallet =
    /* istanbul ignore next */
    (cov_q9orqzdjk().s[39]++, this.wallets.get(address));
    /* istanbul ignore next */
    cov_q9orqzdjk().s[40]++;
    if (
    /* istanbul ignore next */
    (cov_q9orqzdjk().b[8][0]++, !wallet) ||
    /* istanbul ignore next */
    (cov_q9orqzdjk().b[8][1]++, amount > wallet.staked)) {
      /* istanbul ignore next */
      cov_q9orqzdjk().b[7][0]++;
      cov_q9orqzdjk().s[41]++;
      return {
        success: false,
        message: 'Invalid unstake'
      };
    } else
    /* istanbul ignore next */
    {
      cov_q9orqzdjk().b[7][1]++;
    }
    cov_q9orqzdjk().s[42]++;
    wallet.staked -= amount;
    /* istanbul ignore next */
    cov_q9orqzdjk().s[43]++;
    wallet.balance += amount;
    /* istanbul ignore next */
    cov_q9orqzdjk().s[44]++;
    return {
      success: true,
      staked: wallet.staked,
      balance: wallet.balance
    };
  }
  getBalance(address) {
    /* istanbul ignore next */
    cov_q9orqzdjk().f[7]++;
    const wallet =
    /* istanbul ignore next */
    (cov_q9orqzdjk().s[45]++, this.wallets.get(address));
    /* istanbul ignore next */
    cov_q9orqzdjk().s[46]++;
    return wallet ?
    /* istanbul ignore next */
    (cov_q9orqzdjk().b[9][0]++, {
      balance: wallet.balance,
      staked: wallet.staked,
      earned: wallet.earned
    }) :
    /* istanbul ignore next */
    (cov_q9orqzdjk().b[9][1]++, null);
  }
