/* istanbul ignore next */
(cov_15pos7llh9().s[1]++, new Map());

/**
 * Connect to MetaMask or other Web3 wallet
 */
export async function connectWallet() {
  /* istanbul ignore next */
  cov_15pos7llh9().f[0]++;
  cov_15pos7llh9().s[2]++;
  if (!window.ethereum) {
    /* istanbul ignore next */
    cov_15pos7llh9().b[0][0]++;
    cov_15pos7llh9().s[3]++;
    throw new Error('No Web3 wallet found. Please install MetaMask or similar.');
  } else
  /* istanbul ignore next */
  {
    cov_15pos7llh9().b[0][1]++;
  }
  cov_15pos7llh9().s[4]++;
  try {
    // Request account access
    const accounts =
    /* istanbul ignore next */
    (cov_15pos7llh9().s[5]++, await window.ethereum.request({
      method: 'eth_requestAccounts'
    }));
    /* istanbul ignore next */
    cov_15pos7llh9().s[6]++;
    if (
    /* istanbul ignore next */
    (cov_15pos7llh9().b[2][0]++, !accounts) ||
    /* istanbul ignore next */
    (cov_15pos7llh9().b[2][1]++, accounts.length === 0)) {
      /* istanbul ignore next */
      cov_15pos7llh9().b[1][0]++;
      cov_15pos7llh9().s[7]++;
      throw new Error('No accounts found');
    } else
    /* istanbul ignore next */
    {
      cov_15pos7llh9().b[1][1]++;
    }
    const address =
    /* istanbul ignore next */
    (cov_15pos7llh9().s[8]++, accounts[0]);

    // Get chain ID
    const chainIdHex =
    /* istanbul ignore next */
    (cov_15pos7llh9().s[9]++, await window.ethereum.request({
      method: 'eth_chainId'
    }));
    const chainId =
    /* istanbul ignore next */
    (cov_15pos7llh9().s[10]++, parseInt(chainIdHex, 16));

    // Get balance
    const provider =
    /* istanbul ignore next */
    (cov_15pos7llh9().s[11]++, new ethers.BrowserProvider(window.ethereum));
    const balance =
    /* istanbul ignore next */
    (cov_15pos7llh9().s[12]++, await provider.getBalance(address));
    /* istanbul ignore next */
    cov_15pos7llh9().s[13]++;
    return {
      address,
      chainId,
      isConnected: true,
      balance: ethers.formatEther(balance),
      provider: 'MetaMask'
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_15pos7llh9().s[14]++;
    throw new Error(`Wallet connection failed: ${error instanceof Error ?
    /* istanbul ignore next */
    (cov_15pos7llh9().b[3][0]++, error.message) :
    /* istanbul ignore next */
    (cov_15pos7llh9().b[3][1]++, 'Unknown error')}`);
  }
}

/**
 * Disconnect wallet
 */
export function disconnectWallet() {
  /* istanbul ignore next */
  cov_15pos7llh9().f[1]++;
  cov_15pos7llh9().s[15]++;
  // Clear any stored connection data
  signingRequests.clear();
  /* istanbul ignore next */
  cov_15pos7llh9().s[16]++;
  transactionRequests.clear();
}

/**
 * Switch network
 */
export async function switchNetwork(chainId) {
  /* istanbul ignore next */
  cov_15pos7llh9().f[2]++;
  cov_15pos7llh9().s[17]++;
  if (!window.ethereum) {
    /* istanbul ignore next */
    cov_15pos7llh9().b[4][0]++;
    cov_15pos7llh9().s[18]++;
    throw new Error('No Web3 wallet found');
  } else
  /* istanbul ignore next */
  {
    cov_15pos7llh9().b[4][1]++;
  }
  cov_15pos7llh9().s[19]++;
  try {
    const chainIdHex =
    /* istanbul ignore next */
    (cov_15pos7llh9().s[20]++, `0x${chainId.toString(16)}`);
    /* istanbul ignore next */
    cov_15pos7llh9().s[21]++;
    await window.ethereum.request({
      method: 'wallet_switchEthereumChain',
      params: [{
        chainId: chainIdHex
      }]
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_15pos7llh9().s[22]++;
    // If chain not added, add it
    if (error.code === 4902) {
      /* istanbul ignore next */
      cov_15pos7llh9().b[5][0]++;
      cov_15pos7llh9().s[23]++;
      await addNetwork(chainId);
    } else {
      /* istanbul ignore next */
      cov_15pos7llh9().b[5][1]++;
      cov_15pos7llh9().s[24]++;
      throw new Error(`Network switch failed: ${error.message}`);
