/**
 * Request signature
 */
export async function requestSignature(message, address) {
  /* istanbul ignore next */
  cov_15pos7llh9().f[4]++;
  cov_15pos7llh9().s[31]++;
  if (!window.ethereum) {
    /* istanbul ignore next */
    cov_15pos7llh9().b[9][0]++;
    cov_15pos7llh9().s[32]++;
    throw new Error('No Web3 wallet found');
  } else
  /* istanbul ignore next */
  {
    cov_15pos7llh9().b[9][1]++;
  }
  const request =
  /* istanbul ignore next */
  (cov_15pos7llh9().s[33]++, {
    id: `sig_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`,
    message,
    address,
    status: 'pending',
    createdAt: new Date(),
    expiresAt: new Date(Date.now() + 5 * 60 * 1000) // 5 minutes
  });
  /* istanbul ignore next */
  cov_15pos7llh9().s[34]++;
  signingRequests.set(request.id, request);
  /* istanbul ignore next */
  cov_15pos7llh9().s[35]++;
  try {
    const signature =
    /* istanbul ignore next */
    (cov_15pos7llh9().s[36]++, await window.ethereum.request({
      method: 'personal_sign',
      params: [message, address]
    }));
    /* istanbul ignore next */
    cov_15pos7llh9().s[37]++;
    request.signature = signature;
    /* istanbul ignore next */
    cov_15pos7llh9().s[38]++;
    request.status = 'signed';
    /* istanbul ignore next */
    cov_15pos7llh9().s[39]++;
    return request;
  } catch (error) {
    /* istanbul ignore next */
    cov_15pos7llh9().s[40]++;
    request.status = 'rejected';
    /* istanbul ignore next */
    cov_15pos7llh9().s[41]++;
    throw new Error(`Signature request failed: ${error instanceof Error ?
    /* istanbul ignore next */
    (cov_15pos7llh9().b[10][0]++, error.message) :
    /* istanbul ignore next */
    (cov_15pos7llh9().b[10][1]++, 'Unknown error')}`);
  }
}

/**
