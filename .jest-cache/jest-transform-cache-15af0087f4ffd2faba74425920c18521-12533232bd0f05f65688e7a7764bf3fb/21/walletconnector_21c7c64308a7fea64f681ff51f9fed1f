/**
 * Get current wallet connection
 */
export async function getCurrentConnection() {
  /* istanbul ignore next */
  cov_15pos7llh9().f[8]++;
  cov_15pos7llh9().s[58]++;
  if (!window.ethereum) {
    /* istanbul ignore next */
    cov_15pos7llh9().b[17][0]++;
    cov_15pos7llh9().s[59]++;
    return null;
  } else
  /* istanbul ignore next */
  {
    cov_15pos7llh9().b[17][1]++;
  }
  cov_15pos7llh9().s[60]++;
  try {
    const accounts =
    /* istanbul ignore next */
    (cov_15pos7llh9().s[61]++, await window.ethereum.request({
      method: 'eth_accounts'
    }));
    /* istanbul ignore next */
    cov_15pos7llh9().s[62]++;
    if (
    /* istanbul ignore next */
    (cov_15pos7llh9().b[19][0]++, !accounts) ||
    /* istanbul ignore next */
    (cov_15pos7llh9().b[19][1]++, accounts.length === 0)) {
      /* istanbul ignore next */
      cov_15pos7llh9().b[18][0]++;
      cov_15pos7llh9().s[63]++;
      return null;
    } else
    /* istanbul ignore next */
    {
      cov_15pos7llh9().b[18][1]++;
    }
    const chainIdHex =
    /* istanbul ignore next */
    (cov_15pos7llh9().s[64]++, await window.ethereum.request({
      method: 'eth_chainId'
    }));
    const chainId =
    /* istanbul ignore next */
    (cov_15pos7llh9().s[65]++, parseInt(chainIdHex, 16));
    const provider =
    /* istanbul ignore next */
    (cov_15pos7llh9().s[66]++, new ethers.BrowserProvider(window.ethereum));
    const balance =
    /* istanbul ignore next */
    (cov_15pos7llh9().s[67]++, await provider.getBalance(accounts[0]));
    /* istanbul ignore next */
    cov_15pos7llh9().s[68]++;
    return {
      address: accounts[0],
      chainId,
      isConnected: true,
      balance: ethers.formatEther(balance),
      provider: 'MetaMask'
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_15pos7llh9().s[69]++;
    return null;
  }
}

