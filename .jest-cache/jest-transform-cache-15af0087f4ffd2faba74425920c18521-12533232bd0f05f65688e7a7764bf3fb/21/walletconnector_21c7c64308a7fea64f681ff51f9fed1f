  /* istanbul ignore next */
  cov_15pos7llh9().s[46]++;
  try {
    const accounts =
    /* istanbul ignore next */
    (cov_15pos7llh9().s[47]++, await window.ethereum.request({
      method: 'eth_accounts'
    }));
    /* istanbul ignore next */
    cov_15pos7llh9().s[48]++;
    if (
    /* istanbul ignore next */
    (cov_15pos7llh9().b[13][0]++, !accounts) ||
    /* istanbul ignore next */
    (cov_15pos7llh9().b[13][1]++, accounts.length === 0)) {
      /* istanbul ignore next */
      cov_15pos7llh9().b[12][0]++;
      cov_15pos7llh9().s[49]++;
      throw new Error('No accounts connected');
    } else
    /* istanbul ignore next */
    {
      cov_15pos7llh9().b[12][1]++;
    }
    const hash =
    /* istanbul ignore next */
    (cov_15pos7llh9().s[50]++, await window.ethereum.request({
      method: 'eth_sendTransaction',
      params: [{
        from: accounts[0],
        to,
        value: ethers.parseEther(value).toString(),
        data
      }]
    }));
    /* istanbul ignore next */
    cov_15pos7llh9().s[51]++;
    request.hash = hash;
    /* istanbul ignore next */
    cov_15pos7llh9().s[52]++;
    request.status = 'approved';
    /* istanbul ignore next */
    cov_15pos7llh9().s[53]++;
    return request;
  } catch (error) {
    /* istanbul ignore next */
    cov_15pos7llh9().s[54]++;
    request.status = 'rejected';
    /* istanbul ignore next */
    cov_15pos7llh9().s[55]++;
    throw new Error(`Transaction request failed: ${error instanceof Error ?
    /* istanbul ignore next */
    (cov_15pos7llh9().b[14][0]++, error.message) :
    /* istanbul ignore next */
    (cov_15pos7llh9().b[14][1]++, 'Unknown error')}`);
  }
}

