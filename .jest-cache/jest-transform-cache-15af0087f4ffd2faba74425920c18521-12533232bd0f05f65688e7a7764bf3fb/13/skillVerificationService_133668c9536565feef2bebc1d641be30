   * Get verified skills for a user from Sapiens
   */
  static async getVerifiedSkills(userId) {
    /* istanbul ignore next */
    cov_2k4sz4s4qh().f[0]++;
    cov_2k4sz4s4qh().s[1]++;
    try {
      const response =
      /* istanbul ignore next */
      (cov_2k4sz4s4qh().s[2]++, await axios.get(`${EDUCATION_API_URL}/api/certificates/user/${userId}`));
      const certificates =
      /* istanbul ignore next */
      (cov_2k4sz4s4qh().s[3]++,
      /* istanbul ignore next */
      (cov_2k4sz4s4qh().b[1][0]++, response.data.certificates) ||
      /* istanbul ignore next */
      (cov_2k4sz4s4qh().b[1][1]++, []));
      /* istanbul ignore next */
      cov_2k4sz4s4qh().s[4]++;
      return certificates.map(cert => {
        /* istanbul ignore next */
        cov_2k4sz4s4qh().f[1]++;
        cov_2k4sz4s4qh().s[5]++;
        return {
          id: cert.id,
