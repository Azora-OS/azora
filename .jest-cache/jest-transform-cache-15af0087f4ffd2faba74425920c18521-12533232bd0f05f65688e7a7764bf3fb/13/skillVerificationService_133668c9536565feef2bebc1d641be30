  /**
   * Recommend courses based on in-demand Forge skills
   */
  static async recommendCourses(userId) {
    /* istanbul ignore next */
    cov_2k4sz4s4qh().f[6]++;
    cov_2k4sz4s4qh().s[27]++;
    try {
      // Get user's current skills
      const userSkills =
      /* istanbul ignore next */
      (cov_2k4sz4s4qh().s[28]++, await this.getVerifiedSkills(userId));

      // Get in-demand skills on Forge
      const demandSkills =
      /* istanbul ignore next */
      (cov_2k4sz4s4qh().s[29]++, await this.getInDemandSkills());

      // Find skill gaps
      const skillGaps =
      /* istanbul ignore next */
      (cov_2k4sz4s4qh().s[30]++, demandSkills.filter(ds => {
        /* istanbul ignore next */
        cov_2k4sz4s4qh().f[7]++;
        cov_2k4sz4s4qh().s[31]++;
        return !userSkills.some(us => {
          /* istanbul ignore next */
          cov_2k4sz4s4qh().f[8]++;
          cov_2k4sz4s4qh().s[32]++;
          return us.skillName === ds.name;
        });
      }));

      // Get recommended courses from Sapiens
      const recommendations =
      /* istanbul ignore next */
      (cov_2k4sz4s4qh().s[33]++, []);
      /* istanbul ignore next */
      cov_2k4sz4s4qh().s[34]++;
