        /* istanbul ignore next */
        cov_2k4sz4s4qh().s[35]++;
        try {
          const response =
          /* istanbul ignore next */
          (cov_2k4sz4s4qh().s[36]++, await axios.get(`${EDUCATION_API_URL}/api/courses/search`, {
            params: {
              q: skill.name
            }
          }));
          /* istanbul ignore next */
          cov_2k4sz4s4qh().s[37]++;
          recommendations.push(...response.data.courses);
        } catch (error) {
          /* istanbul ignore next */
          cov_2k4sz4s4qh().s[38]++;
          console.error(`‚ùå Failed to get courses for ${skill.name}:`, error);
        }
      }
      /* istanbul ignore next */
      cov_2k4sz4s4qh().s[39]++;
      return recommendations;
    } catch (error) {
      /* istanbul ignore next */
      cov_2k4sz4s4qh().s[40]++;
      console.error('‚ùå Failed to recommend courses:', error);
      /* istanbul ignore next */
      cov_2k4sz4s4qh().s[41]++;
