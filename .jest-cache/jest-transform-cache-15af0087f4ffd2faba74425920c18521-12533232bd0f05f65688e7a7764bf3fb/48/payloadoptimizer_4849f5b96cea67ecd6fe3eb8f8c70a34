      /* istanbul ignore next */
      (cov_jz77pmpnb().b[0][0]++, JSON.stringify(meta)) :
      /* istanbul ignore next */
      (cov_jz77pmpnb().b[0][1]++, ''));
      /* istanbul ignore next */
      cov_jz77pmpnb().s[2]++;
      return `${timestamp} [PAYLOAD] ${level}: ${message} ${metaStr}`;
    }))
  })]
}));

/**
 * Payload Optimizer for reducing response sizes
 * Implements compression and field selection strategies
 */
export class PayloadOptimizer {
  compressionThreshold =
  /* istanbul ignore next */
  (cov_jz77pmpnb().s[3]++, 1024); // 1KB - compress if larger

  /**
   * Compress response payload
   */
  async compressPayload(data, encoding =
  /* istanbul ignore next */
  (cov_jz77pmpnb().b[1][0]++, 'gzip')) {
    /* istanbul ignore next */
    cov_jz77pmpnb().f[1]++;
    const jsonString =
    /* istanbul ignore next */
    (cov_jz77pmpnb().s[4]++, JSON.stringify(data));
    const buffer =
    /* istanbul ignore next */
    (cov_jz77pmpnb().s[5]++, Buffer.from(jsonString, 'utf-8'));
    /* istanbul ignore next */
    cov_jz77pmpnb().s[6]++;
    return new Promise((resolve, reject) => {
      /* istanbul ignore next */
      cov_jz77pmpnb().f[2]++;
      cov_jz77pmpnb().s[7]++;
      if (encoding === 'gzip') {
        /* istanbul ignore next */
        cov_jz77pmpnb().b[2][0]++;
        cov_jz77pmpnb().s[8]++;
        zlib.gzip(buffer, (err, compressed) => {
          /* istanbul ignore next */
          cov_jz77pmpnb().f[3]++;
          cov_jz77pmpnb().s[9]++;
          if (err) {
            /* istanbul ignore next */
            cov_jz77pmpnb().b[3][0]++;
            cov_jz77pmpnb().s[10]++;
            reject(err);
          } else {
            /* istanbul ignore next */
            cov_jz77pmpnb().b[3][1]++;
            cov_jz77pmpnb().s[11]++;
            resolve(compressed);
          }
        });
      } else {
        /* istanbul ignore next */
        cov_jz77pmpnb().b[2][1]++;
        cov_jz77pmpnb().s[12]++;
        if (encoding === 'deflate') {
          /* istanbul ignore next */
          cov_jz77pmpnb().b[4][0]++;
          cov_jz77pmpnb().s[13]++;
          zlib.deflate(buffer, (err, compressed) => {
            /* istanbul ignore next */
            cov_jz77pmpnb().f[4]++;
            cov_jz77pmpnb().s[14]++;
            if (err) {
              /* istanbul ignore next */
              cov_jz77pmpnb().b[5][0]++;
              cov_jz77pmpnb().s[15]++;
              reject(err);
            } else {
              /* istanbul ignore next */
              cov_jz77pmpnb().b[5][1]++;
              cov_jz77pmpnb().s[16]++;
              resolve(compressed);
            }
          });
        } else {
          /* istanbul ignore next */
          cov_jz77pmpnb().b[4][1]++;
          cov_jz77pmpnb().s[17]++;
          resolve(buffer);
        }
      }
    });
  }

  /**
   * Send compressed response
   */
  async sendCompressed(res, data, encoding =
  /* istanbul ignore next */
  (cov_jz77pmpnb().b[6][0]++, 'gzip')) {
    /* istanbul ignore next */
    cov_jz77pmpnb().f[5]++;
    cov_jz77pmpnb().s[18]++;
    try {
      const compressed =
      /* istanbul ignore next */
      (cov_jz77pmpnb().s[19]++, await this.compressPayload(data, encoding));
      const originalSize =
      /* istanbul ignore next */
      (cov_jz77pmpnb().s[20]++, JSON.stringify(data).length);
      const compressedSize =
      /* istanbul ignore next */
      (cov_jz77pmpnb().s[21]++, compressed.length);
      const ratio =
      /* istanbul ignore next */
      (cov_jz77pmpnb().s[22]++, ((1 - compressedSize / originalSize) * 100).toFixed(2));
      /* istanbul ignore next */
      cov_jz77pmpnb().s[23]++;
      res.set('Content-Encoding', encoding);
      /* istanbul ignore next */
      cov_jz77pmpnb().s[24]++;
      res.set('Content-Type', 'application/json');
      /* istanbul ignore next */
      cov_jz77pmpnb().s[25]++;
      res.set('X-Original-Size', originalSize.toString());
      /* istanbul ignore next */
      cov_jz77pmpnb().s[26]++;
      res.set('X-Compressed-Size', compressedSize.toString());
      /* istanbul ignore next */
      cov_jz77pmpnb().s[27]++;
      res.set('X-Compression-Ratio', `${ratio}%`);
      /* istanbul ignore next */
      cov_jz77pmpnb().s[28]++;
      logger.debug('Payload compressed', {
        originalSize,
        compressedSize,
        ratio: `${ratio}%`,
        encoding
      });
      /* istanbul ignore next */
      cov_jz77pmpnb().s[29]++;
      res.send(compressed);
    } catch (error) {
      /* istanbul ignore next */
      cov_jz77pmpnb().s[30]++;
      logger.error('Compression error', {
        error: error.message
      });
      /* istanbul ignore next */
      cov_jz77pmpnb().s[31]++;
      res.json(data);
    }
  }

  /**
   * Select specific fields from object
   */
  selectFields(obj, fields) {
    /* istanbul ignore next */
    cov_jz77pmpnb().f[6]++;
    const result =
    /* istanbul ignore next */
    (cov_jz77pmpnb().s[32]++, {});
    /* istanbul ignore next */
    cov_jz77pmpnb().s[33]++;
    fields.forEach(field => {
      /* istanbul ignore next */
      cov_jz77pmpnb().f[7]++;
      cov_jz77pmpnb().s[34]++;
      if (field in obj) {
        /* istanbul ignore next */
        cov_jz77pmpnb().b[7][0]++;
        cov_jz77pmpnb().s[35]++;
        result[field] = obj[field];
      } else
      /* istanbul ignore next */
      {
        cov_jz77pmpnb().b[7][1]++;
      }
    });
    /* istanbul ignore next */
    cov_jz77pmpnb().s[36]++;
    return result;
  }

  /**
   * Select fields from array of objects
   */
  selectFieldsFromArray(items, fields) {
    /* istanbul ignore next */
    cov_jz77pmpnb().f[8]++;
    cov_jz77pmpnb().s[37]++;
    return items.map(item => {
      /* istanbul ignore next */
      cov_jz77pmpnb().f[9]++;
      cov_jz77pmpnb().s[38]++;
      return this.selectFields(item, fields);
    });
  }

  /**
   * Remove null/undefined values from object
   */
  removeNullValues(obj) {
    /* istanbul ignore next */
    cov_jz77pmpnb().f[10]++;
    const result =
    /* istanbul ignore next */
    (cov_jz77pmpnb().s[39]++, {});
    /* istanbul ignore next */
    cov_jz77pmpnb().s[40]++;
    Object.entries(obj).forEach(([key, value]) => {
      /* istanbul ignore next */
      cov_jz77pmpnb().f[11]++;
      cov_jz77pmpnb().s[41]++;
      if (
      /* istanbul ignore next */
      (cov_jz77pmpnb().b[9][0]++, value !== null) &&
      /* istanbul ignore next */
      (cov_jz77pmpnb().b[9][1]++, value !== undefined)) {
        /* istanbul ignore next */
        cov_jz77pmpnb().b[8][0]++;
        cov_jz77pmpnb().s[42]++;
        result[key] = value;
      } else
      /* istanbul ignore next */
      {
        cov_jz77pmpnb().b[8][1]++;
      }
    });
    /* istanbul ignore next */
    cov_jz77pmpnb().s[43]++;
    return result;
  }

  /**
   * Truncate large strings
   */
  truncateStrings(obj, maxLength =
  /* istanbul ignore next */
  (cov_jz77pmpnb().b[10][0]++, 100)) {
    /* istanbul ignore next */
    cov_jz77pmpnb().f[12]++;
    const result =
    /* istanbul ignore next */
    (cov_jz77pmpnb().s[44]++, {});
    /* istanbul ignore next */
    cov_jz77pmpnb().s[45]++;
    Object.entries(obj).forEach(([key, value]) => {
      /* istanbul ignore next */
      cov_jz77pmpnb().f[13]++;
      cov_jz77pmpnb().s[46]++;
      if (
      /* istanbul ignore next */
      (cov_jz77pmpnb().b[12][0]++, typeof value === 'string') &&
      /* istanbul ignore next */
      (cov_jz77pmpnb().b[12][1]++, value.length > maxLength)) {
        /* istanbul ignore next */
        cov_jz77pmpnb().b[11][0]++;
        cov_jz77pmpnb().s[47]++;
        result[key] = value.substring(0, maxLength) + '...';
      } else {
        /* istanbul ignore next */
        cov_jz77pmpnb().b[11][1]++;
        cov_jz77pmpnb().s[48]++;
        result[key] = value;
      }
    });
    /* istanbul ignore next */
    cov_jz77pmpnb().s[49]++;
    return result;
  }

  /**
   * Flatten nested objects
   */
  flattenObject(obj, prefix =
  /* istanbul ignore next */
  (cov_jz77pmpnb().b[13][0]++, '')) {
    /* istanbul ignore next */
    cov_jz77pmpnb().f[14]++;
    const result =
    /* istanbul ignore next */
    (cov_jz77pmpnb().s[50]++, {});
    /* istanbul ignore next */
    cov_jz77pmpnb().s[51]++;
    Object.entries(obj).forEach(([key, value]) => {
      /* istanbul ignore next */
      cov_jz77pmpnb().f[15]++;
      const newKey =
      /* istanbul ignore next */
      (cov_jz77pmpnb().s[52]++, prefix ?
      /* istanbul ignore next */
      (cov_jz77pmpnb().b[14][0]++, `${prefix}.${key}`) :
      /* istanbul ignore next */
      (cov_jz77pmpnb().b[14][1]++, key));
      /* istanbul ignore next */
      cov_jz77pmpnb().s[53]++;
      if (
      /* istanbul ignore next */
      (cov_jz77pmpnb().b[16][0]++, value !== null) &&
      /* istanbul ignore next */
      (cov_jz77pmpnb().b[16][1]++, typeof value === 'object') &&
      /* istanbul ignore next */
      (cov_jz77pmpnb().b[16][2]++, !Array.isArray(value))) {
        /* istanbul ignore next */
        cov_jz77pmpnb().b[15][0]++;
        cov_jz77pmpnb().s[54]++;
        Object.assign(result, this.flattenObject(value, newKey));
      } else {
        /* istanbul ignore next */
        cov_jz77pmpnb().b[15][1]++;
        cov_jz77pmpnb().s[55]++;
        result[newKey] = value;
      }
    });
    /* istanbul ignore next */
    cov_jz77pmpnb().s[56]++;
    return result;
  }

  /**
   * Paginate array response
   */
  paginateArray(items, page =
  /* istanbul ignore next */
  (cov_jz77pmpnb().b[17][0]++, 1), pageSize =
  /* istanbul ignore next */
  (cov_jz77pmpnb().b[18][0]++, 20)) {
    /* istanbul ignore next */
    cov_jz77pmpnb().f[16]++;
    const start =
    /* istanbul ignore next */
    (cov_jz77pmpnb().s[57]++, (page - 1) * pageSize);
    const end =
    /* istanbul ignore next */
    (cov_jz77pmpnb().s[58]++, start + pageSize);
    const data =
    /* istanbul ignore next */
    (cov_jz77pmpnb().s[59]++, items.slice(start, end));
    const total =
    /* istanbul ignore next */
    (cov_jz77pmpnb().s[60]++, items.length);
    const hasMore =
    /* istanbul ignore next */
    (cov_jz77pmpnb().s[61]++, end < total);
    /* istanbul ignore next */
    cov_jz77pmpnb().s[62]++;
    return {
      data,
      page,
      pageSize,
      total,
      hasMore
    };
  }

  /**
   * Get payload size
   */
  getPayloadSize(data) {
    /* istanbul ignore next */
    cov_jz77pmpnb().f[17]++;
    cov_jz77pmpnb().s[63]++;
    return JSON.stringify(data).length;
  }

  /**
   * Get payload size in human readable format
   */
  getPayloadSizeFormatted(data) {
    /* istanbul ignore next */
    cov_jz77pmpnb().f[18]++;
    const bytes =
    /* istanbul ignore next */
    (cov_jz77pmpnb().s[64]++, this.getPayloadSize(data));
    /* istanbul ignore next */
    cov_jz77pmpnb().s[65]++;
    if (bytes < 1024) {
      /* istanbul ignore next */
      cov_jz77pmpnb().b[19][0]++;
      cov_jz77pmpnb().s[66]++;
      return `${bytes} B`;
    } else
    /* istanbul ignore next */
    {
      cov_jz77pmpnb().b[19][1]++;
    }
    cov_jz77pmpnb().s[67]++;
    if (bytes < 1024 * 1024) {
      /* istanbul ignore next */
      cov_jz77pmpnb().b[20][0]++;
      cov_jz77pmpnb().s[68]++;
      return `${(bytes / 1024).toFixed(2)} KB`;
    } else
    /* istanbul ignore next */
    {
      cov_jz77pmpnb().b[20][1]++;
    }
    cov_jz77pmpnb().s[69]++;
    return `${(bytes / (1024 * 1024)).toFixed(2)} MB`;
  }

  /**
   * Set compression threshold
   */
  setCompressionThreshold(bytes) {
    /* istanbul ignore next */
    cov_jz77pmpnb().f[19]++;
    cov_jz77pmpnb().s[70]++;
    this.compressionThreshold = bytes;
    /* istanbul ignore next */
    cov_jz77pmpnb().s[71]++;
    logger.info('Compression threshold updated', {
      threshold: bytes
    });
  }

  /**
   * Get compression threshold
   */
  getCompressionThreshold() {
    /* istanbul ignore next */
    cov_jz77pmpnb().f[20]++;
    cov_jz77pmpnb().s[72]++;
    return this.compressionThreshold;
  }

  /**
   * Optimize response payload
   */
  optimizeResponse(data, options =
  /* istanbul ignore next */
  (cov_jz77pmpnb().b[21][0]++, {})) {
    /* istanbul ignore next */
    cov_jz77pmpnb().f[21]++;
    let result =
    /* istanbul ignore next */
    (cov_jz77pmpnb().s[73]++, data);

    // Select fields
    /* istanbul ignore next */
    cov_jz77pmpnb().s[74]++;
    if (options.fields) {
      /* istanbul ignore next */
      cov_jz77pmpnb().b[22][0]++;
      cov_jz77pmpnb().s[75]++;
      if (Array.isArray(result)) {
        /* istanbul ignore next */
        cov_jz77pmpnb().b[23][0]++;
        cov_jz77pmpnb().s[76]++;
        result = this.selectFieldsFromArray(result, options.fields);
      } else {
        /* istanbul ignore next */
        cov_jz77pmpnb().b[23][1]++;
        cov_jz77pmpnb().s[77]++;
        result = this.selectFields(result, options.fields);
      }
    } else
    /* istanbul ignore next */
    {
      cov_jz77pmpnb().b[22][1]++;
    }

    // Remove null values
    cov_jz77pmpnb().s[78]++;
    if (options.removeNull) {
      /* istanbul ignore next */
      cov_jz77pmpnb().b[24][0]++;
      cov_jz77pmpnb().s[79]++;
      if (Array.isArray(result)) {
        /* istanbul ignore next */
        cov_jz77pmpnb().b[25][0]++;
        cov_jz77pmpnb().s[80]++;
        result = result.map(item => {
          /* istanbul ignore next */
          cov_jz77pmpnb().f[22]++;
          cov_jz77pmpnb().s[81]++;
          return this.removeNullValues(item);
        });
      } else {
        /* istanbul ignore next */
        cov_jz77pmpnb().b[25][1]++;
        cov_jz77pmpnb().s[82]++;
        result = this.removeNullValues(result);
      }
    } else
    /* istanbul ignore next */
    {
      cov_jz77pmpnb().b[24][1]++;
    }

    // Truncate strings
    cov_jz77pmpnb().s[83]++;
    if (options.truncateStrings) {
      /* istanbul ignore next */
      cov_jz77pmpnb().b[26][0]++;
      const maxLength =
      /* istanbul ignore next */
      (cov_jz77pmpnb().s[84]++,
      /* istanbul ignore next */
      (cov_jz77pmpnb().b[27][0]++, options.maxStringLength) ||
      /* istanbul ignore next */
      (cov_jz77pmpnb().b[27][1]++, 100));
      /* istanbul ignore next */
      cov_jz77pmpnb().s[85]++;
      if (Array.isArray(result)) {
        /* istanbul ignore next */
        cov_jz77pmpnb().b[28][0]++;
        cov_jz77pmpnb().s[86]++;
        result = result.map(item => {
          /* istanbul ignore next */
          cov_jz77pmpnb().f[23]++;
          cov_jz77pmpnb().s[87]++;
          return this.truncateStrings(item, maxLength);
        });
      } else {
        /* istanbul ignore next */
        cov_jz77pmpnb().b[28][1]++;
        cov_jz77pmpnb().s[88]++;
        result = this.truncateStrings(result, maxLength);
      }
    } else
    /* istanbul ignore next */
    {
      cov_jz77pmpnb().b[26][1]++;
    }

    // Paginate
    cov_jz77pmpnb().s[89]++;
    if (
    /* istanbul ignore next */
    (cov_jz77pmpnb().b[30][0]++, options.paginate) &&
    /* istanbul ignore next */
    (cov_jz77pmpnb().b[30][1]++, Array.isArray(result))) {
      /* istanbul ignore next */
      cov_jz77pmpnb().b[29][0]++;
      cov_jz77pmpnb().s[90]++;
      result = this.paginateArray(result, options.page, options.pageSize);
    } else
    /* istanbul ignore next */
    {
      cov_jz77pmpnb().b[29][1]++;
    }
    cov_jz77pmpnb().s[91]++;
    return result;
  }
}

// Export singleton instance
export const payloadOptimizer =
/* istanbul ignore next */
(cov_jz77pmpnb().s[92]++, new PayloadOptimizer());

/**
 * Common field selections for optimization
 */
export const fieldSelections =
/* istanbul ignore next */
(cov_jz77pmpnb().s[93]++, {
  // User fields
  userBasic: ['id', 'email', 'name', 'avatar'],
  userProfile: ['id', 'email', 'name', 'avatar', 'bio', 'createdAt'],
  userFull: ['id', 'email', 'name', 'avatar', 'bio', 'createdAt', 'updatedAt'],
  // Course fields
  courseBasic: ['id', 'title', 'category', 'thumbnail'],
  courseList: ['id', 'title', 'category', 'thumbnail', 'instructor', 'rating', 'price'],
  courseFull: ['id', 'title', 'description', 'category', 'thumbnail', 'instructor', 'rating', 'price', 'createdAt'],
  // Job fields
  jobBasic: ['id', 'title', 'company', 'location'],
  jobList: ['id', 'title', 'company', 'location', 'salary', 'type', 'postedAt'],
  jobFull: ['id', 'title', 'description', 'company', 'location', 'salary', 'type', 'requirements', 'postedAt'],
  // Wallet fields
  walletBasic: ['id', 'balance', 'currency'],
  walletFull: ['id', 'balance', 'currency', 'lastUpdated', 'transactions']
});

/**
 * Response optimization presets
 */
export const responsePresets =
/* istanbul ignore next */
(cov_jz77pmpnb().s[94]++, {
  minimal: {
    removeNull: true,
    truncateStrings: true,
    maxStringLength: 50
  },
  standard: {
    removeNull: true,
    truncateStrings: true,
    maxStringLength: 200
  },
  full: {
    removeNull: false,
    truncateStrings: false
  }
});
