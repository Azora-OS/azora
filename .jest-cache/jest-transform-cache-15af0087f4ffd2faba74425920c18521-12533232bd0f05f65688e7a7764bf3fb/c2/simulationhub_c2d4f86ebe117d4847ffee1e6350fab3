import { phetIntegration } from './phet-integration';
export class SimulationHub {
  /**
   * Get all available simulations (PhET + Custom)
   */
  getAllSimulations() {
    /* istanbul ignore next */
    cov_912dqgfna().f[0]++;
    cov_912dqgfna().s[0]++;
    return {
      phet: phetIntegration.getAllSimulations(),
      custom: {
