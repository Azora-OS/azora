    }

    // Default to English
    cov_1ze3jhk0zh().s[69]++;
    return 'en';
  }

  /**
   * Get supported languages
   */
  getSupportedLanguages() {
    /* istanbul ignore next */
    cov_1ze3jhk0zh().f[9]++;
    cov_1ze3jhk0zh().s[70]++;
    return ['en', 'fr', 'sw', 'ar', 'pt', 'am', 'yo', 'zu'];
  }

  /**
   * Get language name
   */
  getLanguageName(language) {
    /* istanbul ignore next */
    cov_1ze3jhk0zh().f[10]++;
    const names =
    /* istanbul ignore next */
    (cov_1ze3jhk0zh().s[71]++, {
      en: 'English',
      fr: 'Français',
      sw: 'Kiswahili',
      ar: 'العربية',
      pt: 'Português',
      am: 'አማርኛ',
      yo: 'Yorùbá',
      zu: 'isiZulu'
    });
    /* istanbul ignore next */
    cov_1ze3jhk0zh().s[72]++;
    return /* istanbul ignore next */(cov_1ze3jhk0zh().b[18][0]++, names[language]) ||
    /* istanbul ignore next */
    (cov_1ze3jhk0zh().b[18][1]++, 'Unknown');
  }
}
export default LanguageDetector;
