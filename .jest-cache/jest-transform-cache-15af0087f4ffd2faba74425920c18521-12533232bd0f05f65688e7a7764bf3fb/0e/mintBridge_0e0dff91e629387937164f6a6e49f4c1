import axios from 'axios';
const MINT_API_URL =
/* istanbul ignore next */
(cov_avhenk8bl().s[0]++,
/* istanbul ignore next */
(cov_avhenk8bl().b[0][0]++, process.env.MINT_API_URL) ||
/* istanbul ignore next */
(cov_avhenk8bl().b[0][1]++, 'http://localhost:3001'));

/**
 * üí∞ MINT BRIDGE - Connect Forge to Azora Mint
 * 
 * All payments in Forge go through Mint.
 * This includes:
 * - Escrow funding
 * - Fund releases
 * - Refunds
 * - Platform fees
 * - Learn-to-Earn bonuses
 */
export class MintBridge {
  /**
   * Hold funds in escrow
   */
  static async holdFunds(data) {
    /* istanbul ignore next */
    cov_avhenk8bl().f[0]++;
    cov_avhenk8bl().s[1]++;
    try {
      const response =
      /* istanbul ignore next */
      (cov_avhenk8bl().s[2]++, await axios.post(`${MINT_API_URL}/api/escrow/hold`, {
        amount: data.amount,
        currency: data.currency,
        fromUserId: data.fromUserId,
        metadata: {
          service: 'azora-forge',
          escrowId: data.escrowId,
          transactionHash: data.transactionHash
        }
      }));
      /* istanbul ignore next */
      cov_avhenk8bl().s[3]++;
      return {
        success: true,
        transactionId: response.data.transactionId
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_avhenk8bl().s[4]++;
      console.error('‚ùå Mint holdFunds failed:', error);
      /* istanbul ignore next */
      cov_avhenk8bl().s[5]++;
      throw new Error('Failed to hold funds in escrow');
    }
  }

  /**
   * Release funds from escrow to seller
   */
  static async releaseFunds(data) {
    /* istanbul ignore next */
    cov_avhenk8bl().f[1]++;
    cov_avhenk8bl().s[6]++;
    try {
      const response =
      /* istanbul ignore next */
      (cov_avhenk8bl().s[7]++, await axios.post(`${MINT_API_URL}/api/escrow/release`, {
        escrowId: data.escrowId,
        amount: data.amount,
        toUserId: data.toUserId,
        reason: data.reason,
        metadata: {
          service: 'azora-forge'
        }
      }));

      // Calculate and distribute platform fees
      /* istanbul ignore next */
      cov_avhenk8bl().s[8]++;
      await this.distributePlatformFees({
        amount: data.amount,
        sellerId: data.toUserId,
        projectId: data.escrowId
      });
      /* istanbul ignore next */
      cov_avhenk8bl().s[9]++;
      return {
        success: true,
        transactionId: response.data.transactionId
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_avhenk8bl().s[10]++;
      console.error('‚ùå Mint releaseFunds failed:', error);
      /* istanbul ignore next */
      cov_avhenk8bl().s[11]++;
      throw new Error('Failed to release funds');
    }
  }

  /**
   * Distribute platform fees
   * Takes commission and distributes to:
   * - Platform (Azora)
   * - Learn-to-Earn bonus pool
   * - Metabolic tax (20%)
   */
  static async distributePlatformFees(data) {
    /* istanbul ignore next */
    cov_avhenk8bl().f[2]++;
    const PLATFORM_FEE_RATE =
    /* istanbul ignore next */
    (cov_avhenk8bl().s[12]++, 0.15); // 15% commission
    const METABOLIC_TAX_RATE =
    /* istanbul ignore next */
    (cov_avhenk8bl().s[13]++, 0.20); // 20% constitutional tax
    const LEARN_TO_EARN_RATE =
    /* istanbul ignore next */
    (cov_avhenk8bl().s[14]++, 0.05); // 5% to Learn-to-Earn pool

    const platformFee =
    /* istanbul ignore next */
    (cov_avhenk8bl().s[15]++, data.amount * PLATFORM_FEE_RATE);
    const metabolicTax =
    /* istanbul ignore next */
    (cov_avhenk8bl().s[16]++, platformFee * METABOLIC_TAX_RATE);
    const learnToEarnBonus =
    /* istanbul ignore next */
    (cov_avhenk8bl().s[17]++, platformFee * LEARN_TO_EARN_RATE);
    const platformRevenue =
    /* istanbul ignore next */
    (cov_avhenk8bl().s[18]++, platformFee - metabolicTax - learnToEarnBonus);
    /* istanbul ignore next */
    cov_avhenk8bl().s[19]++;
    try {
      /* istanbul ignore next */
      cov_avhenk8bl().s[20]++;
      // Distribute fees via Mint
      await axios.post(`${MINT_API_URL}/api/fees/distribute`, {
        sellerId: data.sellerId,
        projectId: data.projectId,
        totalAmount: data.amount,
        fees: {
          platformRevenue,
          metabolicTax,
          learnToEarnBonus
        },
        metadata: {
          service: 'azora-forge'
        }
      });
      /* istanbul ignore next */
      cov_avhenk8bl().s[21]++;
      console.log(`‚úÖ Distributed fees for project ${data.projectId}`);
    } catch (error) {
      /* istanbul ignore next */
      cov_avhenk8bl().s[22]++;
      console.error('‚ùå Fee distribution failed:', error);
      // Don't throw - fees can be retried later
    }
  }

  /**
   * Process refund
   */
  static async processRefund(data) {
    /* istanbul ignore next */
    cov_avhenk8bl().f[3]++;
    cov_avhenk8bl().s[23]++;
    try {
      const response =
      /* istanbul ignore next */
      (cov_avhenk8bl().s[24]++, await axios.post(`${MINT_API_URL}/api/escrow/refund`, {
        escrowId: data.escrowId,
        amount: data.amount,
        toUserId: data.toUserId,
        reason: data.reason,
        metadata: {
          service: 'azora-forge'
        }
      }));
      /* istanbul ignore next */
      cov_avhenk8bl().s[25]++;
      return {
        success: true,
        transactionId: response.data.transactionId
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_avhenk8bl().s[26]++;
      console.error('‚ùå Mint processRefund failed:', error);
      /* istanbul ignore next */
      cov_avhenk8bl().s[27]++;
      throw new Error('Failed to process refund');
    }
  }

  /**
   * Get user's balance
   */
  static async getUserBalance(userId) {
    /* istanbul ignore next */
    cov_avhenk8bl().f[4]++;
    cov_avhenk8bl().s[28]++;
    try {
      const response =
      /* istanbul ignore next */
      (cov_avhenk8bl().s[29]++, await axios.get(`${MINT_API_URL}/api/wallet/${userId}/balance`));
      /* istanbul ignore next */
      cov_avhenk8bl().s[30]++;
      return response.data.balance;
    } catch (error) {
      /* istanbul ignore next */
      cov_avhenk8bl().s[31]++;
      console.error('‚ùå Failed to get user balance:', error);
      /* istanbul ignore next */
      cov_avhenk8bl().s[32]++;
      return {
        AZR: 0,
        USD: 0,
        ZAR: 0
      };
    }
  }

  /**
   * Award Learn-to-Earn bonus
   * Users earn extra AZR for completing Forge projects
   */
  static async awardLearnToEarnBonus(data) {
    /* istanbul ignore next */
