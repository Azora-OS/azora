  /**
   * Check if user has sufficient balance
   */
  static async hasSufficientBalance(userId, amount, currency) {
    /* istanbul ignore next */
    cov_avhenk8bl().f[8]++;
    cov_avhenk8bl().s[47]++;
    try {
      const balance =
      /* istanbul ignore next */
      (cov_avhenk8bl().s[48]++, await this.getUserBalance(userId));
      /* istanbul ignore next */
      cov_avhenk8bl().s[49]++;
      return balance[currency] >= amount;
    } catch (error) {
      /* istanbul ignore next */
      cov_avhenk8bl().s[50]++;
      console.error('‚ùå Failed to check balance:', error);
      /* istanbul ignore next */
      cov_avhenk8bl().s[51]++;
      return false;
    }
  }

  /**
   * Enable Mint-Mine Engine for user
   * Users earn passive income while working on Forge projects
   */
  static async enableMintMineEngine(userId) {
    /* istanbul ignore next */
    cov_avhenk8bl().f[9]++;
    cov_avhenk8bl().s[52]++;
    try {
      /* istanbul ignore next */
      cov_avhenk8bl().s[53]++;
      await axios.post(`${MINT_API_URL}/api/mint-mine/enable`, {
        userId,
        source: 'azora-forge',
        learningMultiplier: 2.0 // 2x mining rate while working on Forge
      });
      /* istanbul ignore next */
      cov_avhenk8bl().s[54]++;
      console.log(`‚úÖ Enabled Mint-Mine Engine for ${userId} on Forge`);
    } catch (error) {
      /* istanbul ignore next */
      cov_avhenk8bl().s[55]++;
      console.error('‚ùå Failed to enable Mint-Mine Engine:', error);
    }
  }

  /**
   * Organism Health Check - Ensure Mint is operational
   */
  static async healthCheck() {
    /* istanbul ignore next */
    cov_avhenk8bl().f[10]++;
    cov_avhenk8bl().s[56]++;
    try {
      const response =
      /* istanbul ignore next */
      (cov_avhenk8bl().s[57]++, await axios.get(`${MINT_API_URL}/health`, {
        timeout: 5000
      }));
      /* istanbul ignore next */
      cov_avhenk8bl().s[58]++;
      return response.data.status === 'healthy';
    } catch (error) {
      /* istanbul ignore next */
      cov_avhenk8bl().s[59]++;
      console.error('‚ùå Mint health check failed:', error);
      /* istanbul ignore next */
      cov_avhenk8bl().s[60]++;
      return false;
    }
  }
