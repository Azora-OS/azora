    (cov_28phey1our().s[25]++, await blockchain.updateReputation(address, change));
    /* istanbul ignore next */
    cov_28phey1our().s[26]++;
    res.json({
      success
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_28phey1our().s[27]++;
    res.status(500).json({
      error: error.message
    });
  }
});

// Auth endpoint (public) - proxy to auth service
/* istanbul ignore next */
cov_28phey1our().s[28]++;
app.post('/api/auth/login', async (req, res) => {
  /* istanbul ignore next */
  cov_28phey1our().f[5]++;
  const {
    email,
    password
  } =
  /* istanbul ignore next */
  (cov_28phey1our().s[29]++, req.body);
  /* istanbul ignore next */
  cov_28phey1our().s[30]++;
  if (
  /* istanbul ignore next */
  (cov_28phey1our().b[4][0]++, !email) ||
  /* istanbul ignore next */
  (cov_28phey1our().b[4][1]++, !password)) {
    /* istanbul ignore next */
    cov_28phey1our().b[3][0]++;
    cov_28phey1our().s[31]++;
    return res.status(400).json({
      error: 'Email and password required'
    });
  } else
  /* istanbul ignore next */
  {
    cov_28phey1our().b[3][1]++;
  }
  cov_28phey1our().s[32]++;
  try {
    // Call production auth service
    const authService =
    /* istanbul ignore next */
    (cov_28phey1our().s[33]++, require('../../azora-auth/src/auth-service'));
    const result =
    /* istanbul ignore next */
    (cov_28phey1our().s[34]++, await authService.login({
      email,
      password
    }));
    /* istanbul ignore next */
    cov_28phey1our().s[35]++;
    res.json({
      token: result.accessToken,
      refreshToken: result.refreshToken,
      user: result.user
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_28phey1our().s[36]++;
    res.status(401).json({
      error:
      /* istanbul ignore next */
      (cov_28phey1our().b[5][0]++, error.message) ||
      /* istanbul ignore next */
      (cov_28phey1our().b[5][1]++, 'Authentication failed')
    });
  }
});

// Service Routes Configuration
const routes =
/* istanbul ignore next */
(cov_28phey1our().s[37]++, {
  '/api/blockchain': 'http://localhost:3001',
  '/api/mint': 'http://localhost:3002',
  '/api/constitutional': 'http://localhost:3003'
});

// Protected routes - require authentication
/* istanbul ignore next */
cov_28phey1our().s[38]++;
Object.entries(routes).forEach(([path, target]) => {
  /* istanbul ignore next */
  cov_28phey1our().f[6]++;
  cov_28phey1our().s[39]++;
  app.use(path, authMiddleware, createProxyMiddleware({
    target,
    changeOrigin: true,
    pathRewrite: {
      [`^${path}`]: ''
    },
    onError: (err, req, res) => {
      /* istanbul ignore next */
      cov_28phey1our().f[7]++;
      cov_28phey1our().s[40]++;
      console.error(`[Gateway] Proxy error for ${path}:`, err.message);
      /* istanbul ignore next */
      cov_28phey1our().s[41]++;
      res.status(503).json({
        error: 'Service temporarily unavailable'
      });
    }
  }));
});

// Export app for testing
export { app };

// Start Gateway only if run directly
/* istanbul ignore next */
cov_28phey1our().s[42]++;
if (require.main === module) {
  /* istanbul ignore next */
  cov_28phey1our().b[6][0]++;
  cov_28phey1our().s[43]++;
  (async () => {
    /* istanbul ignore next */
    cov_28phey1our().f[8]++;
    cov_28phey1our().s[44]++;
