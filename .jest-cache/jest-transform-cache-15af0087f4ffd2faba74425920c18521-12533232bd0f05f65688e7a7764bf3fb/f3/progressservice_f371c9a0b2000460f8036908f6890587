import { prisma } from '../index';
import { logger } from '../utils/logger';
export class ProgressService {
  async recordOutcome(data) {
    /* istanbul ignore next */
    cov_vnpyqvh8r().f[0]++;
    cov_vnpyqvh8r().s[0]++;
    try {
      const outcome =
      /* istanbul ignore next */
      (cov_vnpyqvh8r().s[1]++, await prisma.learningOutcome.create({
        data: {
          enrollmentId: data.enrollmentId,
          courseId: data.courseId,
          moduleId: data.moduleId,
          assessmentScore: data.assessmentScore,
          timeSpent: data.timeSpent,
          conceptMastery: Math.min(data.assessmentScore, 100)
        }
      }));
      /* istanbul ignore next */
      cov_vnpyqvh8r().s[2]++;
      logger.info(`Learning outcome recorded: ${outcome.id}`);
      /* istanbul ignore next */
      cov_vnpyqvh8r().s[3]++;
      return outcome;
    } catch (error) {
      /* istanbul ignore next */
      cov_vnpyqvh8r().s[4]++;
      logger.error('Error recording learning outcome:', error);
      /* istanbul ignore next */
      cov_vnpyqvh8r().s[5]++;
      throw error;
    }
  }
  async getEnrollmentProgress(enrollmentId) {
    /* istanbul ignore next */
    cov_vnpyqvh8r().f[1]++;
    cov_vnpyqvh8r().s[6]++;
    try {
      const enrollment =
      /* istanbul ignore next */
      (cov_vnpyqvh8r().s[7]++, await prisma.enrollment.findUnique({
        where: {
          id: enrollmentId
        },
        include: {
          course: {
            include: {
              modules: true
            }
          }
        }
      }));
      /* istanbul ignore next */
      cov_vnpyqvh8r().s[8]++;
      if (!enrollment) {
        /* istanbul ignore next */
        cov_vnpyqvh8r().b[0][0]++;
        cov_vnpyqvh8r().s[9]++;
        throw new Error('Enrollment not found');
      } else
      /* istanbul ignore next */
      {
        cov_vnpyqvh8r().b[0][1]++;
      }
      const outcomes =
      /* istanbul ignore next */
      (cov_vnpyqvh8r().s[10]++, await prisma.learningOutcome.findMany({
        where: {
          enrollmentId
        }
      }));
      const totalModules =
      /* istanbul ignore next */
      (cov_vnpyqvh8r().s[11]++, enrollment.course.modules.length);
      const completedModules =
      /* istanbul ignore next */
      (cov_vnpyqvh8r().s[12]++, new Set(outcomes.map(o => {
        /* istanbul ignore next */
        cov_vnpyqvh8r().f[2]++;
        cov_vnpyqvh8r().s[13]++;
        return o.moduleId;
      })).size);
      const completionRate =
      /* istanbul ignore next */
      (cov_vnpyqvh8r().s[14]++, totalModules > 0 ?
      /* istanbul ignore next */
      (cov_vnpyqvh8r().b[1][0]++, completedModules / totalModules * 100) :
      /* istanbul ignore next */
      (cov_vnpyqvh8r().b[1][1]++, 0));
      const averageScore =
      /* istanbul ignore next */
      (cov_vnpyqvh8r().s[15]++, outcomes.length > 0 ?
      /* istanbul ignore next */
      (cov_vnpyqvh8r().b[2][0]++, outcomes.reduce((sum, o) => {
        /* istanbul ignore next */
        cov_vnpyqvh8r().f[3]++;
        cov_vnpyqvh8r().s[16]++;
        return sum + o.assessmentScore;
      }, 0) / outcomes.length) :
      /* istanbul ignore next */
      (cov_vnpyqvh8r().b[2][1]++, 0));
      const timeSpent =
      /* istanbul ignore next */
      (cov_vnpyqvh8r().s[17]++, outcomes.reduce((sum, o) => {
        /* istanbul ignore next */
        cov_vnpyqvh8r().f[4]++;
        cov_vnpyqvh8r().s[18]++;
        return sum + o.timeSpent;
      }, 0));
      /* istanbul ignore next */
      cov_vnpyqvh8r().s[19]++;
      return {
        enrollment,
        outcomes: outcomes,
        completionRate,
        averageScore,
        timeSpent
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_vnpyqvh8r().s[20]++;
      logger.error('Error getting enrollment progress:', error);
      /* istanbul ignore next */
      cov_vnpyqvh8r().s[21]++;
      throw error;
    }
  }
  async getStudentProgress(studentId) {
    /* istanbul ignore next */
    cov_vnpyqvh8r().f[5]++;
    cov_vnpyqvh8r().s[22]++;
    try {
      const enrollments =
      /* istanbul ignore next */
      (cov_vnpyqvh8r().s[23]++, await prisma.enrollment.findMany({
        where: {
          studentId
        },
        include: {
          course: true,
          outcomes: true
        }
      }));
      const completedCourses =
      /* istanbul ignore next */
      (cov_vnpyqvh8r().s[24]++, enrollments.filter(e => {
        /* istanbul ignore next */
        cov_vnpyqvh8r().f[6]++;
        cov_vnpyqvh8r().s[25]++;
        return e.status === 'completed';
      }).length);
      const allOutcomes =
      /* istanbul ignore next */
      (cov_vnpyqvh8r().s[26]++, enrollments.flatMap(e => {
        /* istanbul ignore next */
        cov_vnpyqvh8r().f[7]++;
        cov_vnpyqvh8r().s[27]++;
        return e.outcomes;
      }));
      const averageScore =
      /* istanbul ignore next */
      (cov_vnpyqvh8r().s[28]++, allOutcomes.length > 0 ?
      /* istanbul ignore next */
      (cov_vnpyqvh8r().b[3][0]++, allOutcomes.reduce((sum, o) => {
        /* istanbul ignore next */
        cov_vnpyqvh8r().f[8]++;
        cov_vnpyqvh8r().s[29]++;
        return sum + o.assessmentScore;
      }, 0) / allOutcomes.length) :
      /* istanbul ignore next */
      (cov_vnpyqvh8r().b[3][1]++, 0));
      const totalTimeSpent =
      /* istanbul ignore next */
      (cov_vnpyqvh8r().s[30]++, allOutcomes.reduce((sum, o) => {
        /* istanbul ignore next */
        cov_vnpyqvh8r().f[9]++;
        cov_vnpyqvh8r().s[31]++;
        return sum + o.timeSpent;
      }, 0));
      /* istanbul ignore next */
      cov_vnpyqvh8r().s[32]++;
      return {
        enrollments,
        totalCourses: enrollments.length,
        completedCourses,
        averageScore,
        totalTimeSpent
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_vnpyqvh8r().s[33]++;
      logger.error('Error getting student progress:', error);
      /* istanbul ignore next */
      cov_vnpyqvh8r().s[34]++;
      throw error;
    }
  }
  async identifyStruggling(enrollmentId, threshold =
  /* istanbul ignore next */
  (cov_vnpyqvh8r().b[4][0]++, 50)) {
    /* istanbul ignore next */
    cov_vnpyqvh8r().f[10]++;
    cov_vnpyqvh8r().s[35]++;
    try {
      const outcomes =
      /* istanbul ignore next */
      (cov_vnpyqvh8r().s[36]++, await prisma.learningOutcome.findMany({
        where: {
          enrollmentId
        }
      }));
      const averageScore =
      /* istanbul ignore next */
      (cov_vnpyqvh8r().s[37]++, outcomes.length > 0 ?
      /* istanbul ignore next */
      (cov_vnpyqvh8r().b[5][0]++, outcomes.reduce((sum, o) => {
        /* istanbul ignore next */
        cov_vnpyqvh8r().f[11]++;
        cov_vnpyqvh8r().s[38]++;
        return sum + o.assessmentScore;
      }, 0) / outcomes.length) :
      /* istanbul ignore next */
      (cov_vnpyqvh8r().b[5][1]++, 0));
      const strugglingConcepts =
      /* istanbul ignore next */
      (cov_vnpyqvh8r().s[39]++, outcomes.filter(o => {
        /* istanbul ignore next */
        cov_vnpyqvh8r().f[12]++;
        cov_vnpyqvh8r().s[40]++;
        return o.assessmentScore < threshold;
      }).map(o => {
        /* istanbul ignore next */
        cov_vnpyqvh8r().f[13]++;
        cov_vnpyqvh8r().s[41]++;
        return `Module ${o.moduleId}`;
      }));
      /* istanbul ignore next */
      cov_vnpyqvh8r().s[42]++;
      return {
        isStruggling: averageScore < threshold,
        averageScore,
        strugglingConcepts
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_vnpyqvh8r().s[43]++;
      logger.error('Error identifying struggling student:', error);
      /* istanbul ignore next */
      cov_vnpyqvh8r().s[44]++;
      throw error;
    }
  }
}
