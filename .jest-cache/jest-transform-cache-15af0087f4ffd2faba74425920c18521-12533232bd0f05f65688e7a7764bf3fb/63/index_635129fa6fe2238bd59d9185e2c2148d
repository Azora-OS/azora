/**
 * Log database operation
 */
export function logDatabaseOperation(logger, operation, table, duration, success, error) {
  /* istanbul ignore next */
  cov_w3hm223zv().f[10]++;
  const level =
  /* istanbul ignore next */
  (cov_w3hm223zv().s[17]++, success ?
  /* istanbul ignore next */
  (cov_w3hm223zv().b[1][0]++, 'info') :
  /* istanbul ignore next */
  (cov_w3hm223zv().b[1][1]++, 'error'));
  const message =
  /* istanbul ignore next */
  (cov_w3hm223zv().s[18]++, `Database ${operation} on ${table}`);
  /* istanbul ignore next */
  cov_w3hm223zv().s[19]++;
  logger.log(level, message, {
    operation,
    table,
    duration: `${duration}ms`,
    success,
    error: error?.message
  });
}

