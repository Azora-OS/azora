/**
 * Winston Structured Logging Setup
 * Provides centralized logging for all Azora services
 */

import winston from 'winston';
/**
 * Create a logger instance for a service
 */
export function createLogger(serviceName) {
  /* istanbul ignore next */
  cov_w3hm223zv().f[0]++;
  const logger =
  /* istanbul ignore next */
  (cov_w3hm223zv().s[0]++, winston.createLogger({
    defaultMeta: {
      service: serviceName
    },
    format: winston.format.combine(winston.format.timestamp({
      format: 'YYYY-MM-DD HH:mm:ss'
    }), winston.format.errors({
      stack: true
    }), winston.format.json(), winston.format.printf(({
      timestamp,
      level,
      message,
      service,
      ...meta
    }) => {
      /* istanbul ignore next */
      cov_w3hm223zv().f[1]++;
      cov_w3hm223zv().s[1]++;
      return JSON.stringify({
        timestamp,
        level,
        service,
        message,
        ...meta
      });
    })),
    transports: [
    // Console output
    new winston.transports.Console({
      format: winston.format.combine(winston.format.colorize(), winston.format.printf(({
        timestamp,
        level,
        message,
        service,
        ...meta
      }) => {
        /* istanbul ignore next */
        cov_w3hm223zv().f[2]++;
        const metaStr =
        /* istanbul ignore next */
        (cov_w3hm223zv().s[2]++, Object.keys(meta).length ?
        /* istanbul ignore next */
        (cov_w3hm223zv().b[0][0]++, JSON.stringify(meta)) :
        /* istanbul ignore next */
        (cov_w3hm223zv().b[0][1]++, ''));
        /* istanbul ignore next */
        cov_w3hm223zv().s[3]++;
        return `${timestamp} [${service}] ${level}: ${message} ${metaStr}`;
      }))
    }),
    // File output - all logs
    new winston.transports.File({
      filename: `logs/${serviceName}-all.log`,
      maxsize: 10485760,
      // 10MB
      maxFiles: 5
    }),
    // File output - errors only
    new winston.transports.File({
      filename: `logs/${serviceName}-error.log`,
      level: 'error',
      maxsize: 10485760,
      maxFiles: 5
    })]
  }));
  /* istanbul ignore next */
  cov_w3hm223zv().s[4]++;
  return logger;
}

/**
 * Express middleware for request logging
 */
export function requestLoggingMiddleware(logger) {
  /* istanbul ignore next */
  cov_w3hm223zv().f[3]++;
  cov_w3hm223zv().s[5]++;
  return (req, res, next) => {
    /* istanbul ignore next */
    cov_w3hm223zv().f[4]++;
    const start =
    /* istanbul ignore next */
    (cov_w3hm223zv().s[6]++, Date.now());

    // Capture response
    const originalSend =
    /* istanbul ignore next */
    (cov_w3hm223zv().s[7]++, res.send);
    /* istanbul ignore next */
    cov_w3hm223zv().s[8]++;
    res.send = function (data) {
      /* istanbul ignore next */
      cov_w3hm223zv().f[5]++;
      const duration =
      /* istanbul ignore next */
      (cov_w3hm223zv().s[9]++, Date.now() - start);
      /* istanbul ignore next */
      cov_w3hm223zv().s[10]++;
      logger.info('HTTP Request', {
        method: req.method,
        path: req.path,
        status: res.statusCode,
        duration: `${duration}ms`,
        ip: req.ip,
        userAgent: req.get('user-agent'),
        traceId: req.traceId,
        spanId: req.spanId
      });
      /* istanbul ignore next */
      cov_w3hm223zv().s[11]++;
      return originalSend.call(this, data);
    };
    /* istanbul ignore next */
    cov_w3hm223zv().s[12]++;
    next();
  };
}

