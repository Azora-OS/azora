        data: {
          status: 'DISPUTED',
          disputedAt: new Date(),
          disputedBy,
          disputeReason: reason
        }
      });

      // Create dispute record for admin review
      /* istanbul ignore next */
      cov_t8hm6mswa().s[32]++;
      await prisma.dispute.create({
        data: {
          escrowId,
          disputedBy,
          reason,
          status: 'OPEN',
          createdAt: new Date()
        }
      });
      /* istanbul ignore next */
      cov_t8hm6mswa().s[33]++;
      logger.info('Escrow disputed', {
        escrowId,
        disputedBy,
        reason
      });
      /* istanbul ignore next */
      cov_t8hm6mswa().s[34]++;
      return {
        success: true
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_t8hm6mswa().s[35]++;
      logger.error('Escrow dispute failed', {
        error,
        escrowId,
        disputedBy
      });
      /* istanbul ignore next */
      cov_t8hm6mswa().s[36]++;
      throw error;
    }
  }
  async isAdmin(userId) {
    /* istanbul ignore next */
    cov_t8hm6mswa().f[3]++;
    const user =
    /* istanbul ignore next */
    (cov_t8hm6mswa().s[37]++, await prisma.user.findUnique({
      where: {
        id: userId
      },
      select: {
        role: true
      }
    }));
    /* istanbul ignore next */
    cov_t8hm6mswa().s[38]++;
    return user?.role === 'ADMIN';
  }
}
