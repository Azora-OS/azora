import { prisma } from '../../infrastructure/database/prisma-config';
import { logger } from '../../infrastructure/monitoring/logger';
export class EscrowSystem {
  async createEscrow(projectId, clientId, freelancerId, amount) {
    /* istanbul ignore next */
    cov_t8hm6mswa().f[0]++;
    cov_t8hm6mswa().s[0]++;
    try {
      // Check client has sufficient balance
      const clientWallet =
      /* istanbul ignore next */
      (cov_t8hm6mswa().s[1]++, await prisma.wallet.findUnique({
        where: {
          userId: clientId
        }
      }));
      /* istanbul ignore next */
      cov_t8hm6mswa().s[2]++;
      if (
      /* istanbul ignore next */
      (cov_t8hm6mswa().b[1][0]++, !clientWallet) ||
      /* istanbul ignore next */
      (cov_t8hm6mswa().b[1][1]++, clientWallet.balance < amount)) {
        /* istanbul ignore next */
        cov_t8hm6mswa().b[0][0]++;
        cov_t8hm6mswa().s[3]++;
        throw new Error('Insufficient balance');
      } else
      /* istanbul ignore next */
      {
        cov_t8hm6mswa().b[0][1]++;
      }

      // Create escrow record
      const escrow =
      /* istanbul ignore next */
      (cov_t8hm6mswa().s[4]++, await prisma.escrow.create({
        data: {
          projectId,
          clientId,
          freelancerId,
          amount,
          status: 'PENDING',
          createdAt: new Date()
        }
      }));

      // Lock funds in client wallet
      /* istanbul ignore next */
      cov_t8hm6mswa().s[5]++;
      await prisma.wallet.update({
        where: {
          userId: clientId
        },
        data: {
          balance: {
            decrement: amount
          },
          locked: {
            increment: amount
          }
        }
      });

      // Create transaction record
      /* istanbul ignore next */
      cov_t8hm6mswa().s[6]++;
      await prisma.transaction.create({
        data: {
          type: 'ESCROW_LOCK',
          status: 'COMPLETED',
          amount,
          coinType: 'AZR',
          usdEquivalent: amount,
          hash: `escrow-${escrow.id}`,
          senderId: clientWallet.id,
          notes: `Escrow for project ${projectId}`
        }
      });
      /* istanbul ignore next */
      cov_t8hm6mswa().s[7]++;
      logger.info('Escrow created', {
        escrowId: escrow.id,
        projectId,
        amount
      });
      /* istanbul ignore next */
      cov_t8hm6mswa().s[8]++;
      return escrow;
    } catch (error) {
      /* istanbul ignore next */
      cov_t8hm6mswa().s[9]++;
      logger.error('Escrow creation failed', {
        error,
        projectId,
        clientId,
        freelancerId
      });
      /* istanbul ignore next */
      cov_t8hm6mswa().s[10]++;
      throw error;
    }
  }
  async releaseEscrow(escrowId, releasedBy) {
    /* istanbul ignore next */
    cov_t8hm6mswa().f[1]++;
    cov_t8hm6mswa().s[11]++;
    try {
      const escrow =
      /* istanbul ignore next */
      (cov_t8hm6mswa().s[12]++, await prisma.escrow.findUnique({
        where: {
          id: escrowId
        }
      }));
      /* istanbul ignore next */
      cov_t8hm6mswa().s[13]++;
      if (
      /* istanbul ignore next */
      (cov_t8hm6mswa().b[3][0]++, !escrow) ||
      /* istanbul ignore next */
      (cov_t8hm6mswa().b[3][1]++, escrow.status !== 'PENDING')) {
        /* istanbul ignore next */
        cov_t8hm6mswa().b[2][0]++;
        cov_t8hm6mswa().s[14]++;
        throw new Error('Invalid escrow or already processed');
      } else
      /* istanbul ignore next */
      {
        cov_t8hm6mswa().b[2][1]++;
      }

      // Verify authorization (client or admin)
      cov_t8hm6mswa().s[15]++;
      if (
      /* istanbul ignore next */
      (cov_t8hm6mswa().b[5][0]++, releasedBy !== escrow.clientId) &&
      /* istanbul ignore next */
      (cov_t8hm6mswa().b[5][1]++, !(await this.isAdmin(releasedBy)))) {
        /* istanbul ignore next */
        cov_t8hm6mswa().b[4][0]++;
        cov_t8hm6mswa().s[16]++;
        throw new Error('Unauthorized to release escrow');
      } else
      /* istanbul ignore next */
      {
        cov_t8hm6mswa().b[4][1]++;
      }

      // Update escrow status
      cov_t8hm6mswa().s[17]++;
      await prisma.escrow.update({
        where: {
          id: escrowId
        },
        data: {
          status: 'RELEASED',
          releasedAt: new Date(),
          releasedBy
        }
      });

      // Transfer funds to freelancer
      const freelancerWallet =
      /* istanbul ignore next */
      (cov_t8hm6mswa().s[18]++, await prisma.wallet.findUnique({
        where: {
          userId: escrow.freelancerId
        }
      }));
      /* istanbul ignore next */
      cov_t8hm6mswa().s[19]++;
      if (freelancerWallet) {
        /* istanbul ignore next */
        cov_t8hm6mswa().b[6][0]++;
        cov_t8hm6mswa().s[20]++;
        await prisma.wallet.update({
          where: {
            userId: escrow.freelancerId
          },
          data: {
            balance: {
              increment: escrow.amount
            }
          }
        });
      } else
      /* istanbul ignore next */
      {
        cov_t8hm6mswa().b[6][1]++;
      }

      // Unlock funds from client wallet
      cov_t8hm6mswa().s[21]++;
      await prisma.wallet.update({
        where: {
          userId: escrow.clientId
        },
        data: {
          locked: {
            decrement: escrow.amount
          }
        }
      });

      // Create transaction record
      /* istanbul ignore next */
      cov_t8hm6mswa().s[22]++;
      await prisma.transaction.create({
        data: {
          type: 'ESCROW_RELEASE',
          status: 'COMPLETED',
          amount: escrow.amount,
          coinType: 'AZR',
          usdEquivalent: escrow.amount,
          hash: `escrow-release-${escrowId}`,
          recipientId: freelancerWallet?.id,
          notes: `Escrow released for project ${escrow.projectId}`
        }
      });
      /* istanbul ignore next */
      cov_t8hm6mswa().s[23]++;
      logger.info('Escrow released', {
        escrowId,
        releasedBy
      });
      /* istanbul ignore next */
      cov_t8hm6mswa().s[24]++;
      return {
        success: true
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_t8hm6mswa().s[25]++;
      logger.error('Escrow release failed', {
        error,
        escrowId,
        releasedBy
      });
      /* istanbul ignore next */
      cov_t8hm6mswa().s[26]++;
      throw error;
    }
  }
  async disputeEscrow(escrowId, disputedBy, reason) {
    /* istanbul ignore next */
    cov_t8hm6mswa().f[2]++;
    cov_t8hm6mswa().s[27]++;
    try {
      const escrow =
      /* istanbul ignore next */
