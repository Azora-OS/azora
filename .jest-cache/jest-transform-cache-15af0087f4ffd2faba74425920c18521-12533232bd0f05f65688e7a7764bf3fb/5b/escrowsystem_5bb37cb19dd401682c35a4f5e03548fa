          id: escrowId
        }
      }));
      /* istanbul ignore next */
      cov_t8hm6mswa().s[29]++;
      if (
      /* istanbul ignore next */
      (cov_t8hm6mswa().b[8][0]++, !escrow) ||
      /* istanbul ignore next */
      (cov_t8hm6mswa().b[8][1]++, escrow.status !== 'PENDING')) {
        /* istanbul ignore next */
        cov_t8hm6mswa().b[7][0]++;
        cov_t8hm6mswa().s[30]++;
        throw new Error('Invalid escrow or already processed');
      } else
      /* istanbul ignore next */
      {
        cov_t8hm6mswa().b[7][1]++;
      }

      // Update escrow status
      cov_t8hm6mswa().s[31]++;
      await prisma.escrow.update({
