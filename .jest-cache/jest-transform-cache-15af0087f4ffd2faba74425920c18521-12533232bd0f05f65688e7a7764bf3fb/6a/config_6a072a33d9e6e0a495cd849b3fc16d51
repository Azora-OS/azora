      (cov_12lfe1pjvg().b[2][1]++, 'azora-knowledge-ocean')
    },
    openai: {
      apiKey:
      /* istanbul ignore next */
      (cov_12lfe1pjvg().b[3][0]++, process.env.OPENAI_API_KEY) ||
      /* istanbul ignore next */
      (cov_12lfe1pjvg().b[3][1]++, ''),
      embeddingModel:
      /* istanbul ignore next */
      (cov_12lfe1pjvg().b[4][0]++, process.env.OPENAI_EMBEDDING_MODEL) ||
      /* istanbul ignore next */
      (cov_12lfe1pjvg().b[4][1]++, 'text-embedding-ada-002')
    },
    retrieval: {
      internalRatio: parseFloat(
      /* istanbul ignore next */
      (cov_12lfe1pjvg().b[5][0]++, process.env.KNOWLEDGE_OCEAN_INTERNAL_RATIO) ||
      /* istanbul ignore next */
      (cov_12lfe1pjvg().b[5][1]++, '0.7')),
      externalRatio: parseFloat(
