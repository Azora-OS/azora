/**
 * Validate configuration
 */
export function validateConfig(config) {
  /* istanbul ignore next */
  cov_12lfe1pjvg().f[1]++;
  const errors =
  /* istanbul ignore next */
  (cov_12lfe1pjvg().s[1]++, []);

  // Validate Pinecone config
  /* istanbul ignore next */
  cov_12lfe1pjvg().s[2]++;
  if (!config.pinecone.apiKey) {
    /* istanbul ignore next */
    cov_12lfe1pjvg().b[13][0]++;
    cov_12lfe1pjvg().s[3]++;
    errors.push('PINECONE_API_KEY is required');
  } else
  /* istanbul ignore next */
  {
    cov_12lfe1pjvg().b[13][1]++;
  }
  cov_12lfe1pjvg().s[4]++;
  if (!config.pinecone.indexName) {
    /* istanbul ignore next */
    cov_12lfe1pjvg().b[14][0]++;
    cov_12lfe1pjvg().s[5]++;
    errors.push('PINECONE_INDEX_NAME is required');
  } else
  /* istanbul ignore next */
  {
    cov_12lfe1pjvg().b[14][1]++;
  }

  // Validate OpenAI config
  cov_12lfe1pjvg().s[6]++;
  if (!config.openai.apiKey) {
    /* istanbul ignore next */
    cov_12lfe1pjvg().b[15][0]++;
    cov_12lfe1pjvg().s[7]++;
    errors.push('OPENAI_API_KEY is required');
  } else
  /* istanbul ignore next */
  {
    cov_12lfe1pjvg().b[15][1]++;
  }

  // Validate retrieval ratios
  const totalRatio =
  /* istanbul ignore next */
  (cov_12lfe1pjvg().s[8]++, config.retrieval.internalRatio + config.retrieval.externalRatio);
  /* istanbul ignore next */
  cov_12lfe1pjvg().s[9]++;
  if (Math.abs(totalRatio - 1.0) > 0.01) {
    /* istanbul ignore next */
    cov_12lfe1pjvg().b[16][0]++;
    cov_12lfe1pjvg().s[10]++;
    errors.push(`Internal and external ratios must sum to 1.0 (got ${totalRatio})`);
  } else
  /* istanbul ignore next */
  {
    cov_12lfe1pjvg().b[16][1]++;
  }
  cov_12lfe1pjvg().s[11]++;
  if (
  /* istanbul ignore next */
  (cov_12lfe1pjvg().b[18][0]++, config.retrieval.internalRatio < 0) ||
  /* istanbul ignore next */
  (cov_12lfe1pjvg().b[18][1]++, config.retrieval.internalRatio > 1)) {
    /* istanbul ignore next */
    cov_12lfe1pjvg().b[17][0]++;
    cov_12lfe1pjvg().s[12]++;
    errors.push('Internal ratio must be between 0 and 1');
  } else
  /* istanbul ignore next */
  {
    cov_12lfe1pjvg().b[17][1]++;
  }
  cov_12lfe1pjvg().s[13]++;
  if (
  /* istanbul ignore next */
  (cov_12lfe1pjvg().b[20][0]++, config.retrieval.minRelevanceScore < 0) ||
  /* istanbul ignore next */
  (cov_12lfe1pjvg().b[20][1]++, config.retrieval.minRelevanceScore > 1)) {
    /* istanbul ignore next */
    cov_12lfe1pjvg().b[19][0]++;
    cov_12lfe1pjvg().s[14]++;
    errors.push('Min relevance score must be between 0 and 1');
  } else
  /* istanbul ignore next */
  {
    cov_12lfe1pjvg().b[19][1]++;
  }
  cov_12lfe1pjvg().s[15]++;
  if (config.retrieval.maxResults < 1) {
    /* istanbul ignore next */
    cov_12lfe1pjvg().b[21][0]++;
    cov_12lfe1pjvg().s[16]++;
    errors.push('Max results must be at least 1');
  } else
  /* istanbul ignore next */
  {
    cov_12lfe1pjvg().b[21][1]++;
  }

  // Validate performance settings
  cov_12lfe1pjvg().s[17]++;
  if (config.performance.cacheTTL < 0) {
    /* istanbul ignore next */
    cov_12lfe1pjvg().b[22][0]++;
    cov_12lfe1pjvg().s[18]++;
    errors.push('Cache TTL must be non-negative');
  } else
  /* istanbul ignore next */
  {
    cov_12lfe1pjvg().b[22][1]++;
  }
  cov_12lfe1pjvg().s[19]++;
  if (config.performance.batchSize < 1) {
    /* istanbul ignore next */
    cov_12lfe1pjvg().b[23][0]++;
    cov_12lfe1pjvg().s[20]++;
    errors.push('Batch size must be at least 1');
  } else
  /* istanbul ignore next */
  {
    cov_12lfe1pjvg().b[23][1]++;
  }
  cov_12lfe1pjvg().s[21]++;
  if (config.performance.timeoutMs < 100) {
    /* istanbul ignore next */
    cov_12lfe1pjvg().b[24][0]++;
    cov_12lfe1pjvg().s[22]++;
    errors.push('Timeout must be at least 100ms');
  } else
  /* istanbul ignore next */
  {
    cov_12lfe1pjvg().b[24][1]++;
  }
  cov_12lfe1pjvg().s[23]++;
  if (errors.length > 0) {
    /* istanbul ignore next */
    cov_12lfe1pjvg().b[25][0]++;
    cov_12lfe1pjvg().s[24]++;
    throw new Error(`Configuration validation failed:\n${errors.join('\n')}`);
  } else
  /* istanbul ignore next */
  {
    cov_12lfe1pjvg().b[25][1]++;
  }
}

/**
 * Get validated configuration
 */
export function getConfig() {
  /* istanbul ignore next */
  cov_12lfe1pjvg().f[2]++;
  const config =
  /* istanbul ignore next */
  (cov_12lfe1pjvg().s[25]++, loadConfig());
  /* istanbul ignore next */
  cov_12lfe1pjvg().s[26]++;
  validateConfig(config);
  /* istanbul ignore next */
  cov_12lfe1pjvg().s[27]++;
  return config;
}

