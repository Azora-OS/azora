    // Check if token is expired
    cov_21htorimzy().s[7]++;
    if (
    /* istanbul ignore next */
    (cov_21htorimzy().b[4][0]++, this.config.expiresAt) &&
    /* istanbul ignore next */
    (cov_21htorimzy().b[4][1]++, Date.now() >= this.config.expiresAt)) {
