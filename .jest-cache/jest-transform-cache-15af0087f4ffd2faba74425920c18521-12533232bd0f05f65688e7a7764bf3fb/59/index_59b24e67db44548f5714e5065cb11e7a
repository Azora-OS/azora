      /* istanbul ignore next */
      cov_21htorimzy().b[7][0]++;
      cov_21htorimzy().s[22]++;
      return {};
    } else
    /* istanbul ignore next */
    {
      cov_21htorimzy().b[7][1]++;
    }
    cov_21htorimzy().s[23]++;
    return {
      'Authorization': `Bearer ${token}`
    };
  }
}

/**
 * Create JWT token (utility)
 */
export function createJWT(payload, secret, expiresIn =
/* istanbul ignore next */
(cov_21htorimzy().b[8][0]++, 3600)) {
  /* istanbul ignore next */
  cov_21htorimzy().f[7]++;
  // In production: use actual JWT library
  const header =
  /* istanbul ignore next */
  (cov_21htorimzy().s[24]++, btoa(JSON.stringify({
    alg: 'HS256',
    typ: 'JWT'
  })));
  const body =
  /* istanbul ignore next */
  (cov_21htorimzy().s[25]++, btoa(JSON.stringify({
    ...payload,
    exp: Math.floor(Date.now() / 1000) + expiresIn
  })));
  const signature =
  /* istanbul ignore next */
  (cov_21htorimzy().s[26]++, btoa(`${header}.${body}.${secret}`));
  /* istanbul ignore next */
  cov_21htorimzy().s[27]++;
  return `${header}.${body}.${signature}`;
}

/**
 * Decode JWT token (utility)
 */
export function decodeJWT(token) {
  /* istanbul ignore next */
  cov_21htorimzy().f[8]++;
  cov_21htorimzy().s[28]++;
  try {
    const [header, payload, signature] =
    /* istanbul ignore next */
    (cov_21htorimzy().s[29]++, token.split('.'));
    /* istanbul ignore next */
    cov_21htorimzy().s[30]++;
    return {
      header: JSON.parse(atob(header)),
      payload: JSON.parse(atob(payload)),
      signature
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_21htorimzy().s[31]++;
    return null;
  }
}

/**
 * Check if JWT is expired
 */
export function isJWTExpired(token) {
  /* istanbul ignore next */
  cov_21htorimzy().f[9]++;
  const decoded =
  /* istanbul ignore next */
  (cov_21htorimzy().s[32]++, decodeJWT(token));
  /* istanbul ignore next */
  cov_21htorimzy().s[33]++;
  if (!decoded) {
    /* istanbul ignore next */
    cov_21htorimzy().b[9][0]++;
    cov_21htorimzy().s[34]++;
    return true;
  } else
  /* istanbul ignore next */
  {
    cov_21htorimzy().b[9][1]++;
  }
  const exp =
  /* istanbul ignore next */
  (cov_21htorimzy().s[35]++, decoded.payload.exp);
  /* istanbul ignore next */
  cov_21htorimzy().s[36]++;
  if (!exp) {
    /* istanbul ignore next */
    cov_21htorimzy().b[10][0]++;
    cov_21htorimzy().s[37]++;
    return false;
  } else
  /* istanbul ignore next */
  {
    cov_21htorimzy().b[10][1]++;
  }
  cov_21htorimzy().s[38]++;
  return Date.now() >= exp * 1000;
}
