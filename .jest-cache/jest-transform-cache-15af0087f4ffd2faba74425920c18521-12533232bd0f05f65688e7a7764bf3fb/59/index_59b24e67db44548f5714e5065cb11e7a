      /* istanbul ignore next */
      cov_21htorimzy().b[1][0]++;
      cov_21htorimzy().s[6]++;
      return /* istanbul ignore next */(cov_21htorimzy().b[2][0]++, this.config.apiKey) ||
      /* istanbul ignore next */
      (cov_21htorimzy().b[2][1]++, null);
    } else
    /* istanbul ignore next */
    {
      cov_21htorimzy().b[1][1]++;
