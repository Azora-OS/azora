    /* istanbul ignore next */
    cov_1phgzjom8s().s[24]++;
    slides.push({
      type: 'title',
      content: 'Lesson Introduction',
      duration: 3
    });

    // Content slides
    /* istanbul ignore next */
    cov_1phgzjom8s().s[25]++;
    paragraphs.forEach((paragraph, index) => {
      /* istanbul ignore next */
      cov_1phgzjom8s().f[7]++;
      cov_1phgzjom8s().s[26]++;
      slides.push({
        type: 'content',
        content: paragraph.trim(),
        duration: Math.max(5, paragraph.split(' ').length / 2) // ~2 words per second
      });

      // Insert code example slides
      /* istanbul ignore next */
      cov_1phgzjom8s().s[27]++;
      if (codeExamples[index]) {
        /* istanbul ignore next */
        cov_1phgzjom8s().b[6][0]++;
        cov_1phgzjom8s().s[28]++;
        slides.push({
          type: 'code',
          content: codeExamples[index],
          duration: 10
        });
      } else
      /* istanbul ignore next */
      {
        cov_1phgzjom8s().b[6][1]++;
      }
    });

    // Summary slide
    /* istanbul ignore next */
    cov_1phgzjom8s().s[29]++;
    slides.push({
      type: 'summary',
      content: 'Key Takeaways',
      duration: 5
    });
    /* istanbul ignore next */
    cov_1phgzjom8s().s[30]++;
    return slides;
  }
  calculateDuration(text) {
    /* istanbul ignore next */
    cov_1phgzjom8s().f[8]++;
    const words =
    /* istanbul ignore next */
    (cov_1phgzjom8s().s[31]++, text.split(/\s+/).length);
    /* istanbul ignore next */
    cov_1phgzjom8s().s[32]++;
    return Math.ceil(words / 150 * 60); // 150 words per minute
  }
  getMockVideo(params) {
    /* istanbul ignore next */
    cov_1phgzjom8s().f[9]++;
    cov_1phgzjom8s().s[33]++;
    return {
      url: `/videos/mock-${params.lessonId}.mp4`,
      thumbnail: `/thumbnails/mock-${params.lessonId}.jpg`
    };
  }
  async generateVideoForLesson(lesson, audioUrl) {
    /* istanbul ignore next */
    cov_1phgzjom8s().f[10]++;
    const video =
    /* istanbul ignore next */
    (cov_1phgzjom8s().s[34]++, await this.generateVideo({
      lessonId: lesson.title.toLowerCase().replace(/\s+/g, '-'),
      title: lesson.title,
      script: lesson.script,
      codeExamples: lesson.codeExamples,
      audioUrl: audioUrl,
      style: 'slides'
    }));
    /* istanbul ignore next */
    cov_1phgzjom8s().s[35]++;
    return {
      ...lesson,
      video: {
        url: video.url,
        thumbnail: video.thumbnail
      }
    };
  }
  async generateVideosForCourse(lessons) {
    /* istanbul ignore next */
    cov_1phgzjom8s().f[11]++;
    const lessonsWithVideo =
    /* istanbul ignore next */
    (cov_1phgzjom8s().s[36]++, []);
    /* istanbul ignore next */
    cov_1phgzjom8s().s[37]++;
    for (const lesson of lessons) {
      const lessonWithVideo =
      /* istanbul ignore next */
      (cov_1phgzjom8s().s[38]++, await this.generateVideoForLesson(lesson.content, lesson.content.audio?.url));
      /* istanbul ignore next */
      cov_1phgzjom8s().s[39]++;
      lessonsWithVideo.push({
        ...lesson,
        content: lessonWithVideo
      });
    }
    /* istanbul ignore next */
    cov_1phgzjom8s().s[40]++;
    return lessonsWithVideo;
  }
}
