import { verifyAccessToken } from './auth-service';
/**
 * Middleware to verify JWT and attach user to request
 */
export function requireAuth(req, res, next) {
  /* istanbul ignore next */
  cov_1fnaiqtu8().f[0]++;
  cov_1fnaiqtu8().s[0]++;
  try {
    const authHeader =
    /* istanbul ignore next */
    (cov_1fnaiqtu8().s[1]++, req.headers.authorization);
    /* istanbul ignore next */
    cov_1fnaiqtu8().s[2]++;
    if (
    /* istanbul ignore next */
    (cov_1fnaiqtu8().b[1][0]++, !authHeader) ||
    /* istanbul ignore next */
    (cov_1fnaiqtu8().b[1][1]++, !authHeader.startsWith('Bearer '))) {
      /* istanbul ignore next */
      cov_1fnaiqtu8().b[0][0]++;
      cov_1fnaiqtu8().s[3]++;
      return res.status(401).json({
        error: 'Missing or invalid authorization header',
        message: 'Please provide a valid Bearer token'
      });
    } else
    /* istanbul ignore next */
    {
      cov_1fnaiqtu8().b[0][1]++;
    }
    const token =
    /* istanbul ignore next */
    (cov_1fnaiqtu8().s[4]++, authHeader.substring(7)); // Remove 'Bearer ' prefix
    const payload =
    /* istanbul ignore next */
    (cov_1fnaiqtu8().s[5]++, verifyAccessToken(token));

    // Attach user info to request
    /* istanbul ignore next */
    cov_1fnaiqtu8().s[6]++;
    req.user = payload;
    /* istanbul ignore next */
    cov_1fnaiqtu8().s[7]++;
    next();
  } catch (error) {
    /* istanbul ignore next */
    cov_1fnaiqtu8().s[8]++;
    return res.status(401).json({
      error: 'Invalid or expired token',
      message: 'Please login again'
    });
  }
}

/**
 * Middleware to check user role
 */
export function requireRole(...allowedRoles) {
  /* istanbul ignore next */
  cov_1fnaiqtu8().f[1]++;
  cov_1fnaiqtu8().s[9]++;
  return (req, res, next) => {
    /* istanbul ignore next */
    cov_1fnaiqtu8().f[2]++;
    cov_1fnaiqtu8().s[10]++;
    if (!req.user) {
      /* istanbul ignore next */
      cov_1fnaiqtu8().b[2][0]++;
      cov_1fnaiqtu8().s[11]++;
      return res.status(401).json({
        error: 'Authentication required'
      });
    } else
    /* istanbul ignore next */
    {
      cov_1fnaiqtu8().b[2][1]++;
    }
    cov_1fnaiqtu8().s[12]++;
    if (!allowedRoles.includes(req.user.role)) {
      /* istanbul ignore next */
      cov_1fnaiqtu8().b[3][0]++;
      cov_1fnaiqtu8().s[13]++;
      return res.status(403).json({
        error: 'Insufficient permissions',
        required: allowedRoles,
        current: req.user.role
      });
    } else
    /* istanbul ignore next */
    {
      cov_1fnaiqtu8().b[3][1]++;
    }
    cov_1fnaiqtu8().s[14]++;
    next();
  };
}

/**
 * Optional auth - doesn't fail if no token, but attaches user if valid
 */
export function optionalAuth(req, res, next) {
  /* istanbul ignore next */
  cov_1fnaiqtu8().f[3]++;
  cov_1fnaiqtu8().s[15]++;
  try {
    const authHeader =
    /* istanbul ignore next */
    (cov_1fnaiqtu8().s[16]++, req.headers.authorization);
    /* istanbul ignore next */
    cov_1fnaiqtu8().s[17]++;
    if (
    /* istanbul ignore next */
    (cov_1fnaiqtu8().b[5][0]++, authHeader) &&
    /* istanbul ignore next */
    (cov_1fnaiqtu8().b[5][1]++, authHeader.startsWith('Bearer '))) {
      /* istanbul ignore next */
      cov_1fnaiqtu8().b[4][0]++;
      const token =
      /* istanbul ignore next */
      (cov_1fnaiqtu8().s[18]++, authHeader.substring(7));
      const payload =
      /* istanbul ignore next */
      (cov_1fnaiqtu8().s[19]++, verifyAccessToken(token));
      /* istanbul ignore next */
      cov_1fnaiqtu8().s[20]++;
      req.user = payload;
    } else
    /* istanbul ignore next */
    {
      cov_1fnaiqtu8().b[4][1]++;
    }
  } catch (error) {
    // Silently ignore invalid tokens for optional auth
  }
  /* istanbul ignore next */
  cov_1fnaiqtu8().s[21]++;
  next();
}
