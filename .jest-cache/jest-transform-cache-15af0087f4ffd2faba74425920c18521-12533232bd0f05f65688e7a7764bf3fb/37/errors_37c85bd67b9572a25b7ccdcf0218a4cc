    /* istanbul ignore next */
    this.details = details;
    /* istanbul ignore next */
    cov_97ksulsmu().s[1]++;
    this.name = 'AppError';
    /* istanbul ignore next */
    cov_97ksulsmu().s[2]++;
    Object.setPrototypeOf(this, AppError.prototype);
  }
}

