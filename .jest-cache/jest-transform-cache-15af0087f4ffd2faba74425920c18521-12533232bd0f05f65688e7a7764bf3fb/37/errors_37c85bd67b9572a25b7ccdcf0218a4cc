/**
 * Checks if an error is an AppError
 */
export function isAppError(error) {
  /* istanbul ignore next */
  cov_97ksulsmu().f[12]++;
  cov_97ksulsmu().s[38]++;
  return error instanceof AppError;
}

/**
 * Converts any error to an AppError
 */
export function toAppError(error) {
  /* istanbul ignore next */
  cov_97ksulsmu().f[13]++;
  cov_97ksulsmu().s[39]++;
  if (isAppError(error)) {
    /* istanbul ignore next */
    cov_97ksulsmu().b[12][0]++;
    cov_97ksulsmu().s[40]++;
    return error;
  } else
  /* istanbul ignore next */
  {
    cov_97ksulsmu().b[12][1]++;
  }
  cov_97ksulsmu().s[41]++;
  if (error instanceof Error) {
    /* istanbul ignore next */
    cov_97ksulsmu().b[13][0]++;
    cov_97ksulsmu().s[42]++;
    return new InternalServerError(error.message);
  } else
  /* istanbul ignore next */
  {
    cov_97ksulsmu().b[13][1]++;
  }
  cov_97ksulsmu().s[43]++;
  return new InternalServerError('An unexpected error occurred');
}

/**
 * Error response format
 */

/**
 * Creates a standardized error response
 */
export function createErrorResponse(error, requestId) {
  /* istanbul ignore next */
  cov_97ksulsmu().f[14]++;
  cov_97ksulsmu().s[44]++;
  return {
    success: false,
    code: error.code,
    message: error.message,
    details: error.details,
    timestamp: new Date(),
    requestId
  };
}
