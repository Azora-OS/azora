/**
 * Base application error class
 */
export class AppError extends Error {
  constructor(statusCode, code, message, details) {
    /* istanbul ignore next */
    cov_97ksulsmu().f[0]++;
    cov_97ksulsmu().s[0]++;
    super(message);
    /* istanbul ignore next */
