  /**
   * Get recommended allocation based on current balance
   */
  getRecommendedAllocation() {
    /* istanbul ignore next */
    cov_sdh6ahrd7().f[3]++;
    cov_sdh6ahrd7().s[23]++;
    return {
      education: this.balance * this.allocationRules.education,
