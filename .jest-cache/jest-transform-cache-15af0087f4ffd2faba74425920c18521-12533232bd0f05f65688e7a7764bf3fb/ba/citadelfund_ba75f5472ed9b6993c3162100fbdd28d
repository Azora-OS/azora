      /* istanbul ignore next */
      cov_sdh6ahrd7().b[0][0]++;
      cov_sdh6ahrd7().s[13]++;
      throw new Error('Insufficient funds in CitadelFund');
    } else
    /* istanbul ignore next */
    {
      cov_sdh6ahrd7().b[0][1]++;
