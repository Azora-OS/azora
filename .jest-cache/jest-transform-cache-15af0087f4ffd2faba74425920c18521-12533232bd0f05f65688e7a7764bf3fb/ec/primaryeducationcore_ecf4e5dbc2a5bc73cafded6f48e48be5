  /**
   * Enroll student in primary education
   */
  async enrollStudent(studentId, gradeLevel, preferences) {
    /* istanbul ignore next */
    cov_r23uq74l7().f[7]++;
    const grade =
    /* istanbul ignore next */
    (cov_r23uq74l7().s[24]++, this.grades.get(gradeLevel));
    /* istanbul ignore next */
    cov_r23uq74l7().s[25]++;
    if (!grade) {
      /* istanbul ignore next */
      cov_r23uq74l7().b[0][0]++;
      cov_r23uq74l7().s[26]++;
      throw new Error(`Grade ${gradeLevel} not found`);
    } else
    /* istanbul ignore next */
    {
      cov_r23uq74l7().b[0][1]++;
    }

    // Create personalized learning path with Elara AI
    const learningPath =
    /* istanbul ignore next */
    (cov_r23uq74l7().s[27]++, await elaraIntegration.getEducationalHelp(`Create personalized learning path for Grade ${gradeLevel} student`, {
      studentId,
      gradeLevel,
      preferences,
      subjects: grade.subjects.map(s => {
        /* istanbul ignore next */
        cov_r23uq74l7().f[8]++;
        cov_r23uq74l7().s[28]++;
        return s.name;
      })
    }));
    const progress =
    /* istanbul ignore next */
    (cov_r23uq74l7().s[29]++, {
      studentId,
      gradeLevel,
      enrollmentDate: new Date(),
      subjects: grade.subjects.map(subject => {
        /* istanbul ignore next */
        cov_r23uq74l7().f[9]++;
        cov_r23uq74l7().s[30]++;
        return {
          subjectId: subject.id,
          progress: 0,
          assessments: [],
          lastActivity: new Date()
        };
      }),
      academicAgent: await this.assignAcademicAgent(studentId, gradeLevel),
      learningPath: learningPath.response
    });
    /* istanbul ignore next */
    cov_r23uq74l7().s[31]++;
    this.studentProgress.set(studentId, progress);
    /* istanbul ignore next */
    cov_r23uq74l7().s[32]++;
    this.emit('student-enrolled', {
      studentId,
      gradeLevel,
      progress
    });
    /* istanbul ignore next */
    cov_r23uq74l7().s[33]++;
    return progress;
  }

  /**
   * Assign Academic Agent to student
   */
  async assignAcademicAgent(studentId, gradeLevel) {
    /* istanbul ignore next */
    cov_r23uq74l7().f[10]++;
    // Use Elara AI to match student with best agent
    const agents =
    /* istanbul ignore next */
    (cov_r23uq74l7().s[34]++, Array.from(this.academicAgents.values()));
    // For now, assign primary tutor agent
    /* istanbul ignore next */
    cov_r23uq74l7().s[35]++;
    return /* istanbul ignore next */(cov_r23uq74l7().b[1][0]++, agents[0]?.id) ||
    /* istanbul ignore next */
    (cov_r23uq74l7().b[1][1]++, 'agent-primary-tutor');
  }

  /**
   * Get student progress
