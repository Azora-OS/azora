  /**
   * Record student assessment
   */
  async recordAssessment(studentId, subjectId, assessment) {
    /* istanbul ignore next */
    cov_r23uq74l7().f[13]++;
    const progress =
    /* istanbul ignore next */
    (cov_r23uq74l7().s[38]++, this.studentProgress.get(studentId));
    /* istanbul ignore next */
    cov_r23uq74l7().s[39]++;
    if (!progress) {
      /* istanbul ignore next */
      cov_r23uq74l7().b[2][0]++;
      cov_r23uq74l7().s[40]++;
      throw new Error('Student not found');
    } else
    /* istanbul ignore next */
    {
      cov_r23uq74l7().b[2][1]++;
    }
    const subjectProgress =
    /* istanbul ignore next */
    (cov_r23uq74l7().s[41]++, progress.subjects.find(s => {
      /* istanbul ignore next */
      cov_r23uq74l7().f[14]++;
      cov_r23uq74l7().s[42]++;
      return s.subjectId === subjectId;
    }));
    /* istanbul ignore next */
    cov_r23uq74l7().s[43]++;
    if (!subjectProgress) {
      /* istanbul ignore next */
      cov_r23uq74l7().b[3][0]++;
      cov_r23uq74l7().s[44]++;
      throw new Error('Subject not found in student progress');
    } else
    /* istanbul ignore next */
    {
      cov_r23uq74l7().b[3][1]++;
    }
    cov_r23uq74l7().s[45]++;
    subjectProgress.assessments.push(assessment);
    /* istanbul ignore next */
    cov_r23uq74l7().s[46]++;
    subjectProgress.lastActivity = new Date();

    // Get Elara AI feedback
    const feedback =
    /* istanbul ignore next */
    (cov_r23uq74l7().s[47]++, await elaraIntegration.getEducationalHelp(`Provide feedback for student assessment`, {
      studentId,
      subjectId,
      score: assessment.score,
      maxScore: assessment.maxScore,
      type: assessment.type
    }));
    /* istanbul ignore next */
    cov_r23uq74l7().s[48]++;
    this.emit('assessment-recorded', {
      studentId,
      subjectId,
      assessment,
      feedback
    });
    /* istanbul ignore next */
    cov_r23uq74l7().s[49]++;
    return feedback;
  }
