/**
 * Basic authentication middleware
 * Checks for Authorization header with Bearer token
 */
cov_1q2ybv958r().s[0]++;
export const authenticate = (req, res, next) => {
  /* istanbul ignore next */
  cov_1q2ybv958r().f[0]++;
  cov_1q2ybv958r().s[1]++;
  try {
    const authHeader =
    /* istanbul ignore next */
    (cov_1q2ybv958r().s[2]++, req.headers.authorization);
    /* istanbul ignore next */
    cov_1q2ybv958r().s[3]++;
    if (
    /* istanbul ignore next */
    (cov_1q2ybv958r().b[1][0]++, !authHeader) ||
    /* istanbul ignore next */
    (cov_1q2ybv958r().b[1][1]++, !authHeader.startsWith('Bearer '))) {
      /* istanbul ignore next */
      cov_1q2ybv958r().b[0][0]++;
      cov_1q2ybv958r().s[4]++;
      throw new AppError(401, 'Missing or invalid authorization header');
    } else
    /* istanbul ignore next */
    {
      cov_1q2ybv958r().b[0][1]++;
    }
    const token =
    /* istanbul ignore next */
    (cov_1q2ybv958r().s[5]++, authHeader.substring(7));

    // TODO: Validate token with JWT or session store
    // For now, extract userId from token (in production, verify signature)
    /* istanbul ignore next */
    cov_1q2ybv958r().s[6]++;
    try {
      const decoded =
      /* istanbul ignore next */
      (cov_1q2ybv958r().s[7]++, JSON.parse(Buffer.from(token, 'base64').toString()));
      /* istanbul ignore next */
      cov_1q2ybv958r().s[8]++;
      req.user = decoded;
      /* istanbul ignore next */
      cov_1q2ybv958r().s[9]++;
      next();
    } catch (error) {
      /* istanbul ignore next */
      cov_1q2ybv958r().s[10]++;
      throw new AppError(401, 'Invalid token');
    }
  } catch (error) {
    /* istanbul ignore next */
    cov_1q2ybv958r().s[11]++;
    next(error);
  }
};

/**
 * Role-based access control middleware
 */
/* istanbul ignore next */
cov_1q2ybv958r().s[12]++;
export const requireRole = allowedRoles => {
  /* istanbul ignore next */
  cov_1q2ybv958r().f[1]++;
  cov_1q2ybv958r().s[13]++;
  return (req, res, next) => {
    /* istanbul ignore next */
    cov_1q2ybv958r().f[2]++;
    cov_1q2ybv958r().s[14]++;
    if (!req.user) {
      /* istanbul ignore next */
      cov_1q2ybv958r().b[2][0]++;
      cov_1q2ybv958r().s[15]++;
      return next(new AppError(401, 'Authentication required'));
    } else
    /* istanbul ignore next */
    {
      cov_1q2ybv958r().b[2][1]++;
    }
    cov_1q2ybv958r().s[16]++;
    if (!allowedRoles.includes(
    /* istanbul ignore next */
    (cov_1q2ybv958r().b[4][0]++, req.user.role) ||
    /* istanbul ignore next */
    (cov_1q2ybv958r().b[4][1]++, ''))) {
      /* istanbul ignore next */
      cov_1q2ybv958r().b[3][0]++;
      cov_1q2ybv958r().s[17]++;
      return next(new AppError(403, 'Insufficient permissions'));
    } else
    /* istanbul ignore next */
    {
      cov_1q2ybv958r().b[3][1]++;
    }
    cov_1q2ybv958r().s[18]++;
    next();
  };
};

/**
 * Optional authentication - doesn't fail if no token
 */
/* istanbul ignore next */
cov_1q2ybv958r().s[19]++;
export const optionalAuth = (req, res, next) => {
  /* istanbul ignore next */
  cov_1q2ybv958r().f[3]++;
  cov_1q2ybv958r().s[20]++;
  try {
    const authHeader =
    /* istanbul ignore next */
    (cov_1q2ybv958r().s[21]++, req.headers.authorization);
    /* istanbul ignore next */
    cov_1q2ybv958r().s[22]++;
    if (
    /* istanbul ignore next */
    (cov_1q2ybv958r().b[6][0]++, authHeader) &&
    /* istanbul ignore next */
    (cov_1q2ybv958r().b[6][1]++, authHeader.startsWith('Bearer '))) {
      /* istanbul ignore next */
      cov_1q2ybv958r().b[5][0]++;
      const token =
      /* istanbul ignore next */
      (cov_1q2ybv958r().s[23]++, authHeader.substring(7));
      /* istanbul ignore next */
      cov_1q2ybv958r().s[24]++;
