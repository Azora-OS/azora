};

/**
 * Authenticate session and return user context
 * Used by AI data access layer
 */
export async function authenticateSession(req) {
  /* istanbul ignore next */
  cov_satmvo6vu().f[2]++;
  cov_satmvo6vu().s[25]++;
  try {
    const token =
    /* istanbul ignore next */
    (cov_satmvo6vu().s[26]++, req.headers.authorization?.split(' ')[1]);
    /* istanbul ignore next */
    cov_satmvo6vu().s[27]++;
    if (!token) {
      /* istanbul ignore next */
      cov_satmvo6vu().b[4][0]++;
      cov_satmvo6vu().s[28]++;
      return null;
    } else
    /* istanbul ignore next */
    {
      cov_satmvo6vu().b[4][1]++;
    }
    const secret =
    /* istanbul ignore next */
    (cov_satmvo6vu().s[29]++, process.env.JWT_SECRET);
    /* istanbul ignore next */
    cov_satmvo6vu().s[30]++;
    if (!secret) {
      /* istanbul ignore next */
      cov_satmvo6vu().b[5][0]++;
      cov_satmvo6vu().s[31]++;
      console.error('JWT_SECRET is not configured');
      /* istanbul ignore next */
      cov_satmvo6vu().s[32]++;
      return null;
    } else
    /* istanbul ignore next */
    {
      cov_satmvo6vu().b[5][1]++;
    }
    const decoded =
    /* istanbul ignore next */
    (cov_satmvo6vu().s[33]++, jwt.verify(token, secret));
    /* istanbul ignore next */
    cov_satmvo6vu().s[34]++;
