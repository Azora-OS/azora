import jwt from 'jsonwebtoken';
/* istanbul ignore next */
/**
 * Authentication middleware
 * Validates JWT token and attaches user information to request
 */
cov_satmvo6vu().s[0]++;
export const authMiddleware = (req, res, next) => {
  /* istanbul ignore next */
  cov_satmvo6vu().f[0]++;
  cov_satmvo6vu().s[1]++;
  try {
    const token =
    /* istanbul ignore next */
    (cov_satmvo6vu().s[2]++, req.headers.authorization?.split(' ')[1]);
    /* istanbul ignore next */
    cov_satmvo6vu().s[3]++;
    if (!token) {
      /* istanbul ignore next */
      cov_satmvo6vu().b[0][0]++;
      cov_satmvo6vu().s[4]++;
      return res.status(401).json({
        success: false,
        error: 'No token provided'
      });
    } else
    /* istanbul ignore next */
    {
      cov_satmvo6vu().b[0][1]++;
    }
    const secret =
    /* istanbul ignore next */
    (cov_satmvo6vu().s[5]++, process.env.JWT_SECRET);
    /* istanbul ignore next */
    cov_satmvo6vu().s[6]++;
    if (!secret) {
      /* istanbul ignore next */
      cov_satmvo6vu().b[1][0]++;
      cov_satmvo6vu().s[7]++;
      console.error('JWT_SECRET is not configured');
      /* istanbul ignore next */
      cov_satmvo6vu().s[8]++;
      return res.status(500).json({
        success: false,
        error: 'Server configuration error'
      });
    } else
    /* istanbul ignore next */
    {
      cov_satmvo6vu().b[1][1]++;
    }
    const decoded =
    /* istanbul ignore next */
    (cov_satmvo6vu().s[9]++, jwt.verify(token, secret));
    /* istanbul ignore next */
    cov_satmvo6vu().s[10]++;
    req.userId = decoded.userId;
    /* istanbul ignore next */
    cov_satmvo6vu().s[11]++;
    req.user = decoded;
    /* istanbul ignore next */
    cov_satmvo6vu().s[12]++;
    next();
  } catch (error) {
    /* istanbul ignore next */
    cov_satmvo6vu().s[13]++;
    return res.status(401).json({
      success: false,
      error: 'Invalid token'
    });
  }
};

/**
 * Optional authentication middleware
 * Attaches user information if token is present, but doesn't fail if missing
 */
/* istanbul ignore next */
cov_satmvo6vu().s[14]++;
export const optionalAuthMiddleware = (req, res, next) => {
  /* istanbul ignore next */
  cov_satmvo6vu().f[1]++;
  cov_satmvo6vu().s[15]++;
  try {
    const token =
    /* istanbul ignore next */
    (cov_satmvo6vu().s[16]++, req.headers.authorization?.split(' ')[1]);
    /* istanbul ignore next */
    cov_satmvo6vu().s[17]++;
    if (token) {
      /* istanbul ignore next */
      cov_satmvo6vu().b[2][0]++;
      const secret =
      /* istanbul ignore next */
      (cov_satmvo6vu().s[18]++, process.env.JWT_SECRET);
      /* istanbul ignore next */
      cov_satmvo6vu().s[19]++;
      if (secret) {
        /* istanbul ignore next */
        cov_satmvo6vu().b[3][0]++;
        const decoded =
        /* istanbul ignore next */
        (cov_satmvo6vu().s[20]++, jwt.verify(token, secret));
        /* istanbul ignore next */
        cov_satmvo6vu().s[21]++;
        req.userId = decoded.userId;
        /* istanbul ignore next */
        cov_satmvo6vu().s[22]++;
