/* istanbul ignore next */
(cov_hlk5o4obp().s[24]++, null);
export function getEnv() {
  /* istanbul ignore next */
  cov_hlk5o4obp().f[8]++;
  cov_hlk5o4obp().s[25]++;
  if (!validatedEnv) {
    /* istanbul ignore next */
    cov_hlk5o4obp().b[4][0]++;
    cov_hlk5o4obp().s[26]++;
    validatedEnv = validateEnv();
  } else
  /* istanbul ignore next */
  {
    cov_hlk5o4obp().b[4][1]++;
  }
  cov_hlk5o4obp().s[27]++;
  return validatedEnv;
}

// Validate on import (optional - can be called explicitly)
/* istanbul ignore next */
cov_hlk5o4obp().s[28]++;
if (process.env.VALIDATE_ENV_ON_START !== 'false') {
  /* istanbul ignore next */
  cov_hlk5o4obp().b[5][0]++;
  cov_hlk5o4obp().s[29]++;
  try {
    /* istanbul ignore next */
    cov_hlk5o4obp().s[30]++;
    getEnv();
  } catch (error) {
    /* istanbul ignore next */
    cov_hlk5o4obp().s[31]++;
    // Only throw in production
    if (process.env.NODE_ENV === 'production') {
      /* istanbul ignore next */
      cov_hlk5o4obp().b[6][0]++;
      cov_hlk5o4obp().s[32]++;
      throw error;
    } else
    /* istanbul ignore next */
    {
      cov_hlk5o4obp().b[6][1]++;
    }
    // In development, just warn
    const errorMessage =
    /* istanbul ignore next */
    (cov_hlk5o4obp().s[33]++, error instanceof Error ?
    /* istanbul ignore next */
    (cov_hlk5o4obp().b[7][0]++, error.message) :
    /* istanbul ignore next */
    (cov_hlk5o4obp().b[7][1]++, String(error)));
    /* istanbul ignore next */
    cov_hlk5o4obp().s[34]++;
    log.warn('Environment validation failed (non-blocking in development):', {
      error: errorMessage
    });
  }
} else
/* istanbul ignore next */
{
  cov_hlk5o4obp().b[5][1]++;
}
