    /* istanbul ignore next */
    cov_136kd1yvmt().s[39]++;
    if (!/^[a-z]{2}(-[A-Z]{2})?$/.test(language)) {
      /* istanbul ignore next */
      cov_136kd1yvmt().b[5][0]++;
      cov_136kd1yvmt().s[40]++;
      return res.status(400).json({
        success: false,
        error: 'Invalid language code format'
      });
    } else
    /* istanbul ignore next */
    {
      cov_136kd1yvmt().b[5][1]++;
    }
    const certificate =
    /* istanbul ignore next */
    (cov_136kd1yvmt().s[41]++, await certificateService.generateCertificateInLanguage(enrollmentId, language));
    /* istanbul ignore next */
    cov_136kd1yvmt().s[42]++;
    res.status(201).json({
      success: true,
      data: certificate,
      language
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_136kd1yvmt().s[43]++;
    logger.error('Error generating certificate in language', {
      error
    });
    /* istanbul ignore next */
    cov_136kd1yvmt().s[44]++;
    res.status(500).json({
      success: false,
      error: error instanceof Error ?
      /* istanbul ignore next */
      (cov_136kd1yvmt().b[6][0]++, error.message) :
      /* istanbul ignore next */
      (cov_136kd1yvmt().b[6][1]++, 'Failed to generate certificate')
    });
  }
});
export default router;
