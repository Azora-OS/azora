/**
 * Security Headers Middleware
 * Implements OWASP security best practices
 */

import helmet from 'helmet';
import rateLimit from 'express-rate-limit';
import mongoSanitize from 'express-mongo-sanitize';
import hpp from 'hpp';

/**
 * Configure security headers
 */
export function configureSecurityHeaders(app) {
  /* istanbul ignore next */
  cov_1obopfk4n4().f[0]++;
  cov_1obopfk4n4().s[0]++;
  // Helmet for security headers
  app.use(helmet());

  // Content Security Policy
  /* istanbul ignore next */
  cov_1obopfk4n4().s[1]++;
  app.use(helmet.contentSecurityPolicy({
    directives: {
      defaultSrc: ["'self'"],
      scriptSrc: ["'self'", "'unsafe-inline'"],
      styleSrc: ["'self'", "'unsafe-inline'"],
      imgSrc: ["'self'", 'data:', 'https:'],
      fontSrc: ["'self'"],
      connectSrc: ["'self'"],
      frameSrc: ["'none'"],
      objectSrc: ["'none'"]
    }
  }));

  // X-Frame-Options
  /* istanbul ignore next */
  cov_1obopfk4n4().s[2]++;
  app.use(helmet.frameguard({
    action: 'deny'
  }));

  // X-Content-Type-Options
  /* istanbul ignore next */
  cov_1obopfk4n4().s[3]++;
  app.use(helmet.noSniff());

  // X-XSS-Protection
  /* istanbul ignore next */
  cov_1obopfk4n4().s[4]++;
  app.use(helmet.xssFilter());

  // Referrer-Policy
  /* istanbul ignore next */
  cov_1obopfk4n4().s[5]++;
  app.use(helmet.referrerPolicy({
    policy: 'strict-origin-when-cross-origin'
  }));

  // Permissions-Policy
  /* istanbul ignore next */
  cov_1obopfk4n4().s[6]++;
  app.use(helmet.permittedCrossDomainPolicies({
    permittedPolicies: 'none'
  }));
}

/**
 * Configure rate limiting
 */
export function configureRateLimiting(app) {
  /* istanbul ignore next */
  cov_1obopfk4n4().f[1]++;
  // General API rate limiter
  const apiLimiter =
  /* istanbul ignore next */
  (cov_1obopfk4n4().s[7]++, rateLimit({
    windowMs: 15 * 60 * 1000,
    // 15 minutes
    max: 100,
    // limit each IP to 100 requests per windowMs
    message: 'Too many requests from this IP, please try again later.',
    standardHeaders: true,
    // Return rate limit info in the `RateLimit-*` headers
    legacyHeaders: false // Disable the `X-RateLimit-*` headers
  }));

  // Strict rate limiter for auth endpoints
  const authLimiter =
  /* istanbul ignore next */
  (cov_1obopfk4n4().s[8]++, rateLimit({
    windowMs: 15 * 60 * 1000,
    // 15 minutes
    max: 5,
    // limit each IP to 5 requests per windowMs
    message: 'Too many login attempts, please try again later.',
    skipSuccessfulRequests: true // Don't count successful requests
  }));

  // Apply rate limiters
  /* istanbul ignore next */
  cov_1obopfk4n4().s[9]++;
  app.use('/api/', apiLimiter);
  /* istanbul ignore next */
  cov_1obopfk4n4().s[10]++;
  app.use('/api/auth/login', authLimiter);
  /* istanbul ignore next */
  cov_1obopfk4n4().s[11]++;
  app.use('/api/auth/register', authLimiter);
  /* istanbul ignore next */
  cov_1obopfk4n4().s[12]++;
  app.use('/api/auth/forgot-password', authLimiter);
}

/**
 * Configure data sanitization
 */
export function configureSanitization(app) {
  /* istanbul ignore next */
  cov_1obopfk4n4().f[2]++;
  cov_1obopfk4n4().s[13]++;
  // Data sanitization against NoSQL injection
  app.use(mongoSanitize());

  // Prevent HTTP Parameter Pollution attacks
  /* istanbul ignore next */
  cov_1obopfk4n4().s[14]++;
  app.use(hpp());
}

/**
 * Configure CORS
 */
export function configureCORS(app) {
  /* istanbul ignore next */
  cov_1obopfk4n4().f[3]++;
  const cors =
  /* istanbul ignore next */
  (cov_1obopfk4n4().s[15]++, require('cors'));
  const corsOptions =
  /* istanbul ignore next */
  (cov_1obopfk4n4().s[16]++, {
    origin:
    /* istanbul ignore next */
    (cov_1obopfk4n4().b[0][0]++, process.env.CORS_ORIGIN) ||
    /* istanbul ignore next */
    (cov_1obopfk4n4().b[0][1]++, 'http://localhost:3000'),
    credentials: true,
    methods: ['GET', 'POST', 'PUT', 'DELETE', 'PATCH', 'OPTIONS'],
    allowedHeaders: ['Content-Type', 'Authorization'],
    maxAge: 86400 // 24 hours
  });
  /* istanbul ignore next */
  cov_1obopfk4n4().s[17]++;
  app.use(cors(corsOptions));
}

/**
 * Security headers middleware
 */
export function securityHeadersMiddleware(req, res, next) {
  /* istanbul ignore next */
  cov_1obopfk4n4().f[4]++;
  cov_1obopfk4n4().s[18]++;
  // Prevent MIME type sniffing
  res.setHeader('X-Content-Type-Options', 'nosniff');

  // Enable XSS protection
  /* istanbul ignore next */
  cov_1obopfk4n4().s[19]++;
  res.setHeader('X-XSS-Protection', '1; mode=block');

  // Prevent clickjacking
  /* istanbul ignore next */
  cov_1obopfk4n4().s[20]++;
  res.setHeader('X-Frame-Options', 'DENY');

  // Referrer Policy
  /* istanbul ignore next */
  cov_1obopfk4n4().s[21]++;
  res.setHeader('Referrer-Policy', 'strict-origin-when-cross-origin');

  // Feature Policy / Permissions Policy
  /* istanbul ignore next */
  cov_1obopfk4n4().s[22]++;
  res.setHeader('Permissions-Policy', 'geolocation=(), microphone=(), camera=()');

  // Strict Transport Security (HSTS)
  /* istanbul ignore next */
  cov_1obopfk4n4().s[23]++;
  if (process.env.NODE_ENV === 'production') {
    /* istanbul ignore next */
    cov_1obopfk4n4().b[1][0]++;
    cov_1obopfk4n4().s[24]++;
    res.setHeader('Strict-Transport-Security', 'max-age=31536000; includeSubDomains; preload');
  } else
  /* istanbul ignore next */
  {
    cov_1obopfk4n4().b[1][1]++;
  }
  cov_1obopfk4n4().s[25]++;
  next();
}

/**
 * Remove sensitive headers
 */
export function removeSensitiveHeaders(req, res, next) {
  /* istanbul ignore next */
  cov_1obopfk4n4().f[5]++;
  cov_1obopfk4n4().s[26]++;
  // Remove server information
  res.removeHeader('X-Powered-By');
  /* istanbul ignore next */
  cov_1obopfk4n4().s[27]++;
  res.removeHeader('Server');
  /* istanbul ignore next */
  cov_1obopfk4n4().s[28]++;
  next();
}

/**
 * Setup all security middleware
 */
export function setupSecurityMiddleware(app) {
  /* istanbul ignore next */
  cov_1obopfk4n4().f[6]++;
  cov_1obopfk4n4().s[29]++;
  // Configure security headers
  configureSecurityHeaders(app);

  // Configure rate limiting
  /* istanbul ignore next */
  cov_1obopfk4n4().s[30]++;
  configureRateLimiting(app);

  // Configure data sanitization
  /* istanbul ignore next */
  cov_1obopfk4n4().s[31]++;
  configureSanitization(app);

  // Configure CORS
  /* istanbul ignore next */
  cov_1obopfk4n4().s[32]++;
  configureCORS(app);

  // Apply custom security headers
  /* istanbul ignore next */
  cov_1obopfk4n4().s[33]++;
  app.use(securityHeadersMiddleware);

  // Remove sensitive headers
  /* istanbul ignore next */
  cov_1obopfk4n4().s[34]++;
  app.use(removeSensitiveHeaders);
}
export default setupSecurityMiddleware;
