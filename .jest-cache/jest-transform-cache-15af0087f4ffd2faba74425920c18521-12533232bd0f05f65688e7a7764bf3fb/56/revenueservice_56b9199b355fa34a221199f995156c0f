  async getAllPricingTiers() {
    /* istanbul ignore next */
    cov_79i1wt8v5().f[1]++;
    cov_79i1wt8v5().s[2]++;
    return Array.from(this.pricingTiers.values());
  }
  async calculatePrice(tier, isStudent) {
    /* istanbul ignore next */
    cov_79i1wt8v5().f[2]++;
    const pricingTier =
    /* istanbul ignore next */
    (cov_79i1wt8v5().s[3]++, this.pricingTiers.get(tier));
    /* istanbul ignore next */
    cov_79i1wt8v5().s[4]++;
    if (!pricingTier) {
      /* istanbul ignore next */
      cov_79i1wt8v5().b[1][0]++;
      cov_79i1wt8v5().s[5]++;
      throw new Error(`Pricing tier not found: ${tier}`);
    } else
    /* istanbul ignore next */
    {
      cov_79i1wt8v5().b[1][1]++;
    }
    cov_79i1wt8v5().s[6]++;
    if (
    /* istanbul ignore next */
    (cov_79i1wt8v5().b[3][0]++, isStudent) &&
    /* istanbul ignore next */
    (cov_79i1wt8v5().b[3][1]++, tier !== 'free')) {
      /* istanbul ignore next */
      cov_79i1wt8v5().b[2][0]++;
      const discount =
      /* istanbul ignore next */
      (cov_79i1wt8v5().s[7]++, pricingTier.studentDiscount);
      /* istanbul ignore next */
      cov_79i1wt8v5().s[8]++;
      return pricingTier.monthlyPrice * (1 - discount / 100);
    } else
    /* istanbul ignore next */
    {
      cov_79i1wt8v5().b[2][1]++;
    }
    cov_79i1wt8v5().s[9]++;
    return pricingTier.monthlyPrice;
  }
  async trackConversionEvent(studentId, eventType, triggerValue) {
    /* istanbul ignore next */
    cov_79i1wt8v5().f[3]++;
    cov_79i1wt8v5().s[10]++;
    try {
      // Create conversion event
      const event =
      /* istanbul ignore next */
      (cov_79i1wt8v5().s[11]++, await prisma.conversionEvent.create({
        data: {
          studentId,
          eventType,
          triggerValue
        }
      }));
      /* istanbul ignore next */
      cov_79i1wt8v5().s[12]++;
      logger.info(`Conversion event tracked: ${studentId} - ${eventType}`);

      // Check if conversion offer should be triggered
      /* istanbul ignore next */
      cov_79i1wt8v5().s[13]++;
      if (this.shouldTriggerOffer(eventType, triggerValue)) {
        /* istanbul ignore next */
        cov_79i1wt8v5().b[4][0]++;
        cov_79i1wt8v5().s[14]++;
        return await this.createConversionOffer(studentId, event.id);
      } else
      /* istanbul ignore next */
      {
        cov_79i1wt8v5().b[4][1]++;
      }
      cov_79i1wt8v5().s[15]++;
      return null;
    } catch (error) {
      /* istanbul ignore next */
      cov_79i1wt8v5().s[16]++;
      logger.error('Error tracking conversion event:', error);
      /* istanbul ignore next */
      cov_79i1wt8v5().s[17]++;
      throw error;
    }
  }
  shouldTriggerOffer(eventType, triggerValue) {
    /* istanbul ignore next */
    cov_79i1wt8v5().f[4]++;
    const triggers =
    /* istanbul ignore next */
    (cov_79i1wt8v5().s[18]++, {
      'module_completion': 40,
      'assessment_pass': 70,
      'course_completion': 100
    });
    /* istanbul ignore next */
    cov_79i1wt8v5().s[19]++;
    return triggerValue >= (
    /* istanbul ignore next */
    (cov_79i1wt8v5().b[5][0]++, triggers[eventType]) ||
    /* istanbul ignore next */
    (cov_79i1wt8v5().b[5][1]++, 100));
  }
  async createConversionOffer(studentId, eventId) {
    /* istanbul ignore next */
    cov_79i1wt8v5().f[5]++;
    const expiresAt =
    /* istanbul ignore next */
    (cov_79i1wt8v5().s[20]++, new Date());
    /* istanbul ignore next */
    cov_79i1wt8v5().s[21]++;
    expiresAt.setDate(expiresAt.getDate() + 7); // 7-day expiry

    const offer =
    /* istanbul ignore next */
    (cov_79i1wt8v5().s[22]++, await prisma.conversionOffer.create({
      data: {
        eventId,
        studentId,
        offerType: 'upgrade_discount',
        discount: 50,
        expiresAt
      }
    }));
    /* istanbul ignore next */
    cov_79i1wt8v5().s[23]++;
    logger.info(`Conversion offer created: ${studentId}`);
    /* istanbul ignore next */
    cov_79i1wt8v5().s[24]++;
    return offer;
  }
  async acceptConversionOffer(offerId) {
    /* istanbul ignore next */
    cov_79i1wt8v5().f[6]++;
    cov_79i1wt8v5().s[25]++;
    try {
      const offer =
      /* istanbul ignore next */
      (cov_79i1wt8v5().s[26]++, await prisma.conversionOffer.update({
        where: {
          id: offerId
        },
        data: {
          accepted: true,
          acceptedAt: new Date()
        }
      }));
      /* istanbul ignore next */
      cov_79i1wt8v5().s[27]++;
      logger.info(`Conversion offer accepted: ${offerId}`);
      /* istanbul ignore next */
      cov_79i1wt8v5().s[28]++;
      return offer;
    } catch (error) {
      /* istanbul ignore next */
      cov_79i1wt8v5().s[29]++;
      logger.error('Error accepting conversion offer:', error);
      /* istanbul ignore next */
      cov_79i1wt8v5().s[30]++;
      throw error;
    }
  }
  async processPayment(studentId, courseId, tier, amount) {
    /* istanbul ignore next */
    cov_79i1wt8v5().f[7]++;
    cov_79i1wt8v5().s[31]++;
    try {
      const period =
      /* istanbul ignore next */
      (cov_79i1wt8v5().s[32]++, new Date().toISOString().slice(0, 7)); // YYYY-MM

      const payment =
      /* istanbul ignore next */
      (cov_79i1wt8v5().s[33]++, await prisma.payment.create({
        data: {
          studentId,
          courseId,
          amount,
          tier,
          period,
          status: 'completed'
        }
      }));
      /* istanbul ignore next */
      cov_79i1wt8v5().s[34]++;
      logger.info(`Payment processed: ${studentId} - ${courseId} - $${amount}`);
      /* istanbul ignore next */
      cov_79i1wt8v5().s[35]++;
      return {
        ...payment,
        amount: typeof payment.amount === 'number' ?
        /* istanbul ignore next */
        (cov_79i1wt8v5().b[6][0]++, payment.amount) :
        /* istanbul ignore next */
        (cov_79i1wt8v5().b[6][1]++, Number(payment.amount))
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_79i1wt8v5().s[36]++;
      logger.error('Error processing payment:', error);
      /* istanbul ignore next */
      cov_79i1wt8v5().s[37]++;
      throw error;
    }
  }
  async getRevenueByPeriod(period) {
    /* istanbul ignore next */
    cov_79i1wt8v5().f[8]++;
    cov_79i1wt8v5().s[38]++;
    try {
      const result =
      /* istanbul ignore next */
      (cov_79i1wt8v5().s[39]++, await prisma.payment.aggregate({
        where: {
          period,
          status: 'completed'
        },
        _sum: {
          amount: true
        }
      }));
      /* istanbul ignore next */
      cov_79i1wt8v5().s[40]++;
      return result._sum.amount ?
      /* istanbul ignore next */
      (cov_79i1wt8v5().b[7][0]++, Number(result._sum.amount)) :
      /* istanbul ignore next */
      (cov_79i1wt8v5().b[7][1]++, 0);
    } catch (error) {
      /* istanbul ignore next */
      cov_79i1wt8v5().s[41]++;
      logger.error('Error getting revenue by period:', error);
      /* istanbul ignore next */
      cov_79i1wt8v5().s[42]++;
      throw error;
    }
  }
  async getConversionMetrics(period) {
    /* istanbul ignore next */
    cov_79i1wt8v5().f[9]++;
    cov_79i1wt8v5().s[43]++;
    try {
      const freeUsers =
      /* istanbul ignore next */
      (cov_79i1wt8v5().s[44]++, await prisma.user.count({
        where: {
          tier: 'free'
        }
      }));
      const premiumUsers =
      /* istanbul ignore next */
      (cov_79i1wt8v5().s[45]++, await prisma.user.count({
        where: {
          tier: 'premium'
        }
      }));
      const proUsers =
      /* istanbul ignore next */
      (cov_79i1wt8v5().s[46]++, await prisma.user.count({
        where: {
          tier: 'pro'
        }
      }));
      const totalUsers =
      /* istanbul ignore next */
      (cov_79i1wt8v5().s[47]++, freeUsers + premiumUsers + proUsers);
      const conversionRate =
      /* istanbul ignore next */
      (cov_79i1wt8v5().s[48]++, totalUsers > 0 ?
      /* istanbul ignore next */
      (cov_79i1wt8v5().b[8][0]++, (premiumUsers + proUsers) / totalUsers * 100) :
      /* istanbul ignore next */
      (cov_79i1wt8v5().b[8][1]++, 0));
      /* istanbul ignore next */
      cov_79i1wt8v5().s[49]++;
      return {
        period,
        freeUsers,
        premiumUsers,
        proUsers,
        totalUsers,
        conversionRate: Math.round(conversionRate * 100) / 100
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_79i1wt8v5().s[50]++;
      logger.error('Error getting conversion metrics:', error);
      /* istanbul ignore next */
      cov_79i1wt8v5().s[51]++;
      throw error;
    }
  }
}
export const revenueService =
/* istanbul ignore next */
(cov_79i1wt8v5().s[52]++, new RevenueService());
