import { useQuery } from '@tanstack/react-query';
import { ApiClient } from '../lib/api-client';
// Custom hook for API calls with automatic fallback to mock data
export function useApi({
  serviceName,
  endpoint,
  method =
  /* istanbul ignore next */
  (cov_15gt1xg7zw().b[0][0]++, 'GET'),
  body,
  enabled =
  /* istanbul ignore next */
  (cov_15gt1xg7zw().b[1][0]++, true),
  ...queryOptions
}) {
  /* istanbul ignore next */
  cov_15gt1xg7zw().f[0]++;
  cov_15gt1xg7zw().s[0]++;
  return useQuery({
    queryKey: [serviceName, endpoint, method, body],
    queryFn: async () => {
      /* istanbul ignore next */
      cov_15gt1xg7zw().f[1]++;
      const client =
      /* istanbul ignore next */
      (cov_15gt1xg7zw().s[1]++, new ApiClient(serviceName));
      /* istanbul ignore next */
      cov_15gt1xg7zw().s[2]++;
      switch (method) {
        case 'GET':
          /* istanbul ignore next */
          cov_15gt1xg7zw().b[2][0]++;
          cov_15gt1xg7zw().s[3]++;
          return client.get(endpoint);
        case 'POST':
          /* istanbul ignore next */
          cov_15gt1xg7zw().b[2][1]++;
          cov_15gt1xg7zw().s[4]++;
          return client.post(endpoint, body);
        case 'PUT':
          /* istanbul ignore next */
          cov_15gt1xg7zw().b[2][2]++;
          cov_15gt1xg7zw().s[5]++;
          return client.put(endpoint, body);
        case 'PATCH':
          /* istanbul ignore next */
          cov_15gt1xg7zw().b[2][3]++;
          cov_15gt1xg7zw().s[6]++;
          return client.patch(endpoint, body);
        case 'DELETE':
          /* istanbul ignore next */
          cov_15gt1xg7zw().b[2][4]++;
          cov_15gt1xg7zw().s[7]++;
          return client.delete(endpoint);
        default:
          /* istanbul ignore next */
          cov_15gt1xg7zw().b[2][5]++;
          cov_15gt1xg7zw().s[8]++;
          throw new Error(`Unsupported method: ${method}`);
      }
    },
    enabled,
    retry: false,
    // Don't retry - fallback to mock data on failure
    refetchOnWindowFocus: false,
    staleTime: 5 * 60 * 1000,
    // 5 minutes
    ...queryOptions
  });
}

