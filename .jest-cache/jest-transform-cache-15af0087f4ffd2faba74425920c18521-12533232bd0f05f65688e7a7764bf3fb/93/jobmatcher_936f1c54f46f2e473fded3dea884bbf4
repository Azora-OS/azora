class JobMatcher {
  matchJobs(candidate, jobs) {
    /* istanbul ignore next */
    cov_2bcgr9j9xc().f[0]++;
    cov_2bcgr9j9xc().s[0]++;
    return jobs.map(job => {
      /* istanbul ignore next */
      cov_2bcgr9j9xc().f[1]++;
      cov_2bcgr9j9xc().s[1]++;
      return {
        job,
        score: this.calculateMatchScore(candidate, job),
        reasons: this.getMatchReasons(candidate, job)
      };
    }).filter(m => {
      /* istanbul ignore next */
      cov_2bcgr9j9xc().f[2]++;
      cov_2bcgr9j9xc().s[2]++;
      return m.score >= 60;
    }).sort((a, b) => {
      /* istanbul ignore next */
      cov_2bcgr9j9xc().f[3]++;
      cov_2bcgr9j9xc().s[3]++;
      return b.score - a.score;
    });
  }
  calculateMatchScore(candidate, job) {
    /* istanbul ignore next */
    cov_2bcgr9j9xc().f[4]++;
    const skillMatch =
    /* istanbul ignore next */
    (cov_2bcgr9j9xc().s[4]++, this.skillMatchScore(candidate.skills, job.requiredSkills));
    const experienceMatch =
    /* istanbul ignore next */
    (cov_2bcgr9j9xc().s[5]++, this.experienceMatchScore(candidate.experience, job.requiredExperience));
    const locationMatch =
    /* istanbul ignore next */
    (cov_2bcgr9j9xc().s[6]++, this.locationMatchScore(candidate.location, job.location));
    const salaryMatch =
    /* istanbul ignore next */
    (cov_2bcgr9j9xc().s[7]++, this.salaryMatchScore(candidate.expectedSalary, job.salary));
    /* istanbul ignore next */
    cov_2bcgr9j9xc().s[8]++;
    return Math.floor(skillMatch * 0.4 + experienceMatch * 0.3 + locationMatch * 0.15 + salaryMatch * 0.15);
  }
  skillMatchScore(candidateSkills, requiredSkills) {
    /* istanbul ignore next */
    cov_2bcgr9j9xc().f[5]++;
    const matches =
    /* istanbul ignore next */
    (cov_2bcgr9j9xc().s[9]++, requiredSkills.filter(rs => {
      /* istanbul ignore next */
      cov_2bcgr9j9xc().f[6]++;
      cov_2bcgr9j9xc().s[10]++;
      return candidateSkills.some(cs => {
        /* istanbul ignore next */
        cov_2bcgr9j9xc().f[7]++;
        cov_2bcgr9j9xc().s[11]++;
        return /* istanbul ignore next */(cov_2bcgr9j9xc().b[0][0]++, cs.name === rs.name) &&
        /* istanbul ignore next */
        (cov_2bcgr9j9xc().b[0][1]++, cs.level >= rs.level);
      });
    }));
    /* istanbul ignore next */
    cov_2bcgr9j9xc().s[12]++;
    return matches.length / requiredSkills.length * 100;
  }
  experienceMatchScore(candidateExp, requiredExp) {
    /* istanbul ignore next */
    cov_2bcgr9j9xc().f[8]++;
    cov_2bcgr9j9xc().s[13]++;
    if (candidateExp >= requiredExp) {
      /* istanbul ignore next */
      cov_2bcgr9j9xc().b[1][0]++;
      cov_2bcgr9j9xc().s[14]++;
      return 100;
    } else
    /* istanbul ignore next */
    {
      cov_2bcgr9j9xc().b[1][1]++;
    }
    cov_2bcgr9j9xc().s[15]++;
    return candidateExp / requiredExp * 100;
  }
  locationMatchScore(candidateLoc, jobLoc) {
    /* istanbul ignore next */
    cov_2bcgr9j9xc().f[9]++;
    cov_2bcgr9j9xc().s[16]++;
    if (jobLoc === 'remote') {
      /* istanbul ignore next */
      cov_2bcgr9j9xc().b[2][0]++;
      cov_2bcgr9j9xc().s[17]++;
      return 100;
    } else
    /* istanbul ignore next */
    {
      cov_2bcgr9j9xc().b[2][1]++;
    }
    cov_2bcgr9j9xc().s[18]++;
    return candidateLoc === jobLoc ?
    /* istanbul ignore next */
    (cov_2bcgr9j9xc().b[3][0]++, 100) :
    /* istanbul ignore next */
    (cov_2bcgr9j9xc().b[3][1]++, 50);
  }
  salaryMatchScore(expected, offered) {
    /* istanbul ignore next */
    cov_2bcgr9j9xc().f[10]++;
    cov_2bcgr9j9xc().s[19]++;
    if (offered >= expected) {
      /* istanbul ignore next */
      cov_2bcgr9j9xc().b[4][0]++;
      cov_2bcgr9j9xc().s[20]++;
      return 100;
    } else
    /* istanbul ignore next */
    {
      cov_2bcgr9j9xc().b[4][1]++;
    }
    cov_2bcgr9j9xc().s[21]++;
    return offered / expected * 100;
  }
  getMatchReasons(candidate, job) {
    /* istanbul ignore next */
    cov_2bcgr9j9xc().f[11]++;
    const reasons =
    /* istanbul ignore next */
    (cov_2bcgr9j9xc().s[22]++, []);
    const skillScore =
    /* istanbul ignore next */
    (cov_2bcgr9j9xc().s[23]++, this.skillMatchScore(candidate.skills, job.requiredSkills));
    /* istanbul ignore next */
    cov_2bcgr9j9xc().s[24]++;
    if (skillScore >= 80) {
      /* istanbul ignore next */
      cov_2bcgr9j9xc().b[5][0]++;
      cov_2bcgr9j9xc().s[25]++;
      reasons.push('Strong skill match');
    } else
    /* istanbul ignore next */
    {
      cov_2bcgr9j9xc().b[5][1]++;
    }
    cov_2bcgr9j9xc().s[26]++;
    if (candidate.experience >= job.requiredExperience) {
      /* istanbul ignore next */
      cov_2bcgr9j9xc().b[6][0]++;
      cov_2bcgr9j9xc().s[27]++;
      reasons.push('Experience requirement met');
    } else
    /* istanbul ignore next */
    {
      cov_2bcgr9j9xc().b[6][1]++;
    }
    cov_2bcgr9j9xc().s[28]++;
    if (job.location === 'remote') {
      /* istanbul ignore next */
      cov_2bcgr9j9xc().b[7][0]++;
      cov_2bcgr9j9xc().s[29]++;
      reasons.push('Remote opportunity');
    } else
    /* istanbul ignore next */
    {
      cov_2bcgr9j9xc().b[7][1]++;
    }
    cov_2bcgr9j9xc().s[30]++;
    return reasons;
  }
}
