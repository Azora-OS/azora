    console.log(`   Schedule: ${this.config.schedule.frequency}`);
    /* istanbul ignore next */
    cov_11pf6j2z7p().s[8]++;
    console.log(`   Duration: ${this.config.schedule.duration} minutes`);
    /* istanbul ignore next */
    cov_11pf6j2z7p().s[9]++;
    console.log(`   Targets: ${this.config.targetServices.join(', ')}`);
    /* istanbul ignore next */
    cov_11pf6j2z7p().s[10]++;
    console.log(`   Excluded: ${this.config.excludeServices.join(', ')}`);

    // Schedule chaos injection
    /* istanbul ignore next */
    cov_11pf6j2z7p().s[11]++;
    this.scheduledTask = cron.schedule(this.config.schedule.frequency, async () => {
      /* istanbul ignore next */
      cov_11pf6j2z7p().f[2]++;
      cov_11pf6j2z7p().s[12]++;
      console.log('üêµ ChaosMonkey: Scheduled chaos window starting...');
      /* istanbul ignore next */
      cov_11pf6j2z7p().s[13]++;
      await this.runChaosWindow();
    });

    // Listen to chaos events
    /* istanbul ignore next */
    cov_11pf6j2z7p().s[14]++;
    this.engine.on('chaos-injected', result => {
      /* istanbul ignore next */
      cov_11pf6j2z7p().f[3]++;
      cov_11pf6j2z7p().s[15]++;
      console.log(`üêµ Chaos event completed:`, result);
      /* istanbul ignore next */
      cov_11pf6j2z7p().s[16]++;
      this.logToAuditLedger(result);
    });
  }

  /**
   * Stop scheduled chaos injection
   */
  stop() {
    /* istanbul ignore next */
    cov_11pf6j2z7p().f[4]++;
    cov_11pf6j2z7p().s[17]++;
    if (this.scheduledTask) {
      /* istanbul ignore next */
      cov_11pf6j2z7p().b[1][0]++;
      cov_11pf6j2z7p().s[18]++;
      this.scheduledTask.stop();
      /* istanbul ignore next */
      cov_11pf6j2z7p().s[19]++;
      console.log('üêµ ChaosMonkey stopped');
    } else
    /* istanbul ignore next */
    {
      cov_11pf6j2z7p().b[1][1]++;
    }
  }

  /**
   * Run a chaos window (inject multiple failures over duration)
   */
  async runChaosWindow() {
    /* istanbul ignore next */
    cov_11pf6j2z7p().f[5]++;
    const endTime =
    /* istanbul ignore next */
    (cov_11pf6j2z7p().s[20]++, Date.now() + this.config.schedule.duration * 60 * 1000);
    let eventCount =
    /* istanbul ignore next */
    (cov_11pf6j2z7p().s[21]++, 0);
    /* istanbul ignore next */
    cov_11pf6j2z7p().s[22]++;
    while (Date.now() < endTime) {
      /* istanbul ignore next */
      cov_11pf6j2z7p().s[23]++;
      try {
        /* istanbul ignore next */
        cov_11pf6j2z7p().s[24]++;
        // Check if we need Constitutional AI approval
        if (
        /* istanbul ignore next */
        (cov_11pf6j2z7p().b[3][0]++, this.config.requireApproval) &&
        /* istanbul ignore next */
        (cov_11pf6j2z7p().b[3][1]++, this.config.intensity === 'high')) {
          /* istanbul ignore next */
          cov_11pf6j2z7p().b[2][0]++;
          const approved =
          /* istanbul ignore next */
          (cov_11pf6j2z7p().s[25]++, await this.requestConstitutionalApproval());
          /* istanbul ignore next */
          cov_11pf6j2z7p().s[26]++;
          if (!approved) {
            /* istanbul ignore next */
            cov_11pf6j2z7p().b[4][0]++;
            cov_11pf6j2z7p().s[27]++;
            console.log('üêµ High-intensity chaos rejected by Constitutional AI');
            /* istanbul ignore next */
            cov_11pf6j2z7p().s[28]++;
            break;
          } else
          /* istanbul ignore next */
          {
            cov_11pf6j2z7p().b[4][1]++;
          }
        } else
        /* istanbul ignore next */
        {
          cov_11pf6j2z7p().b[2][1]++;
        }

        // Inject chaos
        cov_11pf6j2z7p().s[29]++;
        await this.engine.injectChaos();
        /* istanbul ignore next */
        cov_11pf6j2z7p().s[30]++;
        eventCount++;

        // Wait before next injection
        const waitTime =
        /* istanbul ignore next */
        (cov_11pf6j2z7p().s[31]++, this.calculateWaitTime());
        /* istanbul ignore next */
        cov_11pf6j2z7p().s[32]++;
        await this.sleep(waitTime);
      } catch (error) {
        /* istanbul ignore next */
        cov_11pf6j2z7p().s[33]++;
        console.error('üêµ Chaos injection error:', error);
      }
    }
    /* istanbul ignore next */
    cov_11pf6j2z7p().s[34]++;
    console.log(`üêµ Chaos window complete. Injected ${eventCount} events.`);
  }

  /**
   * Request approval from Constitutional AI
   */
  async requestConstitutionalApproval() {
    /* istanbul ignore next */
    cov_11pf6j2z7p().f[6]++;
    cov_11pf6j2z7p().s[35]++;
    try {
      const response =
      /* istanbul ignore next */
      (cov_11pf6j2z7p().s[36]++, await fetch('http://localhost:3014/api/critique', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json'
        },
        body: JSON.stringify({
          prompt: `High-intensity chaos injection requested. This will intentionally stress the system.`,
          actionType: 'TRANSACTION',
          agentId: 'chaos-monkey'
        })
      }));
      /* istanbul ignore next */
      cov_11pf6j2z7p().s[37]++;
      if (response.ok) {
        /* istanbul ignore next */
        cov_11pf6j2z7p().b[5][0]++;
        const data =
        /* istanbul ignore next */
        (cov_11pf6j2z7p().s[38]++, await response.json());
        /* istanbul ignore next */
        cov_11pf6j2z7p().s[39]++;
        return /* istanbul ignore next */(cov_11pf6j2z7p().b[6][0]++, data.success) &&
        /* istanbul ignore next */
        (cov_11pf6j2z7p().b[6][1]++, data.data.verdict !== 'REJECT');
      } else
      /* istanbul ignore next */
      {
        cov_11pf6j2z7p().b[5][1]++;
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_11pf6j2z7p().s[40]++;
      console.warn('Constitutional AI unavailable, proceeding with chaos');
    }
    /* istanbul ignore next */
    cov_11pf6j2z7p().s[41]++;
    return true; // Fail open
  }

  /**
   * Log chaos event to Auditable Mutation Ledger
   */
  async logToAuditLedger(result) {
    /* istanbul ignore next */
    cov_11pf6j2z7p().f[7]++;
    cov_11pf6j2z7p().s[42]++;
    try {
      /* istanbul ignore next */
      cov_11pf6j2z7p().s[43]++;
      // In production, log to azora-mint blockchain
