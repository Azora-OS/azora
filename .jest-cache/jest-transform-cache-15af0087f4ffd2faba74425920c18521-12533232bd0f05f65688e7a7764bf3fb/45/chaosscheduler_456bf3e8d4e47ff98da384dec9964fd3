    }
  }

  /**
   * Calculate wait time between chaos events
   */
  calculateWaitTime() {
    /* istanbul ignore next */
    cov_11pf6j2z7p().f[9]++;
    const base =
    /* istanbul ignore next */
    (cov_11pf6j2z7p().s[46]++, this.config.intensity === 'high' ?
    /* istanbul ignore next */
    (cov_11pf6j2z7p().b[7][0]++, 30000) :
    /* istanbul ignore next */
    (cov_11pf6j2z7p().b[7][1]++, this.config.intensity === 'medium' ?
    /* istanbul ignore next */
    (cov_11pf6j2z7p().b[8][0]++, 60000) :
    /* istanbul ignore next */
    (cov_11pf6j2z7p().b[8][1]++, 120000)));
    /* istanbul ignore next */
    cov_11pf6j2z7p().s[47]++;
    return base + Math.random() * 30000;
  }

  /**
   * Get chaos history
   */
  getHistory() {
    /* istanbul ignore next */
    cov_11pf6j2z7p().f[10]++;
    cov_11pf6j2z7p().s[48]++;
    return this.engine.getHistory();
  }

  /**
   * Get active events
   */
  getActiveEvents() {
    /* istanbul ignore next */
    cov_11pf6j2z7p().f[11]++;
    cov_11pf6j2z7p().s[49]++;
    return this.engine.getActiveEvents();
  }
  sleep(ms) {
    /* istanbul ignore next */
    cov_11pf6j2z7p().f[12]++;
    cov_11pf6j2z7p().s[50]++;
    return new Promise(resolve => {
      /* istanbul ignore next */
      cov_11pf6j2z7p().f[13]++;
      cov_11pf6j2z7p().s[51]++;
      return setTimeout(resolve, ms);
    });
  }
}
