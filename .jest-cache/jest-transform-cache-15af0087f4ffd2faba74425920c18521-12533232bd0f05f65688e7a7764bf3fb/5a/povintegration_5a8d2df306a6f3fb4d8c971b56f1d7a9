      /* istanbul ignore next */
      cov_20qnu5s86t().b[21][0]++;
      cov_20qnu5s86t().s[70]++;
      console.log(`  Duplicate activity detected`);
      /* istanbul ignore next */
      cov_20qnu5s86t().s[71]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_20qnu5s86t().b[21][1]++;
    }

    // 3. Check for abnormally high value scores
    const avgScore =
    /* istanbul ignore next */
    (cov_20qnu5s86t().s[72]++, await this.getAverageValueScore(activity.type));
    /* istanbul ignore next */
    cov_20qnu5s86t().s[73]++;
