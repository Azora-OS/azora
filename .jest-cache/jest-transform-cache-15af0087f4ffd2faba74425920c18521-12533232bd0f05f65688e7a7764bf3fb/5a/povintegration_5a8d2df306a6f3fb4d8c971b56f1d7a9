  /**
   * Calculate reward amount based on value score
   */
  calculateReward(valueScore) {
    /* istanbul ignore next */
    cov_20qnu5s86t().f[3]++;
    // Convert value score to AZR tokens
    // Base formula: 1 AZR per 100 value points
    const baseReward =
    /* istanbul ignore next */
    (cov_20qnu5s86t().s[60]++, valueScore / 100);

    // Apply reward curve (logarithmic to prevent inflation)
    const reward =
    /* istanbul ignore next */
    (cov_20qnu5s86t().s[61]++, Math.log10(baseReward + 1) * 10);

    // Round to 2 decimal places
    /* istanbul ignore next */
    cov_20qnu5s86t().s[62]++;
    return Math.round(reward * 100) / 100;
  }

  /**
   * Validate activity to prevent gaming
   */
  async validateActivity(userId, activity, valueScore) {
    /* istanbul ignore next */
    cov_20qnu5s86t().f[4]++;
    // Check for suspicious patterns

    // 1. Rate limiting - max activities per hour
    const recentActivities =
    /* istanbul ignore next */
    (cov_20qnu5s86t().s[63]++, await prisma.valueCreationRecord.count({
      where: {
        userId,
        createdAt: {
          gte: new Date(Date.now() - 60 * 60 * 1000) // Last hour
        }
