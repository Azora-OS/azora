  }

  /**
   * Get total rewards for today
   */
  async getDailyRewardsTotal(userId) {
    /* istanbul ignore next */
    cov_20qnu5s86t().f[5]++;
    const startOfDay =
    /* istanbul ignore next */
    (cov_20qnu5s86t().s[85]++, new Date());
    /* istanbul ignore next */
    cov_20qnu5s86t().s[86]++;
    startOfDay.setHours(0, 0, 0, 0);
    const result =
    /* istanbul ignore next */
    (cov_20qnu5s86t().s[87]++, await prisma.valueCreationRecord.aggregate({
      where: {
        userId,
        createdAt: {
          gte: startOfDay
        },
        status: 'completed'
      },
      _sum: {
        rewardAmount: true
      }
    }));
    /* istanbul ignore next */
    cov_20qnu5s86t().s[88]++;
    return /* istanbul ignore next */(cov_20qnu5s86t().b[25][0]++, result._sum.rewardAmount) ||
    /* istanbul ignore next */
    (cov_20qnu5s86t().b[25][1]++, 0);
  }

  /**
   * Get average value score for activity type
   */
  async getAverageValueScore(activityType) {
    /* istanbul ignore next */
    cov_20qnu5s86t().f[6]++;
    const result =
    /* istanbul ignore next */
    (cov_20qnu5s86t().s[89]++, await prisma.valueCreationRecord.aggregate({
      where: {
        activityType,
        status: 'completed',
        createdAt: {
          gte: new Date(Date.now() - 30 * 24 * 60 * 60 * 1000) // Last 30 days
        }
      },
      _avg: {
        valueScore: true
      }
    }));
    /* istanbul ignore next */
    cov_20qnu5s86t().s[90]++;
    return /* istanbul ignore next */(cov_20qnu5s86t().b[26][0]++, result._avg.valueScore) ||
    /* istanbul ignore next */
    (cov_20qnu5s86t().b[26][1]++, 100);
  }

  /**
   * Mint reward tokens
   */
  async mintReward(userId, amount, recordId) {
    /* istanbul ignore next */
    cov_20qnu5s86t().f[7]++;
    // Get user's wallet address
    const user =
    /* istanbul ignore next */
    (cov_20qnu5s86t().s[91]++, await prisma.user.findUnique({
      where: {
        id: userId
      },
      select: {
        walletAddress: true
      }
    }));
    /* istanbul ignore next */
    cov_20qnu5s86t().s[92]++;
    if (!user?.walletAddress) {
      /* istanbul ignore next */
      cov_20qnu5s86t().b[27][0]++;
      cov_20qnu5s86t().s[93]++;
      throw new Error('User wallet address not found');
    } else
    /* istanbul ignore next */
    {
      cov_20qnu5s86t().b[27][1]++;
    }

    // Call blockchain service to mint
    const txHash =
    /* istanbul ignore next */
    (cov_20qnu5s86t().s[94]++, await blockchainService.mintTokens(user.walletAddress, amount, `Proof-of-Value reward: ${recordId}`));
    /* istanbul ignore next */
    cov_20qnu5s86t().s[95]++;
    return txHash;
  }

  /**
   * Batch process pending rewards (run via cron)
   */
  async processPendingRewards() {
    /* istanbul ignore next */
    cov_20qnu5s86t().f[8]++;
    cov_20qnu5s86t().s[96]++;
    console.log(` Processing pending rewards...`);
    const pending =
    /* istanbul ignore next */
    (cov_20qnu5s86t().s[97]++, await prisma.valueCreationRecord.findMany({
      where: {
        status: 'pending',
        createdAt: {
          gte: new Date(Date.now() - 24 * 60 * 60 * 1000) // Last 24 hours only
        }
      },
      take: 100
    }));
    let processed =
    /* istanbul ignore next */
    (cov_20qnu5s86t().s[98]++, 0);
    let failed =
    /* istanbul ignore next */
    (cov_20qnu5s86t().s[99]++, 0);
    /* istanbul ignore next */
    cov_20qnu5s86t().s[100]++;
    for (const record of pending) {
      /* istanbul ignore next */
      cov_20qnu5s86t().s[101]++;
      try {
        const txHash =
        /* istanbul ignore next */
        (cov_20qnu5s86t().s[102]++, await this.mintReward(record.userId, record.rewardAmount, record.id));
        /* istanbul ignore next */
        cov_20qnu5s86t().s[103]++;
        await prisma.valueCreationRecord.update({
          where: {
            id: record.id
          },
          data: {
            status: 'completed',
            txHash,
            mintedAt: new Date()
          }
        });
        /* istanbul ignore next */
        cov_20qnu5s86t().s[104]++;
        processed++;
        /* istanbul ignore next */
        cov_20qnu5s86t().s[105]++;
        console.log(` Processed reward ${record.id}`);
      } catch (error) {
        /* istanbul ignore next */
        cov_20qnu5s86t().s[106]++;
        await prisma.valueCreationRecord.update({
          where: {
            id: record.id
          },
          data: {
            status: 'failed',
            errorMessage: error.message
          }
        });
        /* istanbul ignore next */
        cov_20qnu5s86t().s[107]++;
        failed++;
        /* istanbul ignore next */
        cov_20qnu5s86t().s[108]++;
        console.error(` Failed to process reward ${record.id}:`, error);
      }
    }
    /* istanbul ignore next */
    cov_20qnu5s86t().s[109]++;
    console.log(` Batch complete: ${processed} processed, ${failed} failed`);
    /* istanbul ignore next */
    cov_20qnu5s86t().s[110]++;
    return {
      processed,
      failed
    };
  }

  /**
   * Get user's total earned rewards
   */
  async getUserTotalRewards(userId) {
    /* istanbul ignore next */
    cov_20qnu5s86t().f[9]++;
    const result =
    /* istanbul ignore next */
    (cov_20qnu5s86t().s[111]++, await prisma.valueCreationRecord.aggregate({
      where: {
        userId,
        status: 'completed'
      },
      _sum: {
        rewardAmount: true
      },
      _count: true
    }));
    /* istanbul ignore next */
    cov_20qnu5s86t().s[112]++;
    return {
      total:
      /* istanbul ignore next */
      (cov_20qnu5s86t().b[28][0]++, result._sum.rewardAmount) ||
      /* istanbul ignore next */
      (cov_20qnu5s86t().b[28][1]++, 0),
      count: result._count
    };
  }

  /**
   * Get leaderboard
   */
  async getLeaderboard(timeframe =
  /* istanbul ignore next */
  (cov_20qnu5s86t().b[29][0]++, 'week'), limit =
  /* istanbul ignore next */
  (cov_20qnu5s86t().b[30][0]++, 100)) {
    /* istanbul ignore next */
    cov_20qnu5s86t().f[10]++;
    let startDate;
    /* istanbul ignore next */
    cov_20qnu5s86t().s[113]++;
    switch (timeframe) {
      case 'day':
        /* istanbul ignore next */
        cov_20qnu5s86t().b[31][0]++;
        cov_20qnu5s86t().s[114]++;
        startDate = new Date(Date.now() - 24 * 60 * 60 * 1000);
        /* istanbul ignore next */
        cov_20qnu5s86t().s[115]++;
        break;
      case 'week':
        /* istanbul ignore next */
        cov_20qnu5s86t().b[31][1]++;
        cov_20qnu5s86t().s[116]++;
        startDate = new Date(Date.now() - 7 * 24 * 60 * 60 * 1000);
        /* istanbul ignore next */
        cov_20qnu5s86t().s[117]++;
        break;
      case 'month':
        /* istanbul ignore next */
        cov_20qnu5s86t().b[31][2]++;
        cov_20qnu5s86t().s[118]++;
        startDate = new Date(Date.now() - 30 * 24 * 60 * 60 * 1000);
        /* istanbul ignore next */
        cov_20qnu5s86t().s[119]++;
        break;
      default:
        /* istanbul ignore next */
        cov_20qnu5s86t().b[31][3]++;
        cov_20qnu5s86t().s[120]++;
        startDate = new Date(0);
    }
    const leaderboard =
    /* istanbul ignore next */
    (cov_20qnu5s86t().s[121]++, await prisma.valueCreationRecord.groupBy({
      by: ['userId'],
      where: {
        status: 'completed',
        createdAt: {
          gte: startDate
        }
      },
      _sum: {
        rewardAmount: true,
        valueScore: true
      },
      _count: true,
      orderBy: {
        _sum: {
          rewardAmount: 'desc'
        }
      },
      take: limit
    }));

    // Fetch user details
    const userIds =
    /* istanbul ignore next */
    (cov_20qnu5s86t().s[122]++, leaderboard.map(entry => {
      /* istanbul ignore next */
      cov_20qnu5s86t().f[11]++;
      cov_20qnu5s86t().s[123]++;
      return entry.userId;
    }));
    const users =
    /* istanbul ignore next */
    (cov_20qnu5s86t().s[124]++, await prisma.user.findMany({
      where: {
        id: {
          in: userIds
        }
      },
      select: {
        id: true,
        name: true,
        avatar: true
      }
    }));
    /* istanbul ignore next */
    cov_20qnu5s86t().s[125]++;
    return leaderboard.map((entry, index) => {
      /* istanbul ignore next */
      cov_20qnu5s86t().f[12]++;
      cov_20qnu5s86t().s[126]++;
      return {
        rank: index + 1,
        user: users.find(u => {
          /* istanbul ignore next */
          cov_20qnu5s86t().f[13]++;
          cov_20qnu5s86t().s[127]++;
          return u.id === entry.userId;
        }),
        totalRewards:
        /* istanbul ignore next */
        (cov_20qnu5s86t().b[32][0]++, entry._sum.rewardAmount) ||
        /* istanbul ignore next */
        (cov_20qnu5s86t().b[32][1]++, 0),
        totalValueScore:
        /* istanbul ignore next */
        (cov_20qnu5s86t().b[33][0]++, entry._sum.valueScore) ||
        /* istanbul ignore next */
        (cov_20qnu5s86t().b[33][1]++, 0),
        activityCount: entry._count
      };
    });
  }
}

