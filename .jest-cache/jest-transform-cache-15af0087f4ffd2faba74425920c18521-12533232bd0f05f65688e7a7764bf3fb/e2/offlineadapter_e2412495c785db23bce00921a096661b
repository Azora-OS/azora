    this.queue.push(queueItem);
    /* istanbul ignore next */
    cov_1aredqy2u3().s[22]++;
    console.log(`üì• Queued operation: ${operation}`);
  }

  /**
   * Sync queued operations when online
   */
  async syncQueue() {
    /* istanbul ignore next */
    cov_1aredqy2u3().f[6]++;
    cov_1aredqy2u3().s[23]++;
    if (
    /* istanbul ignore next */
    (cov_1aredqy2u3().b[5][0]++, !this.isOnline) ||
    /* istanbul ignore next */
    (cov_1aredqy2u3().b[5][1]++, this.queue.length === 0)) {
      /* istanbul ignore next */
      cov_1aredqy2u3().b[4][0]++;
      cov_1aredqy2u3().s[24]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_1aredqy2u3().b[4][1]++;
    }
    cov_1aredqy2u3().s[25]++;
    console.log(`üîÑ Syncing ${this.queue.length} queued operations...`);
    const toSync =
    /* istanbul ignore next */
    (cov_1aredqy2u3().s[26]++, [...this.queue]);
    /* istanbul ignore next */
    cov_1aredqy2u3().s[27]++;
    this.queue = [];
    /* istanbul ignore next */
    cov_1aredqy2u3().s[28]++;
    for (const item of toSync) {
      /* istanbul ignore next */
      cov_1aredqy2u3().s[29]++;
      try {
        /* istanbul ignore next */
        cov_1aredqy2u3().s[30]++;
        // Attempt to execute queued operation
        await this.executeQueuedOperation(item);
        /* istanbul ignore next */
        cov_1aredqy2u3().s[31]++;
        console.log(`‚úÖ Synced: ${item.operation}`);
      } catch (error) {
        /* istanbul ignore next */
        cov_1aredqy2u3().s[32]++;
        // Re-queue if failed
        item.retries++;
        /* istanbul ignore next */
        cov_1aredqy2u3().s[33]++;
        if (item.retries < 3) {
          /* istanbul ignore next */
          cov_1aredqy2u3().b[6][0]++;
          cov_1aredqy2u3().s[34]++;
          this.queue.push(item);
          /* istanbul ignore next */
          cov_1aredqy2u3().s[35]++;
          console.warn(`‚ö†Ô∏è Sync failed, re-queued: ${item.operation}`);
        } else {
          /* istanbul ignore next */
          cov_1aredqy2u3().b[6][1]++;
          cov_1aredqy2u3().s[36]++;
          console.error(`‚ùå Sync failed permanently: ${item.operation}`);
        }
      }
    }
  }

  /**
   * Execute a queued operation
   */
  async executeQueuedOperation(item) {
    /* istanbul ignore next */
    cov_1aredqy2u3().f[7]++;
    cov_1aredqy2u3().s[37]++;
    // In production, this would execute the actual operation
    // For now, we simulate
    await new Promise(resolve => {
      /* istanbul ignore next */
      cov_1aredqy2u3().f[8]++;
      cov_1aredqy2u3().s[38]++;
      return setTimeout(resolve, 100);
    });
  }

  /**
   * Handle online event
   */
  handleOnline() {
    /* istanbul ignore next */
    cov_1aredqy2u3().f[9]++;
    cov_1aredqy2u3().s[39]++;
    console.log('üåê Connection restored - syncing...');
    /* istanbul ignore next */
    cov_1aredqy2u3().s[40]++;
    this.isOnline = true;
    /* istanbul ignore next */
    cov_1aredqy2u3().s[41]++;
    this.syncQueue();
  }

  /**
   * Handle offline event
   */
  handleOffline() {
    /* istanbul ignore next */
    cov_1aredqy2u3().f[10]++;
    cov_1aredqy2u3().s[42]++;
    console.log('üì¥ Connection lost - entering offline mode');
    /* istanbul ignore next */
    cov_1aredqy2u3().s[43]++;
    this.isOnline = false;
  }

  /**
   * Check if currently online
   */
  checkOnlineStatus() {
    /* istanbul ignore next */
    cov_1aredqy2u3().f[11]++;
    cov_1aredqy2u3().s[44]++;
    return this.isOnline;
  }

  /**
   * Get queue status
   */
  getQueueStatus() {
    /* istanbul ignore next */
    cov_1aredqy2u3().f[12]++;
    cov_1aredqy2u3().s[45]++;
    return {
      isOnline: this.isOnline,
      queuedOperations: this.queue.length,
      cachedItems: this.cache.size
    };
  }
}
export default OfflineAdapter;
