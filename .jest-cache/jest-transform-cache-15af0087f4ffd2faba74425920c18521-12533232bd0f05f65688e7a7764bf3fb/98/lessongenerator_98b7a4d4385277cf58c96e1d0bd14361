      /* istanbul ignore next */
      cov_21s9wuxijm().s[16]++;
      for (const topic of module.topics) {
        const lesson =
        /* istanbul ignore next */
        (cov_21s9wuxijm().s[17]++, await this.generateLesson({
          course: outline.title,
