      exercises: [{
        question: "What is the main concept?",
        answer: "To understand " + params.topic
      }],
      quiz: [{
        question: "True or False?",
        options: ["True", "False"],
        correct: 0
      }]
    };
  }
  async generateCourse(outline) {
    /* istanbul ignore next */
    cov_21s9wuxijm().f[3]++;
    const lessons =
    /* istanbul ignore next */
    (cov_21s9wuxijm().s[14]++, []);
