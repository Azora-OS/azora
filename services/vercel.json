{
  "version": 2,
  "builds": [
    { "src": "ai-family-service/index.js", "use": "@vercel/node" },
    { "src": "azora-sapiens/index.js", "use": "@vercel/node" },
    { "src": "azora-mint/mining-engine.js", "use": "@vercel/node" },
    { "src": "azora-forge/job-matcher.js", "use": "@vercel/node" },
    { "src": "azora-lms/index.js", "use": "@vercel/node" },
    { "src": "azora-nexus/index.js", "use": "@vercel/node" },
    { "src": "analytics-service/index.js", "use": "@vercel/node" },
    { "src": "azora-aegis/index.js", "use": "@vercel/node" },
    { "src": "master-ui-service/index.js", "use": "@vercel/node" },
    { "src": "mobile-service/index.js", "use": "@vercel/node" },
    { "src": "api-integration-service/index.js", "use": "@vercel/node" },
    { "src": "ui-enhancement-service/index.js", "use": "@vercel/node" },
    { "src": "database-service/index.js", "use": "@vercel/node" },
    { "src": "devops-service/index.js", "use": "@vercel/node" },
    { "src": "testing-service/index.js", "use": "@vercel/node" },
    { "src": "documentation-service/index.js", "use": "@vercel/node" },
    { "src": "blockchain-service/index.js", "use": "@vercel/node" },
    { "src": "ai-ml-service/index.js", "use": "@vercel/node" },
    { "src": "enterprise-service/index.js", "use": "@vercel/node" },
    { "src": "global-service/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/ai-family/(.*)", "dest": "/ai-family-service/index.js" },
    { "src": "/sapiens/(.*)", "dest": "/azora-sapiens/index.js" },
    { "src": "/mint/(.*)", "dest": "/azora-mint/mining-engine.js" },
    { "src": "/forge/(.*)", "dest": "/azora-forge/job-matcher.js" },
    { "src": "/lms/(.*)", "dest": "/azora-lms/index.js" },
    { "src": "/nexus/(.*)", "dest": "/azora-nexus/index.js" },
    { "src": "/analytics/(.*)", "dest": "/analytics-service/index.js" },
    { "src": "/aegis/(.*)", "dest": "/azora-aegis/index.js" },
    { "src": "/ui/(.*)", "dest": "/master-ui-service/index.js" },
    { "src": "/mobile/(.*)", "dest": "/mobile-service/index.js" },
    { "src": "/api/(.*)", "dest": "/api-integration-service/index.js" },
    { "src": "/enhance/(.*)", "dest": "/ui-enhancement-service/index.js" },
    { "src": "/db/(.*)", "dest": "/database-service/index.js" },
    { "src": "/devops/(.*)", "dest": "/devops-service/index.js" },
    { "src": "/test/(.*)", "dest": "/testing-service/index.js" },
    { "src": "/docs/(.*)", "dest": "/documentation-service/index.js" },
    { "src": "/blockchain/(.*)", "dest": "/blockchain-service/index.js" },
    { "src": "/ai-ml/(.*)", "dest": "/ai-ml-service/index.js" },
    { "src": "/enterprise/(.*)", "dest": "/enterprise-service/index.js" },
    { "src": "/global/(.*)", "dest": "/global-service/index.js" },
    { "src": "/(.*)", "dest": "/ai-family-service/index.js" }
  ],
  "env": {
    "NODE_ENV": "production",
    "CPU_USAGE": "80",
    "GPU_USAGE": "100"
  },
  "functions": {
    "ai-family-service/index.js": { "maxDuration": 30 },
    "azora-sapiens/index.js": { "maxDuration": 30 },
    "azora-mint/mining-engine.js": { "maxDuration": 60 },
    "azora-forge/job-matcher.js": { "maxDuration": 30 },
    "azora-lms/index.js": { "maxDuration": 30 },
    "azora-nexus/index.js": { "maxDuration": 30 },
    "analytics-service/index.js": { "maxDuration": 30 },
    "azora-aegis/index.js": { "maxDuration": 30 },
    "master-ui-service/index.js": { "maxDuration": 30 },
    "mobile-service/index.js": { "maxDuration": 60 },
    "api-integration-service/index.js": { "maxDuration": 30 },
    "ui-enhancement-service/index.js": { "maxDuration": 30 },
    "database-service/index.js": { "maxDuration": 30 },
    "devops-service/index.js": { "maxDuration": 60 },
    "testing-service/index.js": { "maxDuration": 60 },
    "documentation-service/index.js": { "maxDuration": 30 },
    "blockchain-service/index.js": { "maxDuration": 60 },
    "ai-ml-service/index.js": { "maxDuration": 60 },
    "enterprise-service/index.js": { "maxDuration": 30 },
    "global-service/index.js": { "maxDuration": 30 }
  }
}