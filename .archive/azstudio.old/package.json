{
  "name": "azstudio",
  "version": "0.1.0",
  "description": "AzStudio - Desktop IDE for building Azora platforms with AI-powered development",
  "main": "dist/main/main.js",
  "author": "Azora Team",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "webpack --config webpack.main.config.js --mode development --watch",
    "dev:renderer": "webpack serve --config webpack.renderer.config.js --mode development",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "webpack --config webpack.main.config.js --mode production",
    "build:renderer": "webpack --config webpack.renderer.config.js --mode production",
    "start": "electron .",
    "package": "npm run build && electron-builder build --win --publish never",
    "package:nsis": "npm run build && electron-builder build --win nsis --publish never",
    "package:msix": "npm run build && electron-builder build --win appx --publish never",
    "package:dir": "npm run build && electron-builder build --win --dir",
    "package:all": "npm run build && electron-builder build --win nsis appx --publish never",
    "dist": "npm run build && electron-builder build --win --publish always",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "playwright:install": "playwright install",
    "update-server": "node scripts/update-server.js",
    "deploy:alpha": "node scripts/deploy-updates.js deploy alpha",
    "deploy:beta": "node scripts/deploy-updates.js deploy beta",
    "deploy:stable": "node scripts/deploy-updates.js deploy stable",
    "rollout:increase": "node scripts/deploy-updates.js rollout",
    "rollback": "node scripts/deploy-updates.js rollback",
    "analytics": "node scripts/deploy-updates.js analytics"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.1",
    "@babel/generator": "^7.23.6",
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.6",
    "@babel/types": "^7.23.6",
    "@monaco-editor/react": "^4.6.0",
    "chokidar": "^3.5.3",
    "electron-updater": "^6.1.7",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "isomorphic-git": "^1.25.6",
    "monaco-editor": "^0.45.0",
    "openai": "^4.24.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.40.1",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "axe-core": "^4.11.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "css-loader": "^6.8.1",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "express": "^5.1.0",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "js-yaml": "^4.1.1",
    "lighthouse": "^11.4.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "png-to-ico": "^3.0.1",
    "pngjs": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.10.4",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.1",
    "ts-morph": "^21.0.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "build": {
    "appId": "com.azora.azstudio",
    "productName": "AzStudio",
    "copyright": "Copyright Â© 2025 Azora",
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "files": [
      "dist/**/*",
      "package.json",
      "!node_modules/**/*",
      "!src/**/*",
      "!tests/**/*",
      "!*.md",
      "!.eslintrc.json",
      "!tsconfig.json",
      "!webpack.*.js",
      "!jest.config.js",
      "!playwright.config.ts"
    ],
    "extraResources": [
      {
        "from": "build/resources",
        "to": "resources",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "appx",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "build/icon.png",
      "publisherName": "Azora",
      "verifyUpdateCodeSignature": true,
      "requestedExecutionLevel": "asInvoker",
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "signingHashAlgorithms": [
        "sha256"
      ],
      "signDlls": true,
      "rfc3161TimeStampServer": "http://timestamp.digicert.com",
      "timeStampServer": "http://timestamp.digicert.com",
      "fileAssociations": [
        {
          "ext": "azstudio",
          "name": "AzStudio Project",
          "description": "AzStudio Project File",
          "icon": "build/file-icon.ico",
          "role": "Editor"
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "AzStudio",
      "perMachine": false,
      "runAfterFinish": true,
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/icon.ico",
      "installerHeader": "build/installer-header.bmp",
      "installerSidebar": "build/installer-sidebar.bmp",
      "uninstallerSidebar": "build/installer-sidebar.bmp",
      "license": "LICENSE",
      "language": "1033",
      "warningsAsErrors": false,
      "deleteAppDataOnUninstall": false,
      "menuCategory": true,
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "appx": {
      "applicationId": "AzoraAzStudio",
      "backgroundColor": "#1e1e1e",
      "displayName": "AzStudio",
      "identityName": "Azora.AzStudio",
      "publisher": "CN=Azora",
      "publisherDisplayName": "Azora",
      "languages": [
        "en-US"
      ],
      "addAutoLaunchExtension": false,
      "showNameOnTiles": true,
      "setBuildNumber": true
    },
    "publish": [
      {
        "provider": "generic",
        "url": "https://updates.azora.com/azstudio",
        "channel": "latest"
      }
    ],
    "compression": "maximum",
    "removePackageScripts": true,
    "nodeGypRebuild": false,
    "buildDependenciesFromSource": false
  }
}
